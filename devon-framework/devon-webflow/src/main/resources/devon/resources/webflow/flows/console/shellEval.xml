<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="script" type="java.lang.String" />
    <end-state id="shell" view="fwkConsole/fwkShellJSON" >
    	<on-entry>
    		<evaluate expression="shellPlugin.eval(script)" result="flashScope.result" />
    		<evaluate expression="script" result="flashScope.script" />
    	</on-entry>
    	
    </end-state>
        
</flow>
