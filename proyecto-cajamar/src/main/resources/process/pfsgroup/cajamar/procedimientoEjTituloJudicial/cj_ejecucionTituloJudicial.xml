<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="cj_ejecucionTituloJudicial"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="CJ018_InterposicionDemanda" name="iniciarProceso"></transition>
	</start-state>

	<state name="CJ018_InterposicionDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_InterposicionDemanda" name="activarAlerta"></transition>
		<transition to="CJ018_InterposicionDemandaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ018_AutoDespachando">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_AutoDespachando" name="activarAlerta"></transition>
		<transition to="CJ018_AutoDespachandoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ018_AutoDespachandoDecision" expression="#{CJ018_AutoDespachandoDecision}">
		<transition to="ForkAutoDespachando" name="AdmitidoYBienesRegistrables"></transition>
		<transition to="CJ018_TituloDecision" name="NoAdmitido"></transition>
		<transition to="CJ018_ConfirmarNotificacion" name="Admitido"></transition>
	</decision>

	<node name="CJ018_TituloDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<fork name="ForkAutoDespachando">
		<transition to="CJ018_ConfirmarNotificacion" name="confirmaNotificacion"></transition>
		<transition to="CJ018_BPMVigilanciaCaducidadAnotacion" name="registraAnotacion"></transition>
	</fork>

	<state name="CJ018_ConfirmarNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_ConfirmarNotificacion" name="activarAlerta"></transition>
		<transition to="CJ018_ConfirmarNotificacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ018_ConfirmarNotificacionDecision" expression="#{CJ018_ConfirmarNotificacionDecision}">
		<transition to="CJ018_RegistrarOposicion" name="SI"></transition>
		<transition to="CJ018_BPMTramiteNotificacion" name="NO"></transition>
	</decision>

	<state name="CJ018_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_RegistrarOposicion" name="tramiteNotificacionV4"></transition>
	</state>

	<state name="CJ018_RegistrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_RegistrarOposicion" name="activarAlerta"></transition>
		<transition to="CJ018_RegistrarOposicionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ018_RegistrarOposicionDecision" expression="#{CJ018_RegistrarOposicionDecision}">
		<transition to="CJ018_ConfirmarPresentacionImpugnacion" name="SI"></transition>
		<transition to="CJ018_TituloDecision" name="NO"></transition>
	</decision>

	<state name="CJ018_HayVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_HayVista" name="activarAlerta"></transition>
		<transition to="CJ018_HayVistaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ018_ConfirmarPresentacionImpugnacion" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ018_HayVistaDecision" expression="#{CJ018_HayVistaDecision}">
		<transition to="CJ018_RegistrarVista" name="SI"></transition>
		<transition to="CJ018_RegistrarResolucion" name="NO"></transition>
	</decision>

	<state name="CJ018_RegistrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_RegistrarVista" name="activarAlerta"></transition>
		<transition to="CJ018_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ018_HayVista" name="vueltaAtras"></transition>
	</state>

	<state name="CJ018_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="CJ018_ResolucionFirme" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ018_ResolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="CJ018_ResolucionFirme" name="activarAlerta"></transition>
		<transition to="CJ018_TituloDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ018_RegistrarResolucion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ018_ConfirmarPresentacionImpugnacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ018_ConfirmarPresentacionImpugnacion" name="activarAlerta"></transition>
		<transition to="CJ018_HayVista" name="avanzaBPM"></transition>
		<transition to="CJ018_RegistrarOposicion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ018_BPMVigilanciaCaducidadAnotacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="cj_tramiteVigilanciaCaducidadEmbargos"></transition>
	</state>

	<decision name="CJ018_InterposicionDemandaDecision" expression="#{CJ018_InterposicionDemandaDecision}">
		<transition to="CJ018_AutoDespachando" name="sinProvision"></transition>
		<transition to="ForkInterposicionDemanda" name="conProvision"></transition>
		<transition to="ForkInterposicionDemandaAnti" name="conAnticipo"></transition>
	</decision>

	<fork name="ForkInterposicionDemanda">
		<transition to="CJ018_AutoDespachando" name="despacho"></transition>
		<transition to="CJ018_BPMProvisionFondos" name="provision"></transition>
	</fork>

	<fork name="ForkInterposicionDemandaAnti">
		<transition to="CJ018_AutoDespachando" name="despacho"></transition>
		<transition to="CJ018_BPMAnticipoFondos" name="anticipo"></transition>
	</fork>

	<state name="CJ018_BPMProvisionFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="cj_provisionFondos"></transition>
		<transition to="CJ018_BPMProvisionFondos" name="activarAlerta"></transition>
	</state>

	<state name="CJ018_BPMAnticipoFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="cj_anticipoFondosProcu"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>