<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="hcj_ejecucionTituloNoJudicial">

	<start-state name="Inicio">
		<transition to="CJ020_InterposicionDemandaMasBienes" name="iniciarProceso"></transition>
	</start-state>

	<state name="CJ020_InterposicionDemandaMasBienes">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_InterposicionDemandaMasBienes" name="activarAlerta"></transition>
		<transition to="CJ020_InterposicionDemandaMasBienesDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ020_AutoDespaEjecMasDecretoEmbargo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_AutoDespaEjecMasDecretoEmbargo" name="activarAlerta"></transition>
		<transition to="CJ020_AutoDespaEjecMasDecretoEmbargoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ020_InterposicionDemandaMasBienes" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ020_AutoDespaEjecMasDecretoEmbargoDecision" expression="#{CJ020_AutoDespaEjecMasDecretoEmbargoDecision}">
		<transition to="CJ020_JudicialDecision" name="NoAdmitido"></transition>
		<transition to="fork1" name="AdmitidoYBienesRegistrables"></transition>
		<transition to="CJ020_ConfirmarNotifiReqPago" name="Admitido"></transition>
	</decision>

	<fork name="fork1">
		<transition to="CJ020_BPMVigilanciaCaducidadAnotacion" name="registraAnotacion"></transition>
		<transition to="CJ020_ConfirmarNotifiReqPago" name="confirmaNotificacion"></transition>
	</fork>

	<node name="Decision1">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="CJ020_BPMVigilanciaCaducidadAnotacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="vigilanciaCaducidadAnotacion"></transition>
	</state>

	<state name="CJ020_ConfirmarNotifiReqPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ConfirmarNotifiReqPago" name="activarAlerta"></transition>
		<transition to="CJ020_ConfirmarNotifiReqPagoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ020_ConfirmarNotifiReqPagoDecision" expression="#{CJ020_ConfirmarNotifiReqPagoDecision}">
		<transition to="CJ020_BPMTramiteNotificacion" name="NO"></transition>
		<transition to="CJ020_ConfirmarSiExisteOposicion" name="SI"></transition>
	</decision>

	<state name="CJ020_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ConfirmarSiExisteOposicion" name="tramiteNotificacionV4"></transition>
	</state>

	<state name="CJ020_ConfirmarSiExisteOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ConfirmarSiExisteOposicion" name="activarAlerta"></transition>
		<transition to="CJ020_ConfirmarSiExisteOposicionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ020_ConfirmarSiExisteOposicionDecision" expression="#{CJ020_ConfirmarSiExisteOposicionDecision}">
		<transition to="CJ020_JudicialDecision" name="NO"></transition>
		<transition to="CJ020_ConfirmarPresentacionImpugnacion" name="SI"></transition>
	</decision>

	<state name="CJ020_ConfirmarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ConfirmarVista" name="activarAlerta"></transition>
		<transition to="CJ020_ConfirmarVistaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ020_ConfirmarPresentacionImpugnacion" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ020_ConfirmarVistaDecision" expression="#{CJ020_ConfirmarVistaDecision}">
		<transition to="CJ020_RegistrarCelebracionVista" name="SI"></transition>
		<transition to="CJ020_RegistrarResolucion" name="NO"></transition>
	</decision>

	<node name="CJ020_JudicialDecision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="CJ020_RegistrarCelebracionVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_RegistrarCelebracionVista" name="activarAlerta"></transition>
		<transition to="CJ020_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ020_ConfirmarVista" name="vueltaAtras"></transition>
	</state>

	<state name="CJ020_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="CJ020_ResolucionFirme" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ020_ResolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ResolucionFirme" name="activarAlerta"></transition>
		<transition to="CJ020_JudicialDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ020_RegistrarResolucion" name="vueltaAtras"></transition>
	</state>
	
	<state name="CJ020_ConfirmarPresentacionImpugnacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ020_ConfirmarPresentacionImpugnacion" name="activarAlerta"></transition>
		<transition to="CJ020_ConfirmarVista" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ020_ConfirmarSiExisteOposicion" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ020_InterposicionDemandaMasBienesDecision" expression="#{CJ020_InterposicionDemandaMasBienesDecision}">
		<transition to="CJ020_AutoDespaEjecMasDecretoEmbargo" name="sinProvision"></transition>
		<transition to="ForkInterposicionDemandaMasBienes" name="conProvision"></transition>
		<transition to="ForkInterposicionDemandaMasBienesAnti" name="conAnticipo"></transition>
	</decision>

	<fork name="ForkInterposicionDemandaMasBienes">
		<transition to="CJ020_AutoDespaEjecMasDecretoEmbargo" name="decreto"></transition>
		<transition to="CJ020_BPMProvisionFondos" name="provision"></transition>
	</fork>

	<fork name="ForkInterposicionDemandaMasBienesAnti">
		<transition to="CJ020_AutoDespaEjecMasDecretoEmbargo" name="decreto"></transition>
		<transition to="CJ020_BPMAnticipoFondos" name="anticipo"></transition>
	</fork>

	<state name="CJ020_BPMProvisionFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="hcj_provisionFondos"></transition>
		<transition to="CJ020_BPMProvisionFondos" name="activarAlerta"></transition>
	</state>

	<state name="CJ020_BPMAnticipoFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="hcj_anticipoFondosProcu"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>
