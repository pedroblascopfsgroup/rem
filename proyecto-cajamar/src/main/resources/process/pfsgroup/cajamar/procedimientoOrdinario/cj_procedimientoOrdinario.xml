<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="cj_procedimientoOrdinario" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="CJ024_InterposicionDemanda" name="avanzaBPM"></transition>
	</start-state>

	<state name="CJ024_InterposicionDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_InterposicionDemanda" name="activarAlerta"></transition>
		<transition to="CJ024_InterposicionDemandaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ024_ConfirmarAdmision">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_ConfirmarAdmision" name="activarAlerta"></transition>
		<transition to="CJ024_ConfirmarNotAdmisionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<node name="CJ024_OrdinarioDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin" name="avanzaBPM"></transition>
	</node>

	<state name="CJ024_ConfirmarNotDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_ConfirmarNotDemanda" name="activarAlerta"></transition>
		<transition to="CJ024_ConfirmarNotDemandaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ024_ConfirmarAdmision" name="vueltaAtras"></transition>
	</state>

	<state name="CJ024_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_ConfirmarOposicion" name="tramiteNotificacionV4"></transition>
	</state>

	<state name="CJ024_ConfirmarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_ConfirmarOposicion" name="activarAlerta"></transition>
		<transition to="CJ024_RegistrarAudienciaPrevia" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ024_ConfirmarNotDemanda" name="vueltaAtras"></transition>
	</state>

	<state name="CJ024_RegistrarAudienciaPrevia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_RegistrarAudienciaPrevia" name="activarAlerta"></transition>
		<transition to="CJ024_RegistrarAudienciaPreviaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ024_ConfirmarOposicion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ024_RegistrarJuicio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_RegistrarJuicio" name="activarAlerta"></transition>
		<transition to="CJ024_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ024_RegistrarAudienciaPrevia" name="vueltaAtras"></transition>
	</state>

	<state name="CJ024_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="CJ024_ResolucionFirme" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ024_ResolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ024_ResolucionFirme" name="activarAlerta"></transition>
		<transition to="CJ024_OrdinarioDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ024_RegistrarResolucion" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ024_ConfirmarNotAdmisionDecision" expression="#{CJ024_ConfirmarAdmisionDecision}">
		<transition to="CJ024_OrdinarioDecision" name="NO"></transition>
		<transition to="CJ024_ConfirmarNotDemanda" name="SI"></transition>
	</decision>

	<decision name="CJ024_ConfirmarNotDemandaDecision" expression="#{CJ024_ConfirmarNotDemandaDecision}">
		<transition to="CJ024_BPMTramiteNotificacion" name="NO"></transition>
		<transition to="CJ024_ConfirmarOposicion" name="SI"></transition>
	</decision>

	<decision name="CJ024_RegistrarAudienciaPreviaDecision" expression="#{CJ024_RegistrarAudienciaPreviaDecision}">
		<transition to="CJ024_RegistrarResolucion" name="SI"></transition>
		<transition to="CJ024_RegistrarJuicio" name="NO"></transition>
	</decision>

	<decision name="CJ024_InterposicionDemandaDecision" expression="#{CJ024_InterposicionDemandaDecision}">
		<transition to="CJ024_ConfirmarAdmision" name="sinProvision"></transition>
		<transition to="ForkInterposicionDemanda" name="conProvision"></transition>
		<transition to="ForkInterposicionDemandaAnti" name="conAnticipo"></transition>
	</decision>

	<fork name="ForkInterposicionDemanda">
		<transition to="CJ024_ConfirmarAdmision" name="admision"></transition>
		<transition to="CJ024_BPMProvisionFondos" name="provision"></transition>
	</fork>

	<fork name="ForkInterposicionDemandaAnti">
		<transition to="CJ024_ConfirmarAdmision" name="admision"></transition>
		<transition to="CJ024_BPMAnticipoFondos" name="anticipo"></transition>
	</fork>

	<state name="CJ024_BPMProvisionFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="cj_provisionFondos"></transition>
		<transition to="CJ024_BPMProvisionFondos" name="activarAlerta"></transition>
	</state>

	<state name="CJ024_BPMAnticipoFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="cj_anticipoFondosProcu"></transition>
	</state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>