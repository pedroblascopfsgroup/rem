<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="cj_tramiteConsignacion">


	<start-state name="Inicio">
		<transition to="CJ064_solicitarAutorizacionConsignacionSareb" name="IniciarProceso"></transition>
	</start-state>


	<state name="CJ064_solicitarAutorizacionConsignacionSareb">
		<event type="node-enter">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ064_solicitarAutorizacionConsignacionSareb" name="activarAlerta"></transition>
		<transition to="CJ064_resultadoSolicitudConsignacion" name="avanzaBPM"></transition>
		
	</state>

	<state name="CJ064_resultadoSolicitudConsignacion">
		<event type="node-enter">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ064_resultadoSolicitudConsignacion" name="activarAlerta"></transition>
		<transition to="CJ064_resultadoSolicitudConsignacionDecision" name="avanzaBPM"></transition>
		<transition to="CJ064_solicitarAutorizacionConsignacionSareb" name="vueltaAtras"></transition>
	</state>

	<state name="CJ064_confirmarRealizacionConsignacion">
		<event type="node-enter">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ064_confirmarRealizacionConsignacion" name="activarAlerta"></transition>
		<transition to="CJ064_presentacionJuzgado" name="avanzaBPM"></transition>
		<transition to="CJ064_resultadoSolicitudConsignacion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ064_presentacionJuzgado">
		<event type="node-enter">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ064_presentacionJuzgado" name="activarAlerta"></transition>
		<transition to="CJ064_confirmarRealizacionConsignacion" name="vueltaAtras"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<decision name="CJ064_resultadoSolicitudConsignacionDecision" expression="#{CJ064_resultadoSolicitudConsignacionDecision}">
		<transition to="CJ064_confirmarRealizacionConsignacion"
			name="aceptado">
		</transition>
		<transition to="Fin" name="denegado"></transition>
	</decision>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>