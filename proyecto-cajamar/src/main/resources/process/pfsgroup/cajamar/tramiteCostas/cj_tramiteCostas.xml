<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="cj_tramiteCostas">

	<start-state name="Inicio">
		<transition to="CJ007_SolicitudTasacion" name="avanzaBPM"></transition>
	</start-state>

	<state name="CJ007_SolicitudTasacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_SolicitudTasacion" name="activarAlerta"></transition>
		<transition to="CJ007_TasacionCostas" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ007_TasacionCostas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_TasacionCostas" name="activarAlerta"></transition>
		<transition to="CJ007_ConfirmarNotificacion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ007_SolicitudTasacion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ007_ConfirmarNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_ConfirmarNotificacion" name="activarAlerta"></transition>
		<transition to="CJ007_ConfirmarNotificacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ007_TasacionCostas" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ007_ConfirmarNotificacionDecision" expression="#{CJ007_ConfirmarNotificacionDecision}">
		<transition to="CJ007_JBPMTramiteNotificacion" name="NO"></transition>
		<transition to="CJ007_Impugnacion" name="SI"></transition>
	</decision>

	<state name="CJ007_JBPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_Impugnacion" name="tramiteNotificacionV4"></transition>
	</state>

	<state name="CJ007_Impugnacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_Impugnacion" name="activarAlerta"></transition>
		<transition to="CJ007_ImpugnacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ007_ConfirmarNotificacion" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ007_ImpugnacionDecision" expression="#{CJ007_ImpugnacionDecision}">
		<transition to="CJ007_AutoAprobacionCostas" name="NO"></transition>
		<transition to="CJ007_RegistrarCelebracionVista" name="SI"></transition>
		<transition to="CJ007_AutoAprobacionCostas" name="SI_NOVISTA"></transition>
	</decision>

	<state name="CJ007_AutoAprobacionCostas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_AutoAprobacionCostas" name="activarAlerta"></transition>
		<transition to="CJ007_AutoFirme" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ007_Impugnacion" name="vueltaAtras"></transition>
	</state>

	<state name="CJ007_RegistrarCelebracionVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_RegistrarCelebracionVista" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ007_Impugnacion" name="vueltaAtras"></transition>
		<transition to="CJ007_AutoAprobacionCostas" name="avanzaBPM"></transition>
	</state>

	<node name="CJ007_AutoDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin"></transition>
	</node>

	<state name="CJ007_AutoFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="CJ007_AutoFirme" name="activarAlerta"></transition>
		<transition to="CJ007_AutoDecision" name="avanzaBPM"></transition>
		<transition to="CJ007_AutoAprobacionCostas" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>
