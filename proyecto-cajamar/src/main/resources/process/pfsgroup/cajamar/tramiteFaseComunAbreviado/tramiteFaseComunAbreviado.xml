<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteFaseComunAbreviado"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P23_registrarPublicacionBOE" name="iniciarProceso"></transition>
	</start-state>

	<state name="P23_registrarPublicacionBOE">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_regInsinuacionCreditosSup" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_registrarPublicacionBOE" name="activarAlerta"></transition>
	</state>
	
	<state name="P23_regInsinuacionCreditosSup">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_regInsinuacionCreditosSup" name="activarAlerta"></transition>
		<transition to="P23_registrarPublicacionBOE" name="vueltaAtras"></transition>
		<transition to="P23_asignarGestorLetradoSup" name="avanzaBPM"></transition>
	</state>

	<state name="P23_informeAdministracionConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="P23_revisarResultadoInfAdmon" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_informeAdministracionConcursal" name="activarAlerta"></transition>
		<transition to="P23_regComProyectoInventario" name="vueltaAtras"></transition>
	</state>

	<state name="P23_presentarEscritoInsinuacionCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P23_regComProyectoInventario" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_presentarEscritoInsinuacionCreditos" name="activarAlerta"></transition>
		<transition to="P23_registrarInsinuacionCreditosDef" name="vueltaAtras"></transition>
	</state>

	<state name="P23_revisarResultadoInfAdmon">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P23_revisarResultadoInfAdmonDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_revisarResultadoInfAdmon" name="activarAlerta"></transition>
		<transition to="P23_informeAdministracionConcursal" name="vueltaAtras"></transition>
	</state>

	<decision name="P23_revisarResultadoInfAdmonDecision" expression="#{P23_revisarResultadoInfAdmonDecision}">
		<transition to="P23_registrarFinFaseComun" name="NO"></transition>
		<transition to="P23_BPMTramiteDemandaIncidental" name="SI"></transition>
	</decision>

	<state name="P23_registrarInsinuacionCreditosDef">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_presentarEscritoInsinuacionCreditos" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_registrarInsinuacionCreditosDef" name="activarAlerta"></transition>
		<transition to="P23_registrarAceptacion" name="vueltaAtras"></transition>
	</state>

	<state name="P23_registrarFinFaseComun">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P23_registrarFinFaseComunDecision" name="avanzaBPM"></transition>
		<transition to="P23_revisarResultadoInfAdmon" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P23_registrarFinFaseComunDecision" expression="#{P23_registrarFinFaseComunDecision}">
		<transition to="P23_BPMTramiteFaseLiquidacion" name="faseLiquidacion"></transition>
		<transition to="P23_BPMTramiteFaseConvenio" name="faseConvenio"></transition>
	</decision>

	<state name='P23_BPMTramiteFaseLiquidacion'>
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="tramiteFaseLiquidacion"></transition>
	</state>

	<state name="P23_BPMTramiteFaseConvenio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="tramiteFaseConvenio"></transition>
	</state>

	<state name="P23_actualizarEstadoCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P23_actualizarEstadoCreditos" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P23_registrarFinFaseComun" name="avanzaBPM"></transition>
	</state>

	<state name="P23_regComProyectoInventario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_regComProyectoInventario" name="activarAlerta"></transition>
		<transition to="P23_regComProyectoInventarioDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P23_regComProyectoInventarioDecision" expression="#{P23_regComProyectoInventarioDecision}">
		<transition to="P23_presentarRectificacion" name="NO"></transition>
		<transition to="P23_informeAdministracionConcursal" name="SI"></transition>
	</decision>

	<state name="P23_presentarRectificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_informeAdministracionConcursal" name="avanzaBPM"></transition>
		<transition to="P23_presentarRectificacion" name="activarAlerta"></transition>
		<transition to="P23_regComProyectoInventario" name="vueltaAtras"></transition>
	</state>

	<state name="P23_asignarGestorLetradoSup">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_regInsinuacionCreditosSup" name="vueltraAtras"></transition>
		<transition to="P23_validarAsignacion" name="avanzaBPM"></transition>
		<transition to="P23_asignarGestorLetradoSup" name="activarAlerta"></transition>
	</state>

	<state name="P23_registrarAceptacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_validarAsignacion" name="vueltaAtras"></transition>
		<transition to="P23_registrarAceptacion" name="activarAlerta"></transition>
		<transition to="P23_registrarAceptacionDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P23_registrarAceptacionDecision" expression="${P23_registrarAceptacionDecision}">
		<transition to="P23_validarAsignacion" name="NO"></transition>
		<transition to="P23_registrarInsinuacionCreditosDef" name="SI"></transition>
	</decision>

	<state name="P23_BPMTramiteDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_actualizarEstadoCreditos" name="tramiteDemandaIncidental"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P23_validarAsignacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P23_registrarAceptacion" name="avanzaBPM"></transition>
		<transition to="P23_asignarGestorLetradoSup" name="vuelataAtras"></transition>
		<transition to="P23_validarAsignacion" name="activarAlerta"></transition>
	</state>
	
	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>
