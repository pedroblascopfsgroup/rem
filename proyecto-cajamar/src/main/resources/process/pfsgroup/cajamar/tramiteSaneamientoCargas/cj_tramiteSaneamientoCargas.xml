<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="cj_tramiteSaneamientoCargas">

	<start-state name="Inicio">
		<transition to="CJ008_RevisarEstadoCargas" name="iniciarProceso"></transition>
	</start-state>


	<state name="CJ008_RevisarEstadoCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RevisarEstadoCargasDecision" name="avanzaBPM"></transition>
		<transition to="CJ008_RevisarEstadoCargas" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="CJ008_RevisarEstadoCargasDecision" expression="#{CJ008_RevisarEstadoCargasDecision}">
		<transition to="CJ008_RegistrarPresentacionInscripcion" name="soloRegistrales"></transition>
		<transition to="CJ008_SaneamientoDecision" name="noCargas"></transition>
		<transition to="CJ008_PropuestaCancelacionCargas" name="conEconomicas"></transition>
	</decision>

	<state name="CJ008_RegistrarPresentacionInscripcion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegInsCancelacionCargasReg" name="avanzaBPM"></transition>
		<transition to="CJ008_RegistrarPresentacionInscripcion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ008_RevisarEstadoCargas" name="vueltaAtras"></transition>
	</state>

	<state name="CJ008_PropuestaCancelacionCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_PropuestaCancelacionCargasDecision" name="avanzaBPM"></transition>
		<transition to="CJ008_PropuestaCancelacionCargas" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="CJ008_RevisarEstadoCargas" name="vueltaAtras"></transition>
	</state>

	<state name="CJ008_RegInsCancelacionCargasReg"> 
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegistrarPresentacionInscripcion" name="vueltaAtras"></transition>
		<transition to="CJ008_RegInsCancelacionCargasReg" name="activarAlerta"></transition>
		<transition to="CJ008_RegInsCancelacionCargasRegDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="CJ008_RevisarPropuestaCancelacionCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_LiquidarCargas" name="avanzaBPM"></transition>
		<transition to="CJ008_RevisarPropuestaCancelacionCargas" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="CJ008_ObtenerAprobPropuesta" name="vueltaAtras"></transition>
	</state>

	<state name="CJ008_LiquidarCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegInsCancelacionCargasEconomicas" name="avanzaBPM"></transition>
		<transition to="CJ008_LiquidarCargas" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="CJ008_RegInsCancelacionCargasEconomicas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegInsCancelacionCargasEconomicasDecision" name="avanzaBPM"></transition>
		<transition to="CJ008_RegInsCancelacionCargasEconomicas" name="activarAlerta"></transition>
		<transition to="CJ008_LiquidarCargas" name="vueltaAtras"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<decision name="CJ008_RegInsCancelacionCargasEconomicasDecision" expression="#{CJ008_RegInsCancelacionCargasEconomicasDecision}">
		<transition to="CJ008_RegistrarPresentacionInscripcionEco" name="conRegistrales"></transition>
		<transition to="CJ008_SaneamientoDecision" name="soloEconomicas"></transition>
		<transition to="Fin2" name="fin"></transition>
	</decision>

	<state name="CJ008_RegistrarPresentacionInscripcionEco">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegInsCancelacionCargasRegEco" name="avanzaBPM"></transition>
		<transition to="CJ008_RegInsCancelacionCargasEconomicas" name="vueltaAtras"></transition>
		<transition to="CJ008_RegistrarPresentacionInscripcionEco" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="CJ008_RegInsCancelacionCargasRegEco"> 
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_RegistrarPresentacionInscripcionEco" name="vueltaAtras"></transition>
		<transition to="CJ008_RegInsCancelacionCargasRegEco" name="activarAlerta"></transition>
		<transition to="CJ008_RegInsCancelacionCargasRegEcoDecision" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<node name="CJ008_SaneamientoDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>	
		<transition to="Fin2" name="Fin"></transition>
	</node>

	<decision name="CJ008_RegInsCancelacionCargasRegDecision" expression="#{CJ008_RegInsCancelacionCargasRegDecision}">
		<transition to="CJ008_SaneamientoDecision" name="Decidir"></transition>
		<transition to="Fin" name="Fin"></transition>
	</decision>

	<decision name="CJ008_PropuestaCancelacionCargasDecision" expression="#{CJ008_PropuestaCancelacionCargasDecision}">
		<transition to="CJ008_LiquidarCargas" name="sinAprobacion"></transition>
		<transition to="CJ008_ObtenerAprobPropuesta" name="reqAprobacion"></transition>
	</decision>

	<state name="CJ008_ObtenerAprobPropuesta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ008_PropuestaCancelacionCargas" name="vueltaAtras"></transition>
		<transition to="CJ008_ObtenerAprobPropuesta" name="activarAlerta"></transition>
		<transition to="CJ008_RevisarPropuestaCancelacionCargas" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<decision name="CJ008_RegInsCancelacionCargasRegEcoDecision" expression="#{CJ008_RegInsCancelacionCargasRegEcoDecision}">
		<transition to="CJ008_SaneamientoDecision" name="Decidir"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</decision>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>
	
	
</process-definition>