<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="hcj_tramiteSubastaTerceros">

	<start-state name="Inicio">
		<transition to="CJ004_SenyalamientoSubasta" name="iniciarProceso"></transition>
	</start-state>


	<state name="CJ004_SenyalamientoSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaCajamarEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="fork1" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="CJ004_SenyalamientoSubasta" name="activarAlerta"></transition>
	</state>

	<state name="CJ004_PrepararInformeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_ValidarInformeDeSubasta" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="CJ004_PrepararInformeSubasta" name="activarAlerta"></transition>
	</state>

	<state name="CJ004_ValidarInformeDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
