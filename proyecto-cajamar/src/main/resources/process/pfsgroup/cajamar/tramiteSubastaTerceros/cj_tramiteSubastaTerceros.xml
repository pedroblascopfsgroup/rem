		</event>	
		<transition to="CJ004_AdjuntarTasacionesDecision" name="avanzaBPM"></transition>
		<transition to="CJ004_AdjuntarTasaciones" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="CJ004_SolicitarInformeFiscal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_AdjuntarInformeFiscal" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="CJ004_SolicitarInformeFiscal" name="activarAlerta"></transition>
		<transition to="CJ004_AdjuntarTasaciones" name="vueltaAtras"></transition>
	</state>

	<state name="CJ004_AdjuntarInformeFiscal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin3" name="Fin"></transition>
		<transition to="Fin3" name="avanzaBPM"></transition>
		<transition to="CJ004_AdjuntarInformeFiscal" name="activarAlerta"></transition>
		<transition to="CJ004_SolicitarInformeFiscal" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ004_AdjuntarTasacionesDecision" expression="#{CJ004_AdjuntarTasacionesDecision}">
		<transition to="CJ004_SolicitarInformeFiscal" name="SI"></transition>
		<transition to="Fin2" name="NO"></transition>
	</decision>

	<state name="CJ004_SolicitarSuspenderSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_SolicitarSuspenderSubasta" name="activarAlerta"></transition>
		<transition to="CJ004_DictarInstruccionesSubastaDecision" name="vueltaAtras"></transition>
		<transition to="CJ004_RegistrarResSuspSubasta" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
	</state>


