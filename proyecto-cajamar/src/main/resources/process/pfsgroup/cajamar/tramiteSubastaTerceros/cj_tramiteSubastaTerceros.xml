		</event>	
		<transition to="fork1" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="CJ070_SenyalamientoSubasta" name="activarAlerta"></transition>
	</state>

	<state name="CJ070_PrepararInformeSubasta">
