	<decision name="CJ070_RecepcionMandamientoPagoDecision" expression="#{CJ070_RecepcionMandamientoPagoDecision}">
		<transition to="CJ070_ConfirmarContabilidad" name="DeudaCubierta"></transition>
		<transition to="CJ070_ConfirmarContabilidad" name="ViviendaHabitual"></transition>
		<transition to="fork2" name="DeudaNoCubierta"></transition>
	</decision>

	<decision name="CJ070_ObtenerValidacionComiteDecision" expression="#{CJ070_ObtenerValidacionComiteDecision}">
		<transition to="CJ070_DictarInstruccionesSubasta" name="Aceptada"></transition>
		<transition to="CJ070_PrepararInformeSubasta" name="Rechazada"></transition>
	</decision>

	<fork name="forkCelebracion">
		<transition to="B" name="B"></transition>
		<transition to="D" name="D"></transition>
		<transition to="C" name="C"></transition>
		<transition to="F" name="F"></transition>
		<transition to="G" name="G"></transition>
	</fork>

	<decision name="B" expression="#{DecisionRama_2}">
		<transition to="CJ070_CelebracionDecision" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="D" expression="#{DecisionRama_4}">
		<transition to="CJ070_BPMTramiteAdjudicacion" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="C" expression="#{DecisionRama_3}">
		<transition to="CJ070_BPMTramiteCesionRemate" name="si"></transition>
		<transition to="Fin4" name="no"></transition>
	</decision>

	<decision name="F" expression="#{DecisionRama_5}">
		<transition to="CJ070_SolicitarMandamientoPago" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="G" expression="#{DecisionRama_6}">
		<transition to="CJ070_BPMTramiteSolSolvenciaPatrimonial" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

