		<transition to="Fin3" name="Fin"></transition>
		<transition to="CJ004_ValidarInformeDeSubasta" name="activarAlerta"></transition>
		<transition to="CJ004_PrepararInformeSubasta" name="vueltaAtras"></transition>
	</state>

	<fork name="fork1">
		<transition to="CJ004_CelebracionSubasta" name="celebracionDeSubasta"></transition>
		<transition to="CJ004_RevisarDocumentacion" name="revisarDocumentacion"></transition>
	</fork>

	<state name="CJ004_RegistrarResSuspSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_SuspenderDecision" name="avanzaBPM"></transition>
		<transition to="CJ004_RegistrarResSuspSubasta" name="activarAlerta"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="CJ004_SolicitarSuspenderSubasta" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ004_ValidarInformeDeSubastaDecision" expression="#{CJ004_ValidarInformeDeSubastaDecision}">
		<transition to="Fin3" name="Fin"></transition>
		<transition to="CJ004_PrepararInformeSubasta" name="Rechazado"></transition>
		<transition to="CJ004_ObtenerValidacionComite" name="AceptadoSinAtribuciones"></transition>
		<transition to="CJ004_DictarInstruccionesSubasta" name="AceptadoConAtribuciones"></transition>
	</decision>
	
	<state name="CJ004_ObtenerValidacionComite">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_ObtenerValidacionComite" name="activarAlerta"></transition>
		<transition to="CJ004_ObtenerValidacionComiteDecision" name="avanzaBPM"></transition>
		<transition to="CJ004_ValidarInformeDeSubasta" name="vueltaAtras"></transition>
	</state>

	<state name="CJ004_DictarInstruccionesSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_DictarInstruccionesSubastaDecision" name="avanzaBPM"></transition>
		<transition to="CJ004_DictarInstruccionesSubasta" name="activarAlerta"></transition>
		<transition to="CJ004_ValidarInformeDeSubasta" name="vueltaAtras"></transition>
		<transition to="Fin4" name="Fin"></transition>
	</state>

	<state name="CJ004_CelebracionSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="forkCelebracion" name="avanzaBPM"></transition>
		<transition to="CJ004_CelebracionSubasta" name="activarAlerta"></transition>
		<transition to="Fin5" name="Fin"></transition>
	</state>

	<node name="CJ004_CelebracionDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>	
		<transition to="Fin5" name="Fin"></transition>
	</node>

	<state name="CJ004_BPMTramiteCesionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaCajamarEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="hcj_tramiteCesionRemate"></transition>
	</state>

	<state name="CJ004_BPMTramiteAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaCajamarEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin5" name="hcj_tramiteAdjudicacion"></transition>
		<transition to="Fin5" name="Fin"></transition>
	</state>

	<state name="CJ004_SolicitarMandamientoPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_ConfirmarRecepcionMandamientoDePago" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_SolicitarMandamientoPago" name="activarAlerta"></transition>
	</state>

	<state name="CJ004_ConfirmarRecepcionMandamientoDePago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_RecepcionMandamientoPago" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_ConfirmarRecepcionMandamientoDePago" name="activarAlerta"></transition>
		<transition to="CJ004_SolicitarMandamientoPago" name="vueltaAtras"></transition>
	</state>

	<state name="CJ004_RecepcionMandamientoPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_RecepcionMandamientoPagoDecision" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_RecepcionMandamientoPago" name="activarAlerta"></transition>
		<transition to="CJ004_ConfirmarRecepcionMandamientoDePago" name="vueltaAtras"></transition>
	</state>

	<decision name="CJ004_RecepcionMandamientoPagoDecision" expression="#{CJ004_RecepcionMandamientoPagoDecision}">
		<transition to="CJ004_ConfirmarContabilidad" name="DeudaCubierta"></transition>
		<transition to="CJ004_ConfirmarContabilidad" name="ViviendaHabitual"></transition>
		<transition to="fork2" name="DeudaNoCubierta"></transition>
	</decision>

	<decision name="CJ004_ObtenerValidacionComiteDecision" expression="#{CJ004_ObtenerValidacionComiteDecision}">
		<transition to="CJ004_DictarInstruccionesSubasta" name="Aceptada"></transition>
		<transition to="CJ004_PrepararInformeSubasta" name="Rechazada"></transition>
	</decision>

	<fork name="forkCelebracion">
		<transition to="B" name="B"></transition>
		<transition to="D" name="D"></transition>
		<transition to="C" name="C"></transition>
		<transition to="F" name="F"></transition>
		<transition to="G" name="G"></transition>
	</fork>

	<decision name="B" expression="#{DecisionRama_2}">
		<transition to="CJ004_CelebracionDecision" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="D" expression="#{DecisionRama_4}">
		<transition to="CJ004_BPMTramiteAdjudicacion" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="C" expression="#{DecisionRama_3}">
		<transition to="CJ004_BPMTramiteCesionRemate" name="si"></transition>
		<transition to="Fin4" name="no"></transition>
	</decision>

	<decision name="F" expression="#{DecisionRama_5}">
		<transition to="CJ004_SolicitarMandamientoPago" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="G" expression="#{DecisionRama_6}">
		<transition to="CJ004_BPMTramiteSolSolvenciaPatrimonial" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<node name="CJ004_SuspenderDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin3" name="Fin"></transition>	
	</node>

	<state name="CJ004_BPMTramiteSolSolvenciaPatrimonial">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin" name="hcj_solicitudSolvenciaPatrimonial"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="CJ004_BPMTramiteSolSolvenciaPatrimonial" name="activarAlerta"></transition>
	</state>

	<state name="CJ004_ConfirmarContabilidad">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_ConfirmarContabilidad" name="activarAlerta"></transition>
	</state>

	<fork name="fork2">
		<transition to="CJ004_BPMTramiteSolSolvenciaPatrimonial" name="Solvencia"></transition>
		<transition to="CJ004_ConfirmarContabilidad" name="Contabilidad"></transition>
	</fork>

	<state name="CJ004_RevisarDocumentacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="CJ004_RevisarDocumentacion" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="fork3" name="avanzaBPM"></transition>
	</state>

	<decision name="CJ004_DictarInstruccionesSubastaDecision" expression="#{CJ004_DictarInstruccionesSubastaDecision}">
		<transition to="CJ004_SolicitarSuspenderSubasta" name="Suspender"></transition>
		<transition to="CJ004_LecturaConfirmacionInstrucciones" name="NoSuspender"></transition>
	</decision>

	<state name="CJ004_LecturaConfirmacionInstrucciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaTercerosCajamarLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="Fin"></transition>
		<transition to="CJ004_DictarInstruccionesSubasta" name="vueltaAtras"></transition>
		<transition to="CJ004_LecturaConfirmacionInstrucciones" name="activarAlerta"></transition>
		<transition to="Fin4" name="avanzaBPM"></transition>
	</state>

	<decision name="NS" expression="#{decisionDoc_1}">
		<transition to="Fin2" name="no"></transition>
		<transition to="CJ004_AdjuntarNotasSimples" name="si"></transition>
	</decision>

	<decision name="TA" expression="#{decisionDoc_2}">
		<transition to="Fin2" name="no"></transition>
		<transition to="CJ004_AdjuntarTasaciones" name="si"></transition>
	</decision>

	<decision name="IF" expression="#{decisionDoc_3}">
		<transition to="Fin3" name="no"></transition>
		<transition to="CJ004_SolicitarInformeFiscal" name="si"></transition>
	</decision>

	<decision name="IS" expression="#{decisionDoc_4}">
		<transition to="CJ004_PrepararInformeSubasta" name="si"></transition>
		<transition to="Fin3" name="no"></transition>
	</decision>

	<state name="CJ004_AdjuntarNotasSimples">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
