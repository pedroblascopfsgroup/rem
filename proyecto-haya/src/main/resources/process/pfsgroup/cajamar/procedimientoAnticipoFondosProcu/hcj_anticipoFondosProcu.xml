<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="hcj_anticipoFondosProcu" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P460_SolicitarAnticipoFondos" name="avanzaBPM"></transition>
	</start-state>


	<state name="P460_VisarSolicitud">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_VisarSolicitud" name="activarAlerta"></transition>
		<transition to="P460_VisarSolicitudDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>


	<decision name="P460_VisarSolicitudDecision" expression="#{P460_VisarSolicitudDecision}">
		<transition to="P460_RevisarSolicitud" name="SI"></transition>
		<transition to="P460_AprobarSolicitudFondos" name="NO"></transition>
		<transition to="P460_SolicitarAnticipoFondos" name="KO"></transition>
	</decision>

	<state name="P460_RevisarSolicitud">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_RevisarSolicitud" name="activarAlerta"></transition>
		<transition to="P460_RevisarSolicitudDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P460_VisarSolicitud" name="vueltraAtras"></transition>
	</state>

	<decision name="P460_RevisarSolicitudDecision" expression="#{P460_RevisarSolicitudDecision}">
		<transition to="P460_SolicitarAnticipoFondos" name="subsanar"></transition>
		<transition to="P460_AprobarSolicitudFondos" name="cesion"></transition>
		<transition to="P460_AprobarSolicitudInsc" name="otro"></transition>
	</decision>

	<state name="P460_AprobarSolicitudFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_AprobarSolicitudFondos" name="activarAlerta"></transition>
		<transition to="P460_AprobarSolicitudFondosDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P460_AprobarSolicitudInsc">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_RevisarSolicitud" name="vueltaAtras"></transition>
		<transition to="P460_AprobarSolicitudInsc" name="activarAlerta"></transition>
		<transition to="P460_AprobarSolicitudInscDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P460_AprobarSolicitudFondosDecision" expression="#{P460_AprobarSolicitudFondosDecision}">
		<transition to="P460_ConfirmarSolicitud" name="sinatribuciones"></transition>
		<transition to="P460_RealizarTransferencia" name="APR"></transition>
		<transition to="P460_DecisionAprobFondos" name="REC"></transition>
		<transition to="P460_SolicitarAnticipoFondos" name="SUB"></transition>
	</decision>


	<state name="P460_RealizarTransferencia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>tramiteAntFondosLeaveActionHandler<!--AQUI MODIFICAR HANDLER PARA ENVIAR NOTIFICACION PROCURADOR-->
				</targetBean>
			</action>
		</event>
		<transition to="P460_RealizarTransferencia" name="activarAlerta"></transition>
		<transition to="P460_ConfirmarTransferencia" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<node name="P460_DecisionAprobFondos">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>	
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P460_ConfirmarSolicitud">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_AprobarSolicitudFondos" name="vueltaAtras"></transition>
		<transition to="P460_ConfirmarSolicitudDecision" name="avanzaBPM"></transition>
		<transition to="P460_ConfirmarSolicitud" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P460_AprobarSolicitudInscDecision" expression="#{P460_AprobarSolicitudInscDecision}">
		<transition to="P460_DecisionAprobarSolcIns" name="RECHAZADO"></transition>
		<transition to="P460_RealizarTransferencia" name="ACEPTADO"></transition>
	</decision>

	<node name="P460_DecisionAprobarSolcIns">
		<transition to="Fin" name="Fin"></transition>
	</node>

	<decision name="P460_ConfirmarSolicitudDecision" expression="#{P460_ConfirmarSolicitudDecision}">
		<transition to="P460_RealizarTransferencia" name="aceptado"></transition>
		<transition to="Fin" name="denegado"></transition>
	</decision>

	<state name="P460_ConfirmarTransferencia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_RealizarTransferencia" name="vueltaAtras"></transition>
		<transition to="P460_ConfirmarTransferencia" name="activarAlerta"></transition>
		<transition to="P460_ConfirmarTransferenciaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P460_ConfirmarTransferenciaDecision" expression="#{P460_ConfirmarTransferenciaDecision}">
		<transition to="Fin" name="PAGSUP"></transition>
		<transition to="P460_AdjuntarFactura" name="ANTFON"></transition>
	</decision>

	<state name="P460_AdjuntarFactura">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P460_AdjuntarFactura" name="activarAlerta"></transition>
		<transition to="P460_ConfirmarTransferencia" name="vueltaAtras"></transition>
	</state>

	<state name="P460_SolicitarAnticipoFondos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P460_VisarSolicitud" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P460_SolicitarAnticipoFondos" name="activarAlerta"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>
