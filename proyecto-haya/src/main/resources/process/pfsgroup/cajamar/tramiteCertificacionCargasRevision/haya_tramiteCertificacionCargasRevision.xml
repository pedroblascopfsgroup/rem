<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="hcj_tramiteCertificacionCargasRevision"
	xmlns="urn:jbpm.org:jpdl-3.2">



	<start-state name="Inicio">
		<transition to="H030_SolicitudCertificacion" name="avanzaBPM"></transition>
	</start-state>


	<state name="H030_SolicitarInformacionCargasAnt">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_SolicitarInformacionCargasAnt" name="activarAlerta"></transition>
		<transition to="H030_SolicitarInformacionCargasAntDecision" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H030_SolicitudCertificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_SolicitudCertificacion" name="activarAlerta"></transition>
		<transition to="H030_RegistrarCertificacion" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H030_RegistrarCertificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_RegistrarCertificacion" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H030_SolicitudCertificacion" name="vueltaAtras"></transition>
		<transition to="H030_SolicitarInformacionCargasAnt" name="avanzaBPM"></transition>
	</state>

	<decision name="H030_SolicitarInformacionCargasAntDecision"
		expression="#{H030_SolicitarInformacionCargasAntDecision}">
		<transition to="H030_CertificacionDecision" name="NO"></transition>
		<transition to="H030_RegistrarInformacionCargasAnt" name="SI"></transition>
	</decision>

	<state name="H030_RegistrarInformacionCargasAnt">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_RegistrarInformacionCargasAnt" name="activarAlerta"></transition>
		<transition to="H030_RegistrarInformacionCargasAntDecision"
			name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H030_SolicitarInformacionCargasAnt" name="vueltaAtras"></transition>
	</state>

	<node name="H030_CertificacionDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin"></transition>
	</node>

	<decision name="H030_RegistrarInformacionCargasAntDecision"
		expression="#{H030_RegistrarInformacionCargasAntDecision}">
		<transition to="H030_ActualizarDatosTerceria" name="Completo"></transition>
		<transition to="H030_RequerirInfoFalta" name="Incompleto"></transition>
	</decision>

	<state name="H030_RequerirInfoFalta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_RequerirInfoFalta" name="activarAlerta"></transition>
		<transition to="H030_RegistrarInformacionCargasAnt" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H030_RegistrarInformacionCargasAnt" name="vueltaAtras"></transition>
	</state>

	<state name="H030_ActualizarDatosTerceria">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H030_ActualizarDatosTerceria" name="activarAlerta"></transition>
		<transition to="H030_CertificacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	
	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>