<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="hcj_tramiteCostasVsEntidad">

	<start-state name="Inicio">
		<transition to="H032_RegistrarSolCostasContrario" name="avanzaBPM"></transition>
	</start-state>

	<state name="H032_RegistrarSolCostasContrario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H032_RegistrarDecTasacionCostas" name="avanzaBPM"></transition>
		<transition to="H032_RegistrarSolCostasContrario" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H032_ConfirmarPresImpugnacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H032_ConfirmarPresImpugnacion" name="activarAlerta"></transition>
		<transition to="H032_RegistrarDecTasacionCostas" name="vueltaAtras"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H032_ConfirmarPresImpugnacionDecision" name="avanzaBPM"></transition>
	</state>

	<state name="H032_RegistrarDecTasacionCostas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="H032_RegistrarDecTasacionCostasDecision" name="avanzaBPM"></transition>
		<transition to="H032_RegistrarDecTasacionCostas" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H032_RegistrarSolCostasContrario" name="vueltaAtras"></transition>
	</state>

	<state name="H032_RegistrarCelebracionVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="H032_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="H032_RegistrarCelebracionVista" name="activarAlerta"></transition>
		<transition to="H032_ConfirmarPresImpugnacion" name="vueltaAtras"></transition>
	</state>

	<decision name="H032_RegistrarDecTasacionCostasDecision" expression="#{H032_RegistrarDecTasacionCostasDecision}">
		<transition to="H032_ConfirmarPresImpugnacion" name="SI"></transition>
		<transition to="H032_AutorizarPago" name="NO"></transition>
	</decision>

	<state name="H032_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H032_ResolucionFirme" name="avanzaBPM"></transition>
		<transition to="H032_RegistrarCelebracionVista" name="vueltaAtras"></transition>
		<transition to="H032_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H032_ConfirmarPresImpugnacion" name="vueltaAtras"></transition>
	</state>

	<state name="H032_ResolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="H032_ResolucionFirmeDecision" name="avanzaBPM"></transition>
		<transition to="H032_RegistrarResolucion" name="vueltaAtras"></transition>
		<transition to="H032_ResolucionFirme" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H032_RegistrarPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H032_RegistrarPago" name="activarAlerta"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<decision name="H032_ResolucionFirmeDecision" expression="#{H032_ResolucionFirmeDecision}">
		<transition to="H032_AutorizarPago" name="favorable"></transition>
		<transition to="H032_ResolucionDecision" name="desfavorable"></transition>
	</decision>

	<node name="H032_ResolucionDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<decision name="H032_ConfirmarPresImpugnacionDecision" expression="#{H032_ConfirmarPresImpugnacionDecision}">
		<transition to="H032_RegistrarCelebracionVista" name="Indebida"></transition>
		<transition to="H032_RegistrarResolucion" name="Excesiva"></transition>
		<transition to="H032_RegistrarCelebracionVista" name="Ambas"></transition>
	</decision>

	<state name="H032_AutorizarPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H032_AutorizarPago" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H032_AutorizarPagoDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="H032_AutorizarPagoDecision" expression="#{H032_AutorizarPagoDecision}">
		<transition to="H032_RegistrarPago" name="aceptado"></transition>
		<transition to="Fin" name="denegado"></transition>
	</decision>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>