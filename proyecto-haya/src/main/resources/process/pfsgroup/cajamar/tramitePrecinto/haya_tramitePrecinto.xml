<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="hcj_tramitePrecinto" xmlns="urn:jbpm.org:jpdl-3.2">



	<start-state name="Inicio">
		<transition to="H050_SolicitarPrecinto"></transition>
	</start-state>


	<state name="H050_SolicitarPrecinto">
		<event type="node-enter">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericEnterActionHandler</targetBean>
        	 </action>
      	</event>
      	<event type="node-leave">
	         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericLeaveActionHandler</targetBean>
        	 </action>
      	</event>
      	<transition to="H050_SolicitarPrecinto" name="activarAlerta"></transition>
		<transition to="H050_AcuerdoPrecinto" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H050_AcuerdoPrecinto">
		<event type="node-enter">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericEnterActionHandler</targetBean>
        	 </action>
      	</event>
      	<event type="node-leave">
	         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericLeaveActionHandler</targetBean>
        	 </action>
      	</event>
      	<transition to="H050_AcuerdoPrecinto" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H050_SolicitarPrecinto" name="vueltaAtras"></transition>
		<transition to="H050_ConfirmarFechaPrecintoEfectivo" name="avanzaBPM"></transition>
	</state>

	<node name="H050_PrecintoDecision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
		<transition to="Fin"></transition>
	</node>

	<state name="H050_ConfirmarFechaPrecintoEfectivo">
		<event type="node-enter">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericEnterActionHandler</targetBean>
        	 </action>
      	</event>
      	<event type="node-leave">
	         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericLeaveActionHandler</targetBean>
        	 </action>
      	</event>
		<transition to="H050_PrecintoDecision" name="avanzaBPM"></transition>
		<transition to="H050_AcuerdoPrecinto" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H050_ConfirmarFechaPrecintoEfectivo" name="activarAlerta"></transition>
	</state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>