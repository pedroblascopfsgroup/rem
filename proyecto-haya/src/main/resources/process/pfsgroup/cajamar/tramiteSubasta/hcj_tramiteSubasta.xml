				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="H002_ConfirmarContabilidad" name="activarAlerta"></transition>
	</state>

	<fork name="fork2">
		<transition to="H002_BPMTramiteSolSolvenciaPatrimonial" name="Solvencia"></transition>
		<transition to="H002_ConfirmarContabilidad" name="Contabilidad"></transition>
	</fork>

	<state name="H002_RevisarDocumentacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
