			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="fork1" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H002_SenyalamientoSubasta" name="activarAlerta"></transition>
		<transition to="H002_SolicitudSubasta" name="vueltaAtras"></transition>
	</state>

	<state name="H002_PrepararInformeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_ValidarInformeDeSubasta" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H002_PrepararInformeSubasta" name="activarAlerta"></transition>
	</state>

	<state name="H002_ValidarInformeDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_ValidarInformeDeSubastaDecision" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H002_ValidarInformeDeSubasta" name="activarAlerta"></transition>
		<transition to="H002_PrepararInformeSubasta" name="vueltaAtras"></transition>
	</state>

	<fork name="fork1">
		<transition to="H002_CelebracionSubasta" name="celebracionDeSubasta"></transition>
		<transition to="H002_RevisarDocumentacion" name="revisarDocumentacion"></transition>
	</fork>

	<state name="H002_RegistrarResSuspSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_RegistrarResSuspSubastaDecision" name="avanzaBPM"></transition>
		<transition to="H002_RegistrarResSuspSubasta" name="activarAlerta"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H002_SolicitarSuspenderSubasta" name="vueltaAtras"></transition>
	</state>

	<decision name="H002_ValidarInformeDeSubastaDecision" expression="#{H002_ValidarInformeDeSubastaDecision}">
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H002_PrepararInformeSubasta" name="Rechazado"></transition>
		<transition to="H002_ObtenerValidacionComite" name="AceptadoSinAtribuciones"></transition>
		<transition to="H002_DictarInstruccionesSubasta" name="AceptadoConAtribuciones"></transition>
	</decision>
	
	<state name="H002_ObtenerValidacionComite">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="Fin"></transition>
		<transition to="H002_ObtenerValidacionComite" name="activarAlerta"></transition>
		<transition to="H002_ObtenerValidacionComiteDecision" name="avanzaBPM"></transition>
		<transition to="H002_ValidarInformeDeSubasta" name="vueltaAtras"></transition>
	</state>

	<state name="H002_DictarInstruccionesSubasta">
		<event type="node-enter">
			<action name="miTarea"
