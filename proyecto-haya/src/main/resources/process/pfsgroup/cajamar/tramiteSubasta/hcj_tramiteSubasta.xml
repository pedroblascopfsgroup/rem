			</action>
		</event>	
		<transition to="H002_RevisarDocumentacion" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="fork3" name="avanzaBPM"></transition>
	</state>

	<decision name="H002_RegistrarResSuspSubastaDecision" expression="#{H002_RegistrarResSuspSubastaDecision}">
		<transition to="H002_BPMTramiteSubasta" name="Aceptado"></transition>
		<transition to="H002_DictarInstruccionesDeneSuspension" name="Denegado"></transition>
	</decision>

	<state name="H002_DictarInstruccionesDeneSuspension">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_DictarInstruccionesDeneSuspension"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H002_RegistrarResSuspSubasta" name="vueltaAtras"></transition>
		<transition to="Fin3" name="avanzaBPM"></transition>
	</state>

	<decision name="H002_DictarInstruccionesSubastaDecision" expression="#{H002_DictarInstruccionesSubastaDecision}">
		<transition to="H002_SolicitarSuspenderSubasta" name="Suspender"></transition>
		<transition to="H002_LecturaConfirmacionInstrucciones" name="NoSuspender"></transition>
