			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin" name="hcj_solicitudSolvenciaPatrimonial"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H002_BPMTramiteSolSolvenciaPatrimonial" name="activarAlerta"></transition>
	</state>

	<state name="H002_ConfirmarContabilidad">
