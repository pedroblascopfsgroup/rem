		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_RecepcionMandamientoPago" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="H002_ConfirmarRecepcionMandamientoDePago" name="activarAlerta"></transition>
		<transition to="H002_SolicitarMandamientoPago" name="vueltaAtras"></transition>
	</state>

	<state name="H002_RecepcionMandamientoPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_RecepcionMandamientoPagoDecision" name="avanzaBPM"></transition>
		<transition to="Fin4" name="Fin"></transition>
		<transition to="H002_RecepcionMandamientoPago" name="activarAlerta"></transition>
		<transition to="H002_ConfirmarRecepcionMandamientoDePago" name="vueltaAtras"></transition>
	</state>

	<decision name="H002_RecepcionMandamientoPagoDecision" expression="#{H002_RecepcionMandamientoPagoDecision}">
		<transition to="H002_ConfirmarContabilidad" name="DeudaCubierta"></transition>
		<transition to="H002_ConfirmarContabilidad" name="ViviendaHabitual"></transition>
		<transition to="fork2" name="DeudaNoCubierta"></transition>
	</decision>

	<decision name="H002_ObtenerValidacionComiteDecision" expression="#{H002_ObtenerValidacionComiteDecision}">
		<transition to="H002_DictarInstruccionesSubasta" name="Aceptada"></transition>
		<transition to="H002_PrepararInformeSubasta" name="Rechazada"></transition>
	</decision>

	<fork name="forkCelebracion">
		<transition to="A" name="A"></transition>
		<transition to="B" name="B"></transition>
		<transition to="D" name="D"></transition>
		<transition to="C" name="C"></transition>
		<transition to="F" name="F"></transition>
		<transition to="G" name="G"></transition>
	</fork>

	<decision name="A" expression="#{DecisionRama_1}">
		<transition to="H002_BPMTramiteSubasta" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="B" expression="#{DecisionRama_2}">
		<transition to="H002_BPMTramiteSubasta" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="D" expression="#{DecisionRama_4}">
		<transition to="H002_BPMTramiteAdjudicacion" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="C" expression="#{DecisionRama_3}">
		<transition to="H002_BPMTramiteCesionRemate" name="si"></transition>
		<transition to="Fin4" name="no"></transition>
	</decision>

	<decision name="F" expression="#{DecisionRama_5}">
		<transition to="H002_SolicitarMandamientoPago" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<decision name="G" expression="#{DecisionRama_6}">
		<transition to="H002_BPMTramiteSolSolvenciaPatrimonial" name="si"></transition>
		<transition to="Fin5" name="no"></transition>
	</decision>

	<state name="H002_BPMTramiteSolSolvenciaPatrimonial">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
