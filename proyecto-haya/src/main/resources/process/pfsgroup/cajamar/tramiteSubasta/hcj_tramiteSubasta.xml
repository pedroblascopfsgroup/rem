		<transition to="Fin2" name="no"></transition>
		<transition to="H002_AdjuntarNotasSimples" name="si"></transition>
	</decision>

	<decision name="TA" expression="#{decisionDoc_2}">
		<transition to="Fin2" name="no"></transition>
		<transition to="H002_AdjuntarTasaciones" name="si"></transition>
	</decision>

	<decision name="IF" expression="#{decisionDoc_3}">
