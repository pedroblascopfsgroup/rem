			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_DictarInstruccionesSubastaDecision" name="avanzaBPM"></transition>
		<transition to="H002_DictarInstruccionesSubasta" name="activarAlerta"></transition>
		<transition to="H002_ValidarInformeDeSubasta" name="vueltaAtras"></transition>
		<transition to="Fin4" name="Fin"></transition>
	</state>

	<state name="H002_CelebracionSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="forkCelebracion" name="avanzaBPM"></transition>
		<transition to="H002_CelebracionSubasta" name="activarAlerta"></transition>
		<transition to="Fin5" name="Fin"></transition>
	</state>

	<state name="H002_BPMTramiteSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin5" name="hcj_tramiteSubasta"></transition>
		<transition to="Fin5" name="Fin"></transition>
	</state>

	<node name="H002_CelebracionDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>	
		<transition to="Fin5" name="Fin"></transition>
	</node>

	<state name="H002_BPMTramiteCesionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin4" name="hcj_tramiteCesionRemate"></transition>
	</state>

	<state name="H002_BPMTramiteAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
