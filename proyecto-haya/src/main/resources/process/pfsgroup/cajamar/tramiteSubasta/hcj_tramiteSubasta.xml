		<transition to="H002_SolicitarInformeFiscal" name="si"></transition>
	</decision>

	<decision name="IS" expression="#{decisionDoc_4}">
		<transition to="H002_PrepararInformeSubasta" name="si"></transition>
		<transition to="Fin3" name="no"></transition>
	</decision>

