		<transition to="Fin2" name="Fin"></transition>
		<transition to="Fin2" name="avanzaBPM"></transition>
	</state>

	<fork name="fork3">
		<transition to="NS" name="NS"></transition>
		<transition to="TA" name="TA"></transition>
		<transition to="IF" name="IF"></transition>
		<transition to="IS" name="IS"></transition>
	</fork>

	<state name="H002_AdjuntarTasaciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
