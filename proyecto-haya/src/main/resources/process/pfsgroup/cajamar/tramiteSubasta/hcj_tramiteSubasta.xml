				<targetBean>subastaBccEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaBccLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H002_SenyalamientoSubasta" name="avanzaBPM"></transition>
		<transition to="Fin5" name="Fin"></transition>
		<transition to="H002_SolicitudSubasta" name="activarAlerta"></transition>
	</state>

	<state name="H002_SenyalamientoSubasta">
		<event type="node-enter">
			<action name="miTarea"
