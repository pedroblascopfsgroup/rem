			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_AdjuntarTasacionesDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P458_AdjuntarTasacionesDecision" expression="#{P458_AdjuntarTasacionesDecision}">
		<transition to="P458_SolicitarInformeFiscal" name="si"></transition>
		<transition to="Fin" name="no"></transition>
	</decision>

	<state name="P458_SolicitarInformeFiscal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_AdjuntarInformeFiscal" name="avanzaBPM"></transition>
	</state>

	<state name="P458_AdjuntarInformeFiscal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="P458_SolicitarInformeFiscal" name="vueltaAtras"></transition>
	</state>

	<state name="P458_PrepararInformeDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_ValidarInformeDeSubasta" name="avanzaBPM"></transition>
	</state>

	<state name="P458_ValidarInformeDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_ValidarInformeDeSubastaDecision" name="avanzaBPM"></transition>
		<transition to="P458_PrepararInformeDeSubasta" name="vueltaAtras"></transition>
	</state>

	<decision name="P458_ValidarInformeDeSubastaDecision" expression="#{P458_ValidarInformeDeSubastaDecision}">
		<transition to="P458_PrepararInformeDeSubasta" name="rechazado"></transition>
		<transition to="P458_DictarInstruccionesDeSubastaYPagoDeTasa" name="aceptadoConAtribuciones"></transition>
		<transition to="P458_ObtenerValidacionComite" name="aceptadoSinAtribuciones"></transition>
	</decision>
	
	<state name="P458_ObtenerValidacionComite">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_ObtenerValidacionComiteDecision" name="avanzaBPM"></transition>
		<transition to="P458_ValidarInformeDeSubasta" name="vueltaAtras"></transition>
	</state>
		
	<decision name="P458_ObtenerValidacionComiteDecision" expression="#{P458_ObtenerValidacionComiteDecision}">
		<transition to="P458_PrepararInformeDeSubasta" name="rechazado"></transition>
		<transition to="P458_PendienteConfirmacionPagoDeTasa" name="noPagarTasa"></transition>
		<transition to="P458_DictarInstruccionesDeSubastaYPagoDeTasa" name="siPagarTasa"></transition>
	</decision>

	<state name="P458_PendienteConfirmacionPagoDeTasa">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_PendienteConfirmacionPagoDeTasaDecision" name="avanzaBPM"></transition>
		<transition to="P458_ObtenerValidacionComite" name="vueltaAtras"></transition>
	</state>

	<decision name="P458_PendienteConfirmacionPagoDeTasaDecision" expression="#{P458_PendienteConfirmacionPagoDeTasaDecision}">
		<transition to="P458_DictarInstruccionesDeSubastaYPagoDeTasa" name="si"></transition>
		<transition to="Decision" name="no"></transition>
	</decision>

	<state name="P458_DictarInstruccionesDeSubastaYPagoDeTasa">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="NOTIFICACIONALLETRADO" name="avanzaBPM"></transition>
	</state>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        		<targetBean>genericUserDecisionActionHandler</targetBean>
    		</action>
    		<transition to="Fin" name="Fin"></transition>
	</node>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>  
	</end-state>

</process-definition>
