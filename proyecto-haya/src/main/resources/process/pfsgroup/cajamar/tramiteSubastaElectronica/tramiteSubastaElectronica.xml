<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2"
	name="tramiteSubastaElectronica">

	<start-state name="inicio">
		<transition to="P458_SolicitudDeSubasta"></transition>
	</start-state>

	<state name="P458_SolicitudDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>tramitesAcuerdosLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P458_DecretoConvocatoriaDeSubasta" name="avanzaBPM"></transition>
	</state>

	<state name="P458_DecretoConvocatoriaDeSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>tramitesAcuerdosLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="forkEntregaYRevisa" name="avanzaBPM"></transition>
		<transition to="P458_SolicitudDeSubasta" name="vueltaAtras"></transition>
	</state>

	<fork name="forkEntregaYRevisa">
		<transition to="P458_EntregaYRecepcionEdictoEImpresoTasa" name="entregaYRecepcionEdictoEImpresoTasa"></transition>
		<transition to="P458_RevisarDocumentacion" name="revisarDocumentacion"></transition>
	</fork>
	
