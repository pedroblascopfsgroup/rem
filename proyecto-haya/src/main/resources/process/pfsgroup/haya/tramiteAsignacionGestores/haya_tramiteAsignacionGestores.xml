<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="haya_tramiteAsignacionGestores">

	<start-state name="Inicio">
		<transition to="H067_seleccionarTipoAsunto"></transition>
	</start-state>

	<node name="H067_seleccionarTipoAsunto">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>asignacionHayaDecidirTipoAsuntoHandler
			</targetBean>
		</action>
		<transition to="H067_seleccionarProcedimiento" name="litigio"></transition>
		<transition to="forkConcurso" name="concurso"></transition>
	</node>

	<fork name="forkConcurso">
		<transition to="H067_AsignarGestorExpediente" name="asignarGestor"></transition>
		<transition to="H067_seleccionarProcedimiento" name="seleccionarProcedimiento"></transition>
	</fork>

	<state name="H067_seleccionarProcedimiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H067_seleccionarProcedimiento" name="activarAlerta"></transition>
		<transition to="H067_seleccionarProcedimientoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="H067_seleccionarProcedimientoDecision" expression="#{H067_seleccionarProcedimientoDecision}">
		<transition to="H067_transicionProcedimiento" name="litigio"></transition>
		<transition to="joinTransicion" name="concurso"></transition>		
	</decision>

	<state name="H067_AsignarGestorExpediente">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H067_AsignarGestorExpediente" name="activarAlerta"></transition>
		<transition to="joinTransicion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<join name="joinTransicion">
		<transition to="H067_transicionProcedimiento" name="avanzaBPM"></transition>
	</join>

	<node name="H067_transicionProcedimiento">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>asignacionHayaEnterActionHandler</targetBean>
      	</action>	
      	<transition to="Fin" name="Fin"></transition>
	</node>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>