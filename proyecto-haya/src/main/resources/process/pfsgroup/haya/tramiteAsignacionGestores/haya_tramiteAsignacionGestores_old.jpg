<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="haya_tramiteAsignacionGestores">

	<start-state name="Inicio">
		<transition to="H067_seleccionarTipoAsunto"></transition>
	</start-state>

	<node name="H067_seleccionarTipoAsunto">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>asignacionHayaDecidirTipoAsuntoHandler
			</targetBean>
		</action>
		<transition to="H067_seleccionarProcedimiento" name="litigio"></transition>
		<transition to="H067_SeleccionarProcedimientoAsignarGestor" name="concurso"></transition>
	</node>

	<state name="H067_seleccionarProcedimiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H067_seleccionarProcedimiento" name="activarAlerta"></transition>
		<transition to="H067_transicionProcedimiento" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H067_SeleccionarProcedimientoAsignarGestor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H067_SeleccionarProcedimientoAsignarGestor" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H067_transicionProcedimiento" name="avanzaBPM"></transition>
	</state>

	<node name="H067_transicionProcedimiento">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>asignacionHayaEnterActionHandler</targetBean>
      	</action>	
      	<transition to="Fin" name="Fin"></transition>
	</node>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>