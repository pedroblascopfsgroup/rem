<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="haya_tramiteFaseComunAbreviadoV4"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="H009_RegistrarPublicacionBOE" name="iniciarProceso"></transition>
	</start-state>

	<state name="H009_RegistrarPublicacionBOE">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H009_RegistrarPublicacionBOE" name="activarAlerta"></transition>
		<transition to="fork1" name="avanzaBPM"></transition>
	</state>
	
	<state name="H009_BPMTramiteAceptacionConcurso">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="forkTrasAceptacion" name="tramiteAceptacionConcurso"></transition>
	</state>
	
	
	<state name="H009_RegistrarInsinuacionCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H009_RegistrarInsinuacionCreditos" name="activarAlerta"></transition>
		<transition to="H009_RevisarInsinuacionCreditos" name="avanzaBPM"></transition>
	</state>

	<state name="H009_RegistrarInformeAdmonConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="H009_RevisarResultadoInfAdmon" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H009_RegistrarInformeAdmonConcursal" name="activarAlerta"></transition>
	</state>

	<state name="H009_PresentarEscritoInsinuacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="H009_RegistrarProyectoInventario" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H009_PresentarEscritoInsinuacion" name="activarAlerta"></transition>
	</state>

	<state name="H009_RevisarResultadoInfAdmon">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="H009_RevisarResultadoInfAdmonDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H009_RevisarResultadoInfAdmon" name="activarAlerta"></transition>
		<transition to="H009_RegistrarInformeAdmonConcursal" name="vueltaAtras"></transition>
	</state>

	<decision name="H009_RevisarResultadoInfAdmonDecision" expression="#{H009_RevisarResultadoInfAdmonDecision}">
		<transition to="H009_RegistrarResolucionFaseComun" name="NO"></transition>
		<transition to="H009_ValidarAlegaciones" name="SI"></transition>
	</decision>
	
	<state name="H009_ValidarAlegaciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_ValidarAlegacionesDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H009_ValidarAlegaciones" name="activarAlerta"></transition>
		<transition to="H009_RevisarResultadoInfAdmon" name="vueltaAtras"></transition>
	</state>
	
	<decision name="H009_ValidarAlegacionesDecision" expression="#{H009_ValidarAlegacionesDecision}">
		<transition to="ForkElevarSareb" name="OK"></transition>
		<transition to="H009_RegistrarResolucionFaseComun" name="KO"></transition>
	</decision>

	<state name="H009_RegistrarResolucionFaseComun">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="H009_RegistrarResolucionFaseComunDecision" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<decision name="H009_RegistrarResolucionFaseComunDecision" expression="#{H009_RegistrarResolucionFaseComunDecision}">
		<transition to="H009_BPMTramiteFaseLiquidacion" name="faseLiquidacion"></transition>
		<transition to="H009_BPMTramiteFaseConvenioV4" name="faseConvenio"></transition>
	</decision>

	<state name='H009_BPMTramiteFaseLiquidacion'>
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="haya_tramiteFaseLiquidacion"></transition>
	</state>

	<state name="H009_BPMTramiteFaseConvenioV4">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin2" name="haya_tramiteFaseConvenioV4"></transition>
	</state>

	<state name="H009_ActualizarEstadoCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="H009_ActualizarEstadoCreditos" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H009_RegistrarResolucionFaseComun" name="avanzaBPM"></transition>
	</state>

	<state name="H009_RegistrarProyectoInventario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_RegistrarProyectoInventario" name="activarAlerta"></transition>
		<transition to="H009_RegistrarProyectoInventarioDecision" name="avanzaBPM"></transition>
		<transition to="H009_PresentarEscritoInsinuacion" name="vueltaAtras"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<decision name="H009_RegistrarProyectoInventarioDecision" expression="#{H009_RegistrarProyectoInventarioDecision}">
		<transition to="H009_PresentarRectificacion" name="desfavorable"></transition>
		<transition to="H009_RegistrarInformeAdmonConcursal" name="favorable"></transition>
	</decision>

	<state name="H009_PresentarRectificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_RegistrarInformeAdmonConcursal" name="avanzaBPM"></transition>
		<transition to="H009_PresentarRectificacion" name="activarAlerta"></transition>
		<transition to="H009_RegistrarProyectoInventario" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H009_RevisarInsinuacionCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_RegistrarInsinuacionCreditos" name="vueltaAtras"></transition>
		<transition to="ForkContactarDeudor" name="avanzaBPM"></transition>
		<transition to="H009_RevisarInsinuacionCreditos" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H009_BPMTramiteDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_ActualizarEstadoCreditos" name="haya_tramiteDemandaIncidental"></transition>
	</state>

	<state name="H009_BPMhaya_tramiteElevacionPropSarebLitigiosA">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H009_ElevacionDecision" name="DENEGADA"></transition>
		<transition to="Fin" name="ACEPTADA"></transition>
		<transition to="Fin" name="ACCONCAM"></transition>
	</state>

	<fork name="forkTrasAceptacion">
		<transition to="H009_RegistrarInsinuacionCreditos" name="sigueBPM"></transition>
		<transition to="H009_BPMhaya_tramiteElevacionPropSarebLitigiosA" name="elevarSareb"></transition>
	</fork>

	<node name="H009_ElevacionDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>	
		<transition to="Fin" name="Fin"></transition>
	</node>

	<fork name="ForkElevarSareb">
		<transition to="H009_BPMTramiteDemandaIncidental" name="sigueBPM"></transition>
		<transition to="H009_BPMhaya_tramiteElevacionPropSarebLitigiosB" name="elevarSareb"></transition>
	</fork>

	<state name="H009_BPMhaya_tramiteElevacionPropSarebLitigiosB">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="ACEPTADA"></transition>
		<transition to="H009_SarebDecision" name="DENEGADA"></transition>
		<transition to="Fin" name="ACCONCAM"></transition>
	</state>

	<state name="H009_ContactarDeudor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<fork name="ForkContactarDeudor">
		<transition to="H009_ContactarDeudor" name="contactarDeudor"></transition>
		<transition to="H009_PresentarEscritoInsinuacion" name="sigueBPM"></transition>
	</fork>

	<node name="H009_SarebDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>	
		<transition to="Fin" name="Fin"></transition>
	</node>

	<fork name="fork1">
		<transition to="H009_BPMTramiteAceptacionConcurso" name="aceptacion"></transition>
		<transition to="H009_BPMPrecontencioso" name="precontencioso"></transition>
	</fork>

	<state name="H009_BPMPrecontencioso">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
	</state>
	
	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>
