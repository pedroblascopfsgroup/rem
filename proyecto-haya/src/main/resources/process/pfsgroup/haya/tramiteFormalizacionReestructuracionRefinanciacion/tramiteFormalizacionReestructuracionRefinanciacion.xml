		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ObtenerDocumentacionOriginal" name="vueltaAtras"></transition>
	</state>

	<state name="P451_ContabilizarOperacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
