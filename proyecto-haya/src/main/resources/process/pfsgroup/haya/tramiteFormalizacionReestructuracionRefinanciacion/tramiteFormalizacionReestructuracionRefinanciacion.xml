				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_RevisarPropuesta" name="vueltaAtras"></transition>
		<transition to="P451_RegistrarResolucionSareb" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_RegistrarResolucionSareb">
	<event type="node-enter">
			<action name="miTarea"
