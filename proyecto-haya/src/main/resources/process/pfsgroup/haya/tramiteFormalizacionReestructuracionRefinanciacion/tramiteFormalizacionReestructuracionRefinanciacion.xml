				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ConfeccionarBorradorContratoAcreditado" name="vueltaAtras"></transition>
		<transition to="P451_ValidarBorradorContratoAJDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_ValidarBorradorContratoAJDecision" expression="#{P451_ValidarBorradorContratoAJDecision}">
		<transition to="P451_EnvioBorradorContratoAcreditado" name="Aprobada"></transition>
		<transition to="P451_ComunicacionAcreditadoAJ" name="Rechazada"></transition>
	</decision>
	
	<state name="P451_ComunicacionAcreditadoAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ValidarBorradorContratoAJ" name="vueltaAtras"></transition>
		<transition to="P451_EnviarDesistirWFAJ" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_EnviarDesistirWFAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
