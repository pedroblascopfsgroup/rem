		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_AcordarFechaFirmaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_AcordarFechaFirmaDecision" expression="#{P451_AcordarFechaFirmaDecision}">
		<transition to="P451_RevisarPropuesta" name="Reconsiderar"></transition>
		<transition to="fork1" name="Aceptada"></transition>
		<transition to="P451_EnviarDesistirPreviaFirmaOperacion" name="Desistir"></transition>
	</decision>
	
	<fork name="fork1">
		<transition to="P451_FirmarOperacion" name="firmarOperacion"></transition>
		<transition to="P451_PrepararContrato" name="prepararContrato"></transition>
	</fork>
	
	<state name="P451_RevisarPropuesta">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_AcordarFechaFirma" name="vueltaAtras"></transition>
		<transition to="P451_ElevarSareb" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_ElevarSareb">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
