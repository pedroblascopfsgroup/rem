				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ObtenerAprobacionCNDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_ObtenerAprobacionCNDecision" expression="#{P451_ObtenerAprobacionCNDecision}">
		<transition to="Fin" name="Si"></transition>
		<transition to="P451_ComunicacionAcreditadoCN" name="No"></transition>
