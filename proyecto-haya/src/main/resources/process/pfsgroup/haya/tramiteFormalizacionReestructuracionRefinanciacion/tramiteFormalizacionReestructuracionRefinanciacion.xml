				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ElevarSareb" name="vueltaAtras"></transition>
		<transition to="P451_RegistrarResolucionSarebDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_RegistrarResolucionSarebDecision" expression="#{P451_RegistrarResolucionSarebDecision}">
		<transition to="P451_ConfeccionarBorradorContratoAcreditado" name="Aceptada"></transition>
		<transition to="P451_ComunicacionAcreditadoSareb" name="Rechazada"></transition>
	</decision>
	
	<state name="P451_ComunicacionAcreditadoSareb">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_RegistrarResolucionSareb" name="vueltaAtras"></transition>
		<transition to="P451_EnviarDesistirWFSareb" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_EnviarDesistirWFSareb">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ComunicacionAcreditadoSareb" name="vueltaAtras"></transition>
		<transition to="FinProcedimiento" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_EnviarDesistirPreviaFirmaOperacion">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="FinProcedimiento" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_PrepararContrato">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_PrepararContratoDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_PrepararContratoDecision" expression="#{P451_PrepararContratoDecision}">
		<transition to="P451_ValidacionContratoAJ" name="Si"></transition>
		<transition to="P451_EnvioContratoAcreditado" name="No"></transition>
	</decision>
	
	<state name="P451_ValidacionContratoAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
