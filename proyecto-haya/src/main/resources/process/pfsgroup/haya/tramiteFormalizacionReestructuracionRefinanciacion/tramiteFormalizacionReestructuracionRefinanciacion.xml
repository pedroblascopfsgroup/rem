	</start-state>
	
	<fork name="forkInicio">
		<transition to="P451_ObtenerAprobacionCN" name="obtenerAprobacion"></transition>
		<transition to="P451_ConfeccionarBorradorContratoAcreditado" name="confeccionarBorrador"></transition>
	</fork>	
	
	<state name="P451_ObtenerAprobacionCN">
