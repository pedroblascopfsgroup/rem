		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_ComunicacionAcreditadoCN" name="vueltaAtras"></transition>
		<transition to="FinProcedimiento" name="avanzaBPM"></transition>
	</state>
	
	<state name="P451_ConfeccionarBorradorContratoAcreditado">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
