		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P451_FirmarOperacionDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P451_FirmarOperacionDecision" expression="#{P451_FirmarOperacionDecision}">
		<transition to="P451_EnviarDesistirPreviaFirmaOperacion" name="Rechazada"></transition>
		<transition to="forkOperacion" name="Aceptada"></transition>
	</decision>
	
	<fork name="forkOperacion">
		<transition to="P451_ObtenerDocumentacionOriginal" name="obtenerDocumentacionOriginal"></transition>
		<transition to="P451_ContabilizarOperacion" name="informarCierreSareb"></transition>
	</fork>

<state name="P451_ObtenerDocumentacionOriginal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
