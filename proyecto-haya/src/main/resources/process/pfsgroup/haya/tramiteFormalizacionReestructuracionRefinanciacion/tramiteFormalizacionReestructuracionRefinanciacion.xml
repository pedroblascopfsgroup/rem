	<decision name="P451_ConfeccionarBorradorContratoAcreditadoDecision" expression="#{P451_ConfeccionarBorradorContratoAcreditadoDecision}">
		<transition to="P451_ValidarBorradorContratoAJ" name="Si"></transition>
		<transition to="P451_EnvioBorradorContratoAcreditado" name="No"></transition>
	</decision>
	
	<state name="P451_ValidarBorradorContratoAJ">
	<event type="node-enter">
			<action name="miTarea"
