				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ObtenerAprobacionCN" name="vueltaAtras"></transition>
		<transition to="P455_EnviarDesistirWFCN" name="avanzaBPM"></transition>
	</state>
	
	<state name="P455_EnviarDesistirWFCN">
	<event type="node-enter">
			<action name="miTarea"
