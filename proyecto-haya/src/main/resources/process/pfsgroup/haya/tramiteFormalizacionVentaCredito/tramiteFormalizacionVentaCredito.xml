	<decision name="P455_ConfeccionarBorradorContratoAcreditadoDecision" expression="#{P455_ConfeccionarBorradorContratoAcreditadoDecision}">
		<transition to="P455_ValidarBorradorContratoAJ" name="Si"></transition>
		<transition to="P455_EnvioBorradorContratoAcreditado" name="No"></transition>
	</decision>
	
	<state name="P455_ValidarBorradorContratoAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
