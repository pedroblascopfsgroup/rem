		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ComunicacionAcreditadoCN" name="vueltaAtras"></transition>
		<transition to="FinProcedimiento" name="avanzaBPM"></transition>
	</state>
	
	<state name="P455_ConfeccionarBorradorContratoAcreditado">
