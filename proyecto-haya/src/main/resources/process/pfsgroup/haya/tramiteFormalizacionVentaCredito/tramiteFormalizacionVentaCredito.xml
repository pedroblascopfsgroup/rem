				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ConfeccionarBorradorContratoAcreditado" name="vueltaAtras"></transition>
		<transition to="P455_ValidarBorradorContratoAJDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P455_ValidarBorradorContratoAJDecision" expression="#{P455_ValidarBorradorContratoAJDecision}">
		<transition to="P455_EnvioBorradorContratoAcreditado" name="Aprobada"></transition>
		<transition to="P455_ComunicacionAcreditadoAJ" name="Rechazada"></transition>
	</decision>
	
	<state name="P455_ComunicacionAcreditadoAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ValidarBorradorContratoAJ" name="vueltaAtras"></transition>
		<transition to="P455_EnviarDesistirWFAJ" name="avanzaBPM"></transition>
	</state>
	
	<state name="P455_EnviarDesistirWFAJ">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ComunicacionAcreditadoAJ" name="vueltaAtras"></transition>
		<transition to="FinProcedimiento" name="avanzaBPM"></transition>
	</state>
	
	<state name="P455_EnvioBorradorContratoAcreditado">
	<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="forkBorrador" name="avanzaBPM"></transition>
	</state>
	
	<fork name="forkBorrador">
		<transition to="P455_AcordarFechaFirma" name="acordarFechaFirma"></transition>
		<transition to="P455_PeticionEscriturasCaracterEjecutivo" name="peticionEscrituras"></transition>
	</fork>

	<state name="P455_PeticionEscriturasCaracterEjecutivo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
