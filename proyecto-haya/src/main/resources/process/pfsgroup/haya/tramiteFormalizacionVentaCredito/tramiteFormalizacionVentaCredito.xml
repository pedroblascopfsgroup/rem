<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2"
	name="tramiteVentaDeCredito">

	<start-state name="inicio">
		<transition to="forkInicio" name="iniciarProceso"></transition>
	</start-state>
	
	<fork name="forkInicio">
		<transition to="P455_ObtenerAprobacionCN" name="obtenerAprobacion"></transition>
		<transition to="P455_ConfeccionarBorradorContratoAcreditado" name="confeccionarBorrador"></transition>
	</fork>	
	
	<state name="P455_ObtenerAprobacionCN">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P455_ObtenerAprobacionCNDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P455_ObtenerAprobacionCNDecision" expression="#{P455_ObtenerAprobacionCNDecision}">
		<transition to="Fin" name="Si"></transition>
		<transition to="P455_ComunicacionAcreditadoCN" name="No"></transition>
	</decision>
	
	
	<state name="P455_ComunicacionAcreditadoCN">
	<event type="node-enter">
