<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2"
	name="tramitePDV">


	<start-state name="inicio">
		<transition to="P456_ObtenerAprobacionCN"></transition>
		<transition to="P456_ConfeccionarBorradorContratoAcreditado"></transition>
	</start-state>


	<state name="P456_ObtenerAprobacionCN">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_ComunicacionAcreditado" name="avanzaBPM"></transition>
		
	</state>

	<state name="P456_ConfeccionarBorradorContratoAcreditado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_ConfeccionarBorradorContratoAcreditadoDecision" name="avanzaBPM"></transition>
	</state>
		
	<decision name="P456_ConfeccionarBorradorContratoAcreditadoDecision" expression="#{P456_ConfeccionarBorradorContratoAcreditadoDecision}">
		<transition to="P456_ValidarBorradorContratoAJ" name="Si"></transition>
		<transition to="P456_EnvioBorradorContradoAcreditado" name="No"></transition>
	</decision>


	<state name="P456_ComunicacionAcreditado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_EnviarDesistirWF" name="avanzaBPM"></transition>
	</state>

	<state name="P456_EnviarDesistirWF">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<state name="P456_ValidarBorradorContratoAJ">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_ValidarBorradorContratoAJDecision" name="avanzaBPM"></transition>
	</state>
			
	<decision name="P456_ValidarBorradorContratoAJDecision" expression="#{P456_ValidarBorradorContratoAJDecision}">
		<transition to="P456_EnvioBorradorContradoAcreditado" name="Aprobado"></transition>
		<transition to="P456_ComunicacionAcreditado" name="Rechazado"></transition>
	</decision>

	<state name="P456_EnvioBorradorContradoAcreditado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="fork1" name="avanzaBPM"></transition>
	</state>
	
	<fork name="fork1">
		<transition to="P456_PrepararContrato" name="prepararContrato"></transition>
		<transition to="P456_AcordarFechaFirma" name="acordarFechaFirma"></transition>
	</fork>

	<state name="P456_PrepararContrato">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_ConfeccionarBorradorContratoAcreditadoDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P456_PrepararContratoDecision" expression="#{P456_PrepararContratoDecision}">
		<transition to="P456_ValidarBorradorContratoAJ" name="Si"></transition>
		<transition to="P456_EnvioBorradorContradoAcreditado" name="No"></transition>
	</decision>

	<state name="P456_ValidacionContratoAJ">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_ValidacionContratoAJDecision" name="avanzaBPM"></transition>
	</state>
	
	
	<decision name="P456_ValidacionContratoAJDecision" expression="#{P456_ValidacionContratoAJDecision}">
		<transition to="P456_PrepararContrato" name="Validado"></transition>
		<transition to="P456_EnvioContratoAcreditado" name="NoValidado"></transition>
	</decision>

	<state name="P456_EnvioContratoAcreditado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_FirmarOperacion" name="avanzaBPM"></transition>
	</state>

	<state name="P456_FirmarOperacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_FirmarOperacionDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P456_FirmarOperacionDecision" expression="#{P456_FirmarOperacionDecision}">
		<transition to="P456_VolcadoCondicionesResolucion" name="Aceptado"></transition>
		<transition to="P456_EnviarDesistirWF" name="Rechazado"></transition>
	</decision>

	<state name="P456_AcordarFechaFirma">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P456_AcordarFechaFirmaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="P456_AcordarFechaFirmaDecision" expression="#{P456_AcordarFechaFirmaDecision}">
		<transition to="P456_RevisarPropuesta" name="reconsiderar"></transition>
		<transition to="P456_ComunicacionAcreditadoFin" name="rechazada"></transition>
		<transition to="P456_FirmarOperacion" name="aceptada"></transition>
	</decision>

	<state name="P456_VolcadoCondicionesResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="fork2" name="avanzaBPM"></transition>
	</state>
	
	<fork name="fork2">
		<transition to="P456_ObtenerDocumentacionOriginal" name="obtenerDocumentacionOriginal"></transition>
		<transition to="P456_InformarCierreSareb" name="informarCierreSareb"></transition>
		<transition to="P456_AnalisisEvolucionPDV" name="analisisEvolucionPDV"></transition>
	</fork>

	<state name="P456_ObtenerDocumentacionOriginal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
