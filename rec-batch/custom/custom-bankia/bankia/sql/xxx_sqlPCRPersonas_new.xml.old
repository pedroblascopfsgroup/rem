		FROM TMP_PER_PERSONAS tp 
		WHERE   tp.TMP_PER_COLECTIVO_SINGULAR is not null and
		not exists( SELECT 1 
					FROM DD_COS_COLECTIVO_SINGULAR
                    WHERE DD_COS_CODIGO = trim(tp.TMP_PER_COLECTIVO_SINGULAR))
	]]>
    </entry>

