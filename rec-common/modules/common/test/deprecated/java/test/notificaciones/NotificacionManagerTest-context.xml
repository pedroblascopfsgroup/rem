<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" 
    xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd"
    default-merge="true"
    default-autowire="byName">

    <import resource="classpath:testScanComponents.xml" />
  	
  	<bean name="clienteManager" class="es.capgemini.pfs.cliente.ClienteManager"/>
  	<bean name="expedienteManager" class="es.capgemini.pfs.expediente.ExpedienteManager"/>
  	<!-- Scripts -->
   <bean name="cargarRelacionesValidas" class="org.springframework.core.io.ClassPathResource">
  	<constructor-arg value="es/capgemini/pfs/expediente/generacionExpedienteDataLoad-${entity.dialect}.sql"/>
  </bean>
   <bean name="cargarRelacionesValidas3raRelacion" class="org.springframework.core.io.ClassPathResource">
  	<constructor-arg value="es/capgemini/pfs/expediente/generacionExpedienteDataLoad3raRelacion-${entity.dialect}.sql"/>
  </bean>
  <bean name="cargarContratoExpediente" class="org.springframework.core.io.ClassPathResource">
  	<constructor-arg value="es/capgemini/pfs/expediente/expedienteContrato-${entity.dialect}.sql"/>
  </bean>  
   <bean name="cargarClienteExpedienteAsunto" class="org.springframework.core.io.ClassPathResource">
  	<constructor-arg value="es/capgemini/pfs/notificaciones/NotificacionTestBefore-${entity.dialect}.sql"/>
  </bean>
  <bean name="cargaTareasNotificacion" class="org.springframework.core.io.ClassPathResource">
  	<constructor-arg value="sql/notificaciones/cargaTareasNotificacion-${entity.dialect}.sql"/>
  </bean>    
   
    
</beans>