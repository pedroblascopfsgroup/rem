<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
 
    <input name="idContrato" type="java.lang.Long"/> 
       
    <end-state id="atipicosContrato" view="contratos/tabAtipicosContratoJSON">
        <on-entry>
            <evaluate expression="executor.execute('plugin.mejoras.contrato.getAtipicosContrato', idContrato)"  result="flowScope.atipicosContratos"/>
        </on-entry>
    </end-state>
       
   
    
</flow>