<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" required="true" type="java.lang.Long" />

	<end-state id="borrar" view="default">
		<on-entry>
			<evaluate expression="executor.execute('plugin.precontencioso.borrarProcedimientoPCO',id)"/>
			<evaluate expression="executor.execute('procedimientoManager.borrarProcedimiento', id)" />
		</on-entry>
	</end-state>

</flow>
