<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">    
    
    <input name="codigoTipoDocGen" type="java.lang.String" required="false"/>
    
    <!-- renderiza el formulario -->
    <end-state id="listadoTipoDoc" view="expedientes/listadoTipoDocJSON">
        <on-entry>
            <evaluate expression="executor.execute('ddTipoTituloManager.buscarTipos', codigoTipoDocGen)" result="flashScope.tiposDocumento"/>
        </on-entry>
    </end-state>    
    
</flow>