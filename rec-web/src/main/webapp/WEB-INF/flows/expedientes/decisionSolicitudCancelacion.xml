<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idSolicitud" type="java.lang.Long"/>
	<input name="idExpediente" type="java.lang.Long" required="false"/>	
	
		
    <end-state id="formulario" view="expedientes/decisionSolicitudCancelacion">
		<on-entry>
			<evaluate expression="executor.execute('expedienteManager.buscarSolicitudCancelacion', idSolicitud)" result="flowScope.solicitud"/>
			<evaluate expression="executor.execute('expedienteManager.getExpediente', idExpediente)" result="flowScope.expediente"/>
		</on-entry>
    </end-state> 	
    
       
</flow>
