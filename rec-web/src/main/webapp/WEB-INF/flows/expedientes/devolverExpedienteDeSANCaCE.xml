<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="id" type="java.lang.Long"/>
    <input name="respuesta" type="java.lang.String"/> 
    <input name="isSupervisor" type="java.lang.Boolean"/>  
    
    <!-- eleva el expediente a revision -->
    <end-state id="devolverExpediente" view="default">
        <on-entry>
            <evaluate expression="executor.execute('expedienteManager.devolverExpedienteDeSANCaCE', id, respuesta, isSupervisor)"/>
        </on-entry>
    </end-state>
</flow>