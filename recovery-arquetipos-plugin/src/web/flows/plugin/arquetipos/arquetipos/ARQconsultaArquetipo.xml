<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="id" type="java.lang.Long"/>

	<end-state id="listado" view="plugin/arquetipos/arquetipos/consultarArquetipo">
		<on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.arquetipos.getArquetipo',id)" result="flowScope.arquetipo"/>
		</on-entry>
	</end-state>
</flow>

