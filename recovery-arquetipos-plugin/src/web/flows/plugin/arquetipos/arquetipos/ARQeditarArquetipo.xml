<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idArquetipo" type="java.lang.Long" required="true"/>
	<end-state id="listado" view="plugin/arquetipos/arquetipos/nuevoArquetipo">
		<on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.arquetipos.getArquetipo',idArquetipo)" result="flowScope.arquetipo" />
			<evaluate expression="executor.execute('plugin.arquetipos.rules.lista')" result="flowScope.paquetes"/>
			
			<evaluate expression="executor.execute('plugin.arquetipos.ddTipoSalto.lista')" result="flowScope.tipoSalto"/> 
			 
		</on-entry>
	</end-state>
</flow>
