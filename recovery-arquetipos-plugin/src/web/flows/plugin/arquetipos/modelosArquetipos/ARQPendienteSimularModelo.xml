<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="event" type="java.lang.String" required="true"/>
	<input name="id" type="java.lang.Long"/>
	
	<decision-state id="decision">
		<if test="event=='pendienteSimulacionModelo'" then="pendienteSimulacionModelo"/>
		<if test="event=='cancelarPendienteSimulacionModelo'" then="cancelarPendienteSimulacionModelo"/>
	</decision-state>

	<end-state id="pendienteSimulacionModelo" view="default" >
		<on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.simulacion.pendienteSimulacionModelo',id)"/>
		</on-entry>
	</end-state>
	
	<end-state id="cancelarPendienteSimulacionModelo" view="default" >
		<on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.simulacion.cancelarPendienteSimulacionModelo',id)"/>
		</on-entry>
	</end-state>
	
</flow>
