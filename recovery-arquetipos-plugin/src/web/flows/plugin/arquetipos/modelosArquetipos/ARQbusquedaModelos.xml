<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/arquetipos/modelosArquetipos/busquedaModelos" >
	    <on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.arquetipos.listaArquetipos')" result="flowScope.arquetipos"/>
			<evaluate expression="executor.execute('plugin.arquetipos.estadoModelo.listaEstados')" result="flowScope.ddEstadoModelo"/>
			<evaluate expression="executor.execute('plugin.arquetipos.web.modelosArquetipos.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('plugin.arquetipos.web.modelosArquetipos.buttons.right')" result="flowScope.buttonsRight"/>
		</on-entry>
			
    </end-state> 	
    
</flow>