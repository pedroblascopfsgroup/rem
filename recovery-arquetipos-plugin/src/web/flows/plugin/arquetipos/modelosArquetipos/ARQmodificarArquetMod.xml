<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	<input name="idArquetiposModelo" type="java.lang.Long"/>
	<input name="idModelo" type="java.lang.Long"/>
    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="plugin/arquetipos/modelosArquetipos/insertarArquetipoModelo" >
		<on-entry>
			<evaluate expression="executor.execute('plugin.arquetipos.modelosArquetipos.getModeloArquetipo', idArquetiposModelo)" result="flowScope.modArquetipo"/>
			<evaluate expression="executor.execute('plugin.arquetipos.modelos.getModelo', idModelo)" result="flowScope.modelo"/>
			<evaluate expression="executor.execute('plugin.arquetipos.arquetipos.listaRestoArquetipos', idModelo)" result="flowScope.restoArquetipos"/>
			<evaluate expression="executor.execute('plugin.arquetipos.itinerario.listaItinerario')" result="flowScope.itinerarios"/>
		</on-entry>
	</end-state>
	
    
</flow>

