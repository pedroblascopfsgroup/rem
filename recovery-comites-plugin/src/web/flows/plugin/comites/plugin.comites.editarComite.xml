<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long"/>
	
	<end-state id="listado" view="plugin/comites/altaComite">
		<on-entry>
			
			<evaluate expression="executor.execute('plugin.comites.comite.getComite',id)" result="flowScope.comite"/> 
			<evaluate expression="executor.execute('plugin.comites.zona.listaZonas')" result="flowScope.zonas" />
			<evaluate expression="executor.execute('zonaManager.getNiveles')" result="flowScope.niveles" />
		</on-entry>
		
	</end-state>
</flow>



