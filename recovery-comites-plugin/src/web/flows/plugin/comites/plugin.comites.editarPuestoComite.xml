<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idPuesto" type="java.lang.Long"/>
	
	
	<end-state id="listado" view="plugin/comites/nuevoPuestoComite">
		<on-entry>
			
			<evaluate expression="executor.execute('plugin.comites.puestosComite.get',idPuesto)" result="flowScope.puesto" />
			<evaluate expression="executor.execute('plugin.comites.perfil.listaPerfiles')" result="flowScope.perfiles" />
			<evaluate expression="executor.execute('plugin.comites.zona.listaZonas')" result="flowScope.zonas" />
			<evaluate expression="executor.execute('zonaManager.getNiveles')" result="flowScope.niveles" />
		</on-entry>
		
	</end-state>
</flow>




