<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="formItinerariosComite" class="es.pfsgroup.plugin.recovery.comites.comite.web.CMTFormItinerariosComite" />
    <input name="id" type="java.lang.Long"/>
    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="plugin/comites/itinerariosComiteJSON" >
		<on-entry>
			<evaluate expression="executor.execute('plugin.comites.comite.listaDtoItinerarios',id)" result="formItinerariosComite.dtoItinerario"/>
			<set name="flowScope.formItinerariosComite" value="formItinerariosComite" />
		</on-entry>
	</end-state>
	
    
</flow>

