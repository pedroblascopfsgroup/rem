<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idDespacho" type="java.lang.Long"/>

	<end-state id="listado" view="plugin/config/despachoExterno/agregarGestorDespacho">
	<on-entry>
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getGestoresExtList',idDespacho)" result="flowScope.listaGestores" />
		</on-entry>
	</end-state>
</flow>