<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<end-state id="listado" view="plugin/config/despachoExterno/altaDespachoExterno">
	<secured attributes="ROLE_ADMIN" match="any" />
	<on-entry>
			<evaluate expression="executor.execute('usuarioManager.getUsuarioLogado').entidad.descripcion" result="flowScope.usuarioEntidad" />
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getTipoVia')"
				result="flowScope.ddTipoVia" />
			<evaluate expression="executor.execute('admTipoDespachoExternoManager.getList')"
				result="flowScope.tiposDespachos" />
			<evaluate expression="executor.execute('es.pfsgroup.recovery.ext.api.multigestor.getList')"
				result="flowScope.tiposGestor" />
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getMapasDespachoExtras')"
				result="flowScope.mapasDespExtras" />
			<evaluate expression="executor.execute('ADMDiccionariosManager.getSiNo')" 
				result="flowScope.ddSiNo" />	
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getDDProvincias')" 
				result="flowScope.listaProvincias" />
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getIdTipoLetrado')" 
				result="flowScope.idTipoLetrado" />	
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getIdTipoProcurador')" 
				result="flowScope.idTipoProcurador" />
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getDDTipoDocumento')" 
				result="flowScope.tiposDocumentos" />	
		</on-entry>
	</end-state>
</flow>