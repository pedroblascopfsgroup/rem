<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	<input name="id" type="java.lang.Long"/>
    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="plugin/config/despachoExterno/altaDespachoExterno" >
    	<secured attributes="ROLE_ADMIN" match="any" />
		<on-entry>
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getDespachoExterno',id)" result="flowScope.despacho"/>
			<evaluate expression="executor.execute('ADMDespachoExternoManager.getTipoVia')"
				result="flowScope.ddTipoVia" />
			<evaluate expression="executor.execute('admTipoDespachoExternoManager.getList')"
				result="flowScope.tiposDespachos" />
			<evaluate expression="executor.execute('es.pfsgroup.recovery.ext.api.multigestor.getList')"
				result="flowScope.tiposGestor" />
			<evaluate expression="executor.execute('ADMDespachoExternoManager.dameDespachoExtras',id)" result="flowScope.despachoExtras"/>
			<evaluate expression="executor.execute('ADMDespachoExternoManager.dameAmbitoDespachoExtras',id)" result="flowScope.ambitoDespachoExtras"/>				
		</on-entry>
	</end-state>
	
    
</flow>