<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
    <input name="idFuncion" type="java.lang.Long"/>
    <input name="id" type="java.lang.Long"/>
    <input name="password" type="java.lang.String"/>
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="default" >
    	<secured attributes="ROLE_ADMIN" match="any" />
		<on-entry>
			<evaluate expression="executor.execute('ADMPerfilManager.borrarFuncionPerfilSeguro',id,idFuncion,password)" 
				result="flowScope.pagina"/>
		</on-entry>
	</end-state>	
    
</flow>