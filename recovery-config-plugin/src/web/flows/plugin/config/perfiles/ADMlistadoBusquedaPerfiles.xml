<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/config/perfiles/listadoBusquedaPerfiles" >
	    <secured attributes="ROLE_ADMIN" match="any" />
		<on-entry>
			<evaluate expression="executor.execute('ADMFuncionManager.getList')" result="flowScope.funciones"/>
			<evaluate expression="executor.execute('plugin.config.web.perfiles.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('plugin.config.web.perfiles.buttons.right')" result="flowScope.buttonsRight"/>
	    </on-entry>
		
    </end-state> 	
    
</flow>