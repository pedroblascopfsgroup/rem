<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	<var name="dto" class="es/pfsgroup/recovery/ext/impl/acuerdo/dto/busquedaAcuerdosDTO"/>
	
	<!-- renderiza el formulario -->
    <end-state id="acuerdos" view="acuerdo/listadoAcuerdosJSON" >
	     <on-entry>
	    	<evaluate expression="binder.bindAndValidate(flowRequestContext, dto)" />
	    	<evaluate expression="executor.execute('coreextensionController.listBusquedaAcuerdos', dto)" result="flowScope.pagina"/>
	    </on-entry>
    </end-state> 	
    
</flow>
