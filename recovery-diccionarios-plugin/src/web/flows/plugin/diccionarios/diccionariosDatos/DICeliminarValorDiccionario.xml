<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idDiccionarioEditable" type="java.lang.Long" required="true"/>
	<input name="idLineaEnDiccionario" type="java.lang.Long" required="true"/>
	
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="default" >
		<on-entry>
			<evaluate expression="executor.execute('plugin.diccionarios.diccionarioEditableManager.eliminarDiccionarioDatosLinea',idDiccionarioEditable,idLineaEnDiccionario)" result="flowScope.pagina"/>
		</on-entry>
	</end-state>
	
    
</flow>