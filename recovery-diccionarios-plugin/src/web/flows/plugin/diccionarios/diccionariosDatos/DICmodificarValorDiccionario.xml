<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idDiccionarioEditable" type="java.lang.Long" required="true"/>
	<input name="idLineaEnDiccionario" type="java.lang.Long" required="true"/>
	
	<end-state id="listado" view="plugin/diccionarios/diccionariosDatos/modificarValorDiccionario">
	
	<on-entry>
		<evaluate expression="executor.execute('plugin.diccionarios.diccionarioEditableManager.getDiccionarioDatosLinea',idDiccionarioEditable,idLineaEnDiccionario)" 
				result="flowScope.valorDiccionario" />	
	</on-entry>
	</end-state>
</flow>