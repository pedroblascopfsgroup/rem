<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/instruccionesExterna/buscaInstruccionesTareas" >
	    <secured attributes="ROLE_CONF_INSTRUCC_EXT" match="any" />
		<on-entry>
			<evaluate expression="executor.execute('plugin.instruccionesExterna.plazoTareaExternaPlaza.listaTiposProcedimiento')" result="flowScope.tiposProcedimiento" />
			<!--<evaluate expression="executor.execute('plugin.instruccionesExterna.plazoTareaExternaPlaza.listaTiposTarea',idProcedimiento)" result="flowScope.tareas" />
				
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaJuzgados')" result="flowScope.juzgados" />
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaPlazas')" result="flowScope.plazas" />
				 -->
			<evaluate expression="executor.execute('plugin.instruccionesExterna.web.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('plugin.instruccionesExterna.web.buttons.right')" result="flowScope.buttonsRight"/>
	 	</on-entry>
	
    </end-state> 	
    
</flow>

