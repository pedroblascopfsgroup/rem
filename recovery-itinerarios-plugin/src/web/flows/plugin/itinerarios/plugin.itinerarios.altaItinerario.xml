<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<end-state id="listado" view="plugin/itinerarios/altaItinerario">
		<on-entry>
			<evaluate expression="executor.execute('plugin.itinerarios.ddTipoItinerario.listaTipoItinerario')" result="flowScope.ddTipoItinerario"/>
			<evaluate expression="executor.execute('plugin.itinerarios.ddAmbitoExpediente.listaAmbitoExpediente')" result="flowScope.ddAmbitoExpediente"/>
			
			<evaluate expression="executor.execute('plugin.itinerarios.ddAmbitoExpediente.listaAmbitoExpedienteSeguimiento')" result="flowScope.ddAmbitoExpedienteSeguimiento"/>
			<evaluate expression="executor.execute('plugin.itinerarios.ddAmbitoExpediente.listaAmbitoExpedienteRecuperaciones')" result="flowScope.ddAmbitoExpedienteRecuperaciones"/>
		 
		</on-entry>
		
	</end-state>
</flow>

