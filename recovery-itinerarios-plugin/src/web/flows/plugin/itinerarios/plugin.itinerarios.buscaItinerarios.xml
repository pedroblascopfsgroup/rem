<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/itinerarios/busquedaItinerarios" >
	   <secured attributes="ROLE_CONFITI" match="any" />
	    <on-entry>
			<evaluate expression="executor.execute('plugin.itinerarios.ddTipoItinerario.listaTipoItinerario')" result="flowScope.ddTipoItinerario"/>
			<evaluate expression="executor.execute('plugin.itinerarios.ddAmbitoExpediente.listaAmbitoExpediente')" result="flowScope.ddAmbitoExpediente"/>
			<evaluate expression="executor.execute('plugin.itinerarios.web.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('plugin.itinerarios.web.buttons.right')" result="flowScope.buttonsRight"/>
		</on-entry>	
	 	 	
    </end-state> 	
    
</flow>
