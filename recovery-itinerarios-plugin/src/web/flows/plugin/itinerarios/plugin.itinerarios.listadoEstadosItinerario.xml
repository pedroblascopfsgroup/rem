<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	
	<var name="formEstadosItinerarios" class="es.pfsgroup.plugin.recovery.itinerarios.estado.web.ITIFormListadoEstados"/>
	<input name="id" type="java.lang.Long" required="true" />

	<end-state id="listado" view="plugin/itinerarios/listaEstadosItinerarioJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.itinerarios.estados.listaEstadosItinerario',id)" result="formEstadosItinerarios.estados"/>
			<set name="flowScope.formEstadosItinerarios" value="formEstadosItinerarios" />
		</on-entry>
	</end-state>
</flow>

