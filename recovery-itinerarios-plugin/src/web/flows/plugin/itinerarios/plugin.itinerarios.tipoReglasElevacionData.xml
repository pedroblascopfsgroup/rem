<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="id" type="java.lang.Long" required="true" />
	<input name="idEstado" type="java.lang.Long" required="true"/>

	<end-state id="listado" view="plugin/itinerarios/tipoReglasElevacionJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.itinerarios.reglasElevacion.listaReglasElevacion',id,idEstado)" result="flowScope.reglasElevacion"/>
		</on-entry>
	</end-state>
</flow>



