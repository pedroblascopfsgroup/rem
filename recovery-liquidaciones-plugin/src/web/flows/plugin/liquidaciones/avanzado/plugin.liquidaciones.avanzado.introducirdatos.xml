<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idAsunto" required="true" type="java.lang.Long"/>

	<end-state id="formulario" view="plugin/liquidaciones/avanzado/introducirdatos">

		<on-entry>
			<evaluate expression="executor.execute('asuntosManager.obtenerActuacionesAsunto',idAsunto)"
				result="flowScope.actuaciones" />
		</on-entry>
	</end-state>

</flow>
