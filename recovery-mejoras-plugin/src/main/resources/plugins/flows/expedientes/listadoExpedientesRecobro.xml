<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/mejoras/expedientes/listadoExpedientesRecobro" >
	    <on-entry>
	    	<evaluate expression="executor.execute('estadoItinearioManager.getEstadosExpedientes')" result="flowScope.situacion" />
	    	<evaluate expression="executor.execute('usuarioManager.getZonasUsuarioLogado')" result="flowScope.zonas" />
	    	<evaluate expression="executor.execute('zonaManager.getNiveles')" result="flowScope.niveles" />
            <evaluate expression="executor.execute('ddTipoItinerarioManager.getTipoItinerarios')" result="flowScope.gestion" />
	    	<evaluate expression="executor.execute('dictionaryManager.getList', 'DDEstadoExpediente')" result="flowScope.estado"/>
	    	<evaluate expression="executor.execute('segmentoManager.getSegmentos')" result="flowScope.segmentos" />
	    	<evaluate expression="executor.execute('personaManager.getTiposPersonas')" result="flowScope.tiposPersona" />
            <evaluate expression="executor.execute('comiteManager.getComites')" result="flowScope.comites" />
            <evaluate expression="executor.execute('plugin.mejoras.web.expedientes.busqueda.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('plugin.mejoras.web.expedientes.busqueda.buttons.right')" result="flowScope.buttonsRight"/>
			<evaluate expression="executor.execute('plugin.mejoras.web.expedientes.busqueda.tabs')" result="flowScope.tabsBusqueda"/>
	    </on-entry>
    </end-state> 	
    
</flow>