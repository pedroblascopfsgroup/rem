<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long" />
    <input name="fechaPago" type="java.lang.String" />
    <input name="cumplido" type="java.lang.String" />
    <input name="observaciones" type="java.lang.String" />
    
    <end-state id="finalizarAcuerdo" view="default">
    	<on-entry>
			<evaluate expression="executor.execute('mejacuerdoManager.finalizarAcuerdo',id, fechaPago, cumplido, observaciones)"/>		
	   	</on-entry>
    </end-state>

</flow>
