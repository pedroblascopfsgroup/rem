<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="id" type="java.lang.Long" required="true"/>
	
	<end-state id="form" view="plugin/mejoras/analisisAsunto/modificarAnalisis">
		<on-entry>
			<evaluate expression="executor.execute('plugin.mejoras.analisisAsunto.getAnalisis',id)" result="flowScope.analisis"/>
			<!-- <evaluate expression="executor.execute('plugin.analisisAsunto.getTiposPeriodicidadAcuerdo')" result="flowScope.ddPeriodicidadAcuerdo"/>
			<evaluate expression="executor.execute('plugin.analisisAsunt.getMotivosTipificado')" result="flowScope.ddMotivosTipificado"/>-->
			<evaluate expression="id" result="flowScope.idAsunto"/>
		</on-entry>
	</end-state>
</flow>
