<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long"/>

    <end-state id="buscaCliente" view="plugin/mejoras/generico/consultaMEJAdjuntosConTipoMapeadosJSON">
    	<on-entry>
   			<evaluate expression="executor.execute('plugin.coreextension.persona.getAdjuntosPersonaMapeado', id)" result="flowScope.dtos" />
   		</on-entry>
    </end-state>

</flow>

