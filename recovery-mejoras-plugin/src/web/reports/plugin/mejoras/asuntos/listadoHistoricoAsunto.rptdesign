            <method name="open"><![CDATA[listIt = listado.iterator();]]></method>
            <method name="fetch"><![CDATA[if (!listIt.hasNext()) return false;
var tarea = listIt.next();

row["NumAutos"] = "";
row["Procedimiento"] = "";
row["Principal"] = "";
row["Tarea"] = "";
row["FechaIni"] = "";
row["FechaFin"] = "";
row["FechaVto"] = "";
row["Usuario"] = "";

row["NumAutos"] = tarea.getProcedimiento().getCodigoProcedimientoEnJuzgado();
row["Procedimiento"] = tarea.getProcedimiento().getTipoProcedimiento().getDescripcion();
row["Principal"] = tarea.getProcedimiento().getSaldoRecuperacion();
row["Tarea"] = tarea.getTarea().getNombreTarea();
row["FechaIni"] = tarea.getTarea().getFechaIni();
row["FechaFin"] = tarea.getTarea().getFechaIni();
row["FechaVto"] = tarea.getTarea().getFechaVencimiento();
row["Usuario"] = tarea.getTarea().getNombreUsuario();




return true;]]></method>
