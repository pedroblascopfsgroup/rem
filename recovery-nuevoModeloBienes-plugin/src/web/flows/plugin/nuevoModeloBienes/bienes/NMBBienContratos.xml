<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="idBien" type="java.lang.Long"/>
	
	<!-- renderiza el formulario -->
    <end-state id="buscaContratosBien" view="plugin/nuevoModeloBienes/bienes/NMBcontratosBienJSON">
    	<on-entry>
    		<evaluate expression="executor.execute('plugin.nuevoModeloBienes.bienes.NMBbienManager.getContratos', idBien)" result="flowScope.contratosBienes"/>
    		<evaluate expression="executor.execute('usuarioManager.getUsuarioLogado')" result="flowScope.usuario" />
    	</on-entry>
    </end-state>
        
</flow>
