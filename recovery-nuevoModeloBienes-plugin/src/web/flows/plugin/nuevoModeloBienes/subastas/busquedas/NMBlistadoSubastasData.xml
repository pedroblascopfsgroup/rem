<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="dto" class="es.pfsgroup.plugin.recovery.coreextension.subasta.dto.NMBDtoBuscarSubastas"/>
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/nuevoModeloBienes/subastas/NMBSubastasJSON">	
		<on-entry>
			<evaluate expression="binder.bindAndValidate(flowRequestContext, dto)" />
			<evaluate expression="executor.execute('plugin.nuevoModeloBienes.subastas.manager.SubastaManager.buscarSubastas', dto)" result="flowScope.pagina" />
			 <evaluate expression="executor.execute('usuarioManager.getUsuarioLogado')" result="flowScope.usuarioEntidad"/> 
	    </on-entry>
    </end-state> 
</flow>
