<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	
    <!--<var name="dto" class="es.capgemini.pfs.cliente.dto.DtoBuscarClientes"/> -->
	<var name="dtoAltaJuzgado" class="es.pfsgroup.plugin.recovery.plazasJuzgados.juzgado.dto.JUZDtoAltaJuzgado"/>
	
    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="default" >
		<on-entry>
			<evaluate expression="binder.bindAndValidate(flowRequestContext,dtoAltaJuzgado)" />
			<evaluate expression="executor.execute('plugin.plazasJuzgados.juzgado.saveJuzgado',dtoAltaJuzgado)"/>
		</on-entry>
	</end-state>
	
    
</flow>



