<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
 
	<input name="codigo" required="true" type="java.lang.String"/>

	<end-state id="paginaParaCodigoPlaza" view="plugin/plazasJuzgados/listadoPaginaPlazaJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.procedimientos.bo.findPaginaPlazaByCod', codigo)" 
				result="flowScope.paginaParaPlaza" />
		</on-entry>
	</end-state>
</flow>

