<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <var name="dtoBusquedaPlazos" class="es.pfsgroup.plugin.recovery.plazosExterna.plazoTareaExternaPlaza.dto.PTEDtoBusquedaPlazo"/>

    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="plugin/plazosExterna/listadoPlazosExternaJSON" >
		<on-entry>
			<evaluate expression="binder.bindAndValidate(flowRequestContext,dtoBusquedaPlazos)" />
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.buscaPlazos',dtoBusquedaPlazos)" result="flowScope.plazos"/>
		</on-entry>
	</end-state>
	
    
</flow>

