<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="codigo" type="java.lang.String"/>
	
	<!-- renderiza el formulario -->
    <end-state id="listado" view="plugin/plazosExterna/listadoJuzgadosPlazaJSON" >
	    <on-entry>
	    	<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.buscaJuzgadosPlaza',codigo)" result="flowScope.juzgados" />
	    </on-entry>
    </end-state> 	
    
</flow>


