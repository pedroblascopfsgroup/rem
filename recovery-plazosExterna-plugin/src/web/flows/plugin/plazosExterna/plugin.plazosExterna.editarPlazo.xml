<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long" required="true"/>
	<end-state id="listado" view="plugin/plazosExterna/editarPlazo">
		<on-entry>
			
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.getPlazo',id)" result="flowScope.plazo" />
			 
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaTiposProcedimiento')" result="flowScope.tiposProcedimiento" />
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaTiposTarea')" result="flowScope.tareas" />
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaJuzgados')" result="flowScope.tiposJuzgado" />
			<evaluate expression="executor.execute('plugin.plazosExterna.plazoTareaExternaPlaza.listaPlazas')" result="flowScope.tiposPlaza" />
		</on-entry>
	</end-state>
</flow>

