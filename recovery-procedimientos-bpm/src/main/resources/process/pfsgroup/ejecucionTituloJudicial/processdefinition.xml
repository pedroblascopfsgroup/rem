<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="ejecucionTituloJudicial"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P16_InterposicionDemanda" name="iniciarProceso"></transition>
	</start-state>

	<state name="P16_InterposicionDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_InterposicionDemanda" name="activarAlerta"></transition>
		<transition to="P16_AutoDespachando_new1" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P16_AutoDespachando_new1">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_AutoDespachando_new1" name="activarAlerta"></transition>
		<transition to="P16_AutoDespachando_new1Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_InterposicionDemanda" name="vueltaAtras"></transition>
	</state>

	<decision name="P16_AutoDespachando_new1Decision" expression="#{P16_AutoDespachando_new1Decision}">
		<transition to="ForkAutoDespachando" name="AdmitidoYBienesRegistrables"></transition>
		<transition to="Decision1" name="NoAdmitido"></transition>
		<transition to="P16_ConfirmarNotificacion" name="Admitido"></transition>
	</decision>

	<node name="Decision1">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<fork name="ForkAutoDespachando">
		<transition to="P16_ConfirmarNotificacion" name="confirmaNotificacion"></transition>
		<transition to="P16_BPMVigilanciaCaducidadAnotacion" name="registraAnotacion"></transition>
	</fork>

	<state name="P16_ConfirmarNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_ConfirmarNotificacion" name="activarAlerta"></transition>
		<transition to="P16_ConfirmarNotificacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P16_ConfirmarNotificacionDecision" expression="#{P16_ConfirmarNotificacionDecision}">
		<transition to="P16_RegistrarOposicion" name="SI"></transition>
		<transition to="P16_BPMTramiteNotificacion" name="NO"></transition>
	</decision>

	<state name="P16_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_RegistrarOposicion" name="tramiteNotificacionV4"></transition>
	</state>


	<state name="P16_RegistrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_RegistrarOposicion" name="activarAlerta"></transition>
		<transition to="P16_RegistrarOposicionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_ConfirmarNotificacion" name="vueltaAtras"></transition>
	</state>

	<decision name="P16_RegistrarOposicionDecision" expression="#{P16_RegistrarOposicionDecision}">
		<transition to="P16_ConfirmarPresentacionImpugnacion" name="SI"></transition>
		<transition to="Decision2" name="NO"></transition>
	</decision>

	<node name="Decision2">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P16_HayVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_HayVista" name="activarAlerta"></transition>
		<transition to="P16_HayVistaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_ConfirmarPresentacionImpugnacion" name="vueltaAtras"></transition>
	</state>

	<decision name="P16_HayVistaDecision" expression="#{P16_HayVistaDecision}">
		<transition to="P16_RegistrarVista" name="SI"></transition>
		<transition to="P16_RegistrarResolucion" name="NO"></transition>
	</decision>

	<state name="P16_RegistrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_RegistrarVista" name="activarAlerta"></transition>
		<transition to="P16_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_HayVista" name="vueltaAtras"></transition>
	</state>

	<state name="P16_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="P16_ResolucionFirme" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_HayVista" name="vueltaAtras"></transition>
	</state>

	<state name="P16_ResolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P16_ResolucionFirme" name="activarAlerta"></transition>
		<transition to="Decision3" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_RegistrarResolucion" name="vueltaAtras"></transition>
	</state>

	<node name="Decision3">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P16_ConfirmarPresentacionImpugnacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P16_ConfirmarPresentacionImpugnacion" name="activarAlerta"></transition>
		<transition to="P16_HayVista" name="avanzaBPM"></transition>
		<transition to="P16_RegistrarOposicion" name="vueltaAtras"></transition>
	</state>

	<state name="P16_BPMVigilanciaCaducidadAnotacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="vigilanciaCaducidadAnotacion"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>