	<decision name="P90_registrarDecisionProcedimientoDecision" expression="#{P90_registrarDecisionProcedimientoDecision}">
		<transition to="P90_BPMFaseComunAbreviado" name="P56"></transition>
		<transition to="P90_BPMMonitorio" name="monitorio"></transition>
		<transition to="P90_BPMOrdinario" name="ordinario"></transition>
		<transition to="P90_BPMCambiario" name="cambiario"></transition>
		<transition to="P90_BPMFaseComunOrdinario" name="P24"></transition>
		<transition to="P90_BPMETJ" name="etj"></transition>
		<transition to="P90_BPMVerbal" name="verbal"></transition>
		<transition to="P90_BPMETNJ" name="etnj"></transition>
		<transition to="P90_BPMHipotecario" name="hipotecario"></transition>
		<transition to="P90_BPMAbreviado" name="abreviado"></transition>
		<transition to="P90_BPMSolicitudConcurso" name="P55"></transition>
	</decision>

	<state name="P90_BPMSolicitudConcurso">
		<event type="node-enter">
			<action name="miTarea"
