	<start-state name="Inicio">
		<transition to="P90_registrarDecisionProcedimiento" name="iniciarProceso"></transition>
	</start-state>

	<state name="P90_registrarDecisionProcedimiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P90_registrarDecisionProcedimiento" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P90_registrarDecisionProcedimientoDecision" name="avanzaBPM"></transition>
	</state>
	
	<state name="P90_BPMHipotecario">
