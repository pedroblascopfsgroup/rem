<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteAdjudicacion">

	<start-state name="Inicio">
		<transition to="P05_SolicitudTramiteAdjudicacion" name="iniciarProceso"></transition>
	</start-state>
   
	<state name="P05_SolicitudTramiteAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P05_SolicitudTramiteAdjudicacion" name="activarAlerta"></transition>
		<transition to="P05_NotifcacionDecreAdjudicacion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P05_RegistrarAutoAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P05_RegistrarAutoAdjudicacion" name="activarAlerta"></transition>
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P05_NotificacionDecreContratio" name="vueltaAtras"></transition>
	</state>

   	<node name="Decision">
    	<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin" name="Fin"></transition>
   	</node>

	<state name="P05_NotifcacionDecreAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P05_SolicitudTramiteAdjudicacion" name="vueltaAtras"></transition>
		<transition to="P05_NotifcacionDecreAdjudicacion" name="activarAlerta"></transition>
		<transition to="P05_NotificacionDecreContratio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P05_NotificacionDecreContratio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P05_RegistrarAutoAdjudicacion" name="avanzaBPM"></transition>
		<transition to="P05_NotifcacionDecreAdjudicacion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P05_NotificacionDecreContratio" name="activarAlerta"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	
	</end-state>

</process-definition>