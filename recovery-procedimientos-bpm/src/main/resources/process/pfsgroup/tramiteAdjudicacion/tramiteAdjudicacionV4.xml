<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteAdjudicacionV4">

	<start-state name="Inicio">
		<transition to="P413_SolicitudDecretoAdjudicacion" name="iniciarProceso"></transition>
	</start-state>
   
	<state name="P413_SolicitudDecretoAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P413_SolicitudDecretoAdjudicacion" name="activarAlerta"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAEntidad" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P413_SolicitudTestimonioDecretoAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P413_SolicitudTestimonioDecretoAdjudicacion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAlContrario" name="vueltaAtras"></transition>
		<transition to="P413_ConfirmarTestimonio" name="avanzaBPM"></transition>
	</state>

   	<state name="P413_notificacionDecretoAdjudicacionAEntidad">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P413_SolicitudDecretoAdjudicacion" name="vueltaAtras"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAEntidad" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAEntidadDecision" name="avanzaBPM"></transition>
	</state>

	<state name="P413_notificacionDecretoAdjudicacionAlContrario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P413_notificacionDecretoAdjudicacionAEntidad" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAlContrario" name="activarAlerta"></transition>
		<transition to="P413_notificacionDecretoAdjudicacionAlContrarioDecision" name="avanzaBPM"></transition>
	</state>

	<state name="P413_ConfirmarTestimonio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_ConfirmarTestimonioDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_SolicitudTestimonioDecretoAdjudicacion" name="vueltaAtras"></transition>
		<transition to="P413_ConfirmarTestimonio" name="activarAlerta"></transition>
	</state>

	<state name="P413_RegistrarEntregaTitulo">	
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P413_RegistrarPresentacionEnHacienda" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_RegistrarEntregaTitulo" name="activarAlerta"></transition>
	</state>

	<state name="P413_RegistrarPresentacionEnHacienda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_RegistrarPresentacionEnRegistro" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_RegistrarPresentacionEnHacienda" name="activarAlerta"></transition>
		<transition to="P413_RegistrarEntregaTitulo" name="vueltaAtras"></transition>
	</state>

	<state name="P413_RegistrarPresentacionEnRegistro">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_RegistrarInscripcionDelTitulo" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_RegistrarPresentacionEnRegistro" name="activarAlerta"></transition>
		<transition to="P413_RegistrarPresentacionEnHacienda" name="vueltaAtras"></transition>
	</state>

	<state name="P413_RegistrarInscripcionDelTitulo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_RegistrarInscripcionDelTitulo" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_RegistrarPresentacionEnRegistro" name="vueltaAtras"></transition>
		<transition to="P413_RegistrarInscripcionDelTituloDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P413_notificacionDecretoAdjudicacionAEntidadDecision" expression="#{P413_notificacionDecretoAdjudicacionAEntidadDecision}">
		<transition to="P413_notificacionDecretoAdjudicacionAlContrario" name="NO"></transition>
		<transition to="P413_BPMTramiteSubsanacionEmbargo1" name="SI"></transition>
	</decision>

	<state name="P413_BPMTramiteSubsanacionEmbargo1">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_notificacionDecretoAdjudicacionAEntidad" name="tramiteSubsanacionEmbargo"></transition>
		<!-- <transition to="Fin2" name="Fin"></transition>-->
	</state>

	<state name="P413_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_SolicitudTestimonioDecretoAdjudicacion" name="tramiteNotificacionV4"></transition>
		<!-- <transition to="Fin2" name="Fin"></transition>-->
	</state>

	<decision name="P413_notificacionDecretoAdjudicacionAlContrarioDecision" expression="#{P413_notificacionDecretoAdjudicacionAlContrarioDecision}">
		<transition to="P413_BPMTramiteNotificacion" name="NO"></transition>
		<transition to="P413_SolicitudTestimonioDecretoAdjudicacion" name="SI"></transition>
	</decision>

	<decision name="P413_ConfirmarTestimonioDecision" expression="#{P413_ConfirmarTestimonioDecision}">
		<transition to="P413_BPMTramiteSubsanacionEmbargo2" name="SI"></transition>
		<transition to="fork1" name="NO"></transition>
	</decision>

	<fork name="fork1">
		<transition to="P413_RegistrarEntregaTitulo" name="registrarEntregaTitulo"></transition>
		<transition to="P413_BPMTramiteDePosesion" name="tramiteDePosesion"></transition>
		<transition to="P413_GenerarPropuestaNUSE" name="casoSAREB"></transition>
	</fork>

	<state name="P413_BPMTramiteDePosesion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin2" name="tramiteDePosesionV4"></transition>
	</state>

	<decision name="P413_RegistrarInscripcionDelTituloDecision" expression="#{P413_RegistrarInscripcionDelTituloDecision}">
		<transition to="Fin" name="Fin"></transition>
		<transition to="P413_BPMTramiteSaneamientoCargas" name="NO"></transition>
		<transition to="P413_BPMTramiteSubsanacionEmbargo3" name="SI"></transition>
	</decision>

	<state name="P413_BPMTramiteSaneamientoCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin" name="tramiteSaneamientoCargas"></transition>
	</state>

	<state name="P413_BPMTramiteSubsanacionEmbargo2">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_ConfirmarTestimonio" name="tramiteSubsanacionEmbargo"></transition>
		<!-- <transition to="Fin2" name="Fin"></transition> -->
	</state>

	<state name="P413_BPMTramiteSubsanacionEmbargo3">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P413_RegistrarPresentacionEnRegistro" name="tramiteSubsanacionEmbargo"></transition>
		<!-- <transition to="Fin2" name="Fin"></transition>-->
	</state>

	<node name="P413_GenerarPropuestaNUSE">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>generarPropuestaNUSEHandlerAdj</targetBean>
		</action>
		<transition to="P413_ContabilizarCierreDeuda" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P413_ContabilizarCierreDeuda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>adjudicacionLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="P413_ContabilizarCierreDeuda" name="activarAlerta"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>	
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>	
	</end-state>

</process-definition>
