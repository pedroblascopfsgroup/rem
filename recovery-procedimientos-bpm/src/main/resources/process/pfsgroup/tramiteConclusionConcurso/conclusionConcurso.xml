<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteConclusionConcurso"
	xmlns="urn:jbpm.org:jpdl-3.2">


	<start-state name="Inicio">
		<transition to="P62_registrarCausaConclusion"></transition>
	</start-state>


	<state name="P62_registrarCausaConclusion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarInformeAdmConcursal" name="avanzaBPM"></transition>
		<transition to="P62_registrarCausaConclusion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P62_registrarInformeAdmConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarCausaConclusion" name="vueltaAtras"></transition>
		<transition to="P62_registrarInformeAdmConcursal" name="activarAlerta"></transition>
		<transition to="P62_registrarOposicion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P62_registrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarOposicion" name="activarAlerta"></transition>
		<transition to="P62_registrarInformeAdmConcursal" name="vueltaAtras"></transition>
		<transition to="P62_registrarOposicionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P62_registrarOposicionDecision" expression="#{P62_registrarOposicionDecision}">
		<transition to="P62_BPMTramiteDemandaIncidental" name="entidad"></transition>
		<transition to="P62_registrarConclusionConcurso" name="sinOposicion"></transition>
		<transition to="P62_registrarResolucion" name="otros"></transition>
	</decision>

	<state name="P62_registrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarResolucion" name="activarAlerta"></transition>
		<transition to="P62_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="P62_registrarConclusionConcurso" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P62_BPMTramiteDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarConclusionConcurso" name="tramiteDemandaIncidental"></transition>
	</state>

	<state name="P62_registrarConclusionConcurso">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P62_registrarConclusionConcurso"></transition>
		<transition to="P62_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="P62_registrarConclusionConcursoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P62_registrarConclusionConcursoDecision" expression="#{P62_registrarConclusionConcursoDecision}">
		<transition to="Fin" name="concluye"></transition>
		<transition to="Decision" name="noConcluye"></transition>
	</decision>

	<node name="Decision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin"></transition>
	</node>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>