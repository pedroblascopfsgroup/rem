<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteDePosesion">

	<start-state name="Inicio">
		<transition to="P54_regPorcentajePropiedadAdjudicada"></transition>
	</start-state>

	<state name="P54_regPorcentajePropiedadAdjudicada">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_regSolicitudPosesion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_regPorcentajePropiedadAdjudicada" name="activarAlerta"></transition>
	</state>

	<state name="P54_regSolicitudPosesion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_regSolicitudPosesionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_regPorcentajePropiedadAdjudicada" name="vueltaAtras"></transition>
		<transition to="P54_regSolicitudPosesion" name="activarAlerta"></transition>
	</state>

	<state name="P54_solicitarLanzamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_confirmarAdmisionLanzamiento" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_solicitarLanzamiento" name="activarAlerta"></transition>
		<transition to="P54_regSolicitudPosesion" name="vueltaAtras"></transition>
	</state>

	<state name="P54_confirmarAdmisionLanzamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_notificarAOcupantes" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_solicitarLanzamiento" name="vueltaAtras"></transition>
		<transition to="P54_confirmarAdmisionLanzamiento" name="activarAlerta"></transition>
	</state>

	<state name="P54_notificarAOcupantes">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_notificarAOcupantesDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_confirmarAdmisionLanzamiento" name="vueltaAtras"></transition>
		<transition to="P54_notificarAOcupantes" name="activarAlerta"></transition>
	</state>

	<state name="P54_registrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_registrarResolucion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarVista" name="activarAlerta"></transition>
		<transition to="P54_notificarAOcupantes" name="vueltaAtras"></transition>
	</state>

	<state name="P54_registrarLanzamientoEfectivo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_registrarCambioCerradura" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarLanzamientoEfectivo" name="activarAlerta"></transition>
	</state>

	<state name="P54_registrarPosesion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_registrarLanzamientoEfectivo" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarPosesion" name="activarAlerta"></transition>
		<transition to="P54_regSolicitudPosesion" name="vueltaAtras"></transition>
	</state>

	<state name="P54_registrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_registrarLanzamientoEfectivo" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarVista" name="vueltaAtras"></transition>
		<transition to="P54_registrarResolucion" name="activarAlerta"></transition>
	</state>

	<state name="P54_registrarCambioCerradura">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P54_registrarPersonaDepLlaves" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarLanzamientoEfectivo" name="vueltaAtras"></transition>
		<transition to="P54_registrarCambioCerradura" name="activarAlerta"></transition>
	</state>

	<state name="P54_registrarPersonaDepLlaves">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P54_registrarCambioCerradura" name="vueltaAtras"></transition>
		<transition to="P54_registrarPersonaDepLlaves" name="activarAlerta"></transition>
	</state>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin"></transition>
	</node>

	<decision name="P54_regSolicitudPosesionDecision" expression="#{P54_regSolicitudPosesionDecision}">
		<transition to="P54_solicitarLanzamiento" name="Ocupada"></transition>
		<transition to="P54_registrarPosesion" name="NoOcupada"></transition>
	</decision>

	<decision name="P54_notificarAOcupantesDecision" expression="#{P54_notificarAOcupantesDecision}">
		<transition to="P54_BPMTramiteNotificacion" name="NO"></transition>
		<transition to="P54_registrarVista" name="SI"></transition>
	</decision>

	<state name="P54_BPMTramiteNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P54_registrarVista" name="tramiteNotificacionV4"></transition>
	</state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	
	</end-state>

</process-definition>