<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteDemandaIncidental">

	<start-state name="Inicio">
		<transition to="P25_interposicionDemanda" name="iniciarProceso"></transition>
	</start-state>


	<state name="P25_interposicionDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P25_confirmarAdmisionDemanda" name="avanzaBPM"></transition>
		<transition to="P25_interposicionDemanda" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P25_confirmarAdmisionDemanda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P25_confirmarAdmisionDemandaDecision" name="avanzaBPM"></transition>
		<transition to="P25_confirmarAdmisionDemanda" name="activarAlerta"></transition>
		<transition to="P25_interposicionDemanda" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P25_confirmarOposicionYVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P25_confirmarOposicionYVistaDecision" name="avanzaBPM"></transition>
		<transition to="P25_confirmarOposicionYVista" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P25_confirmarAdmisionDemanda" name="vueltaAtras"></transition>
	</state>

	<state name="P25_registrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P25_RegistrarResolucion" name="avanzaBPM"></transition>
		<transition to="P25_registrarVista" name="activarAlerta"></transition>
		<transition to="P25_confirmarOposicionYVista" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P25_RegistrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P25_resolucionFirme" name="avanzaBPM"></transition>
		<transition to="P25_RegistrarResolucion" name="activarAlerta"></transition>
		<transition to="P25_confirmarOposicionYVista" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P25_resolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="P25_resolucionFirme" name="activarAlerta"></transition>
		<transition to="P25_RegistrarResolucion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P25_confirmarAdmisionDemandaDecision" expression="#{P25_confirmarAdmisionDemandaDecision}">
		<transition to="P25_confirmarOposicionYVista" name="SI"></transition>
		<transition to="Decision1" name="NO"></transition>
	</decision>

	<decision name="P25_confirmarOposicionYVistaDecision" expression="#{P25_confirmarOposicionYVistaDecision}">
		<transition to="P25_RegistrarResolucion" name="NO"></transition>
		<transition to="P25_registrarVista" name="SI"></transition>
	</decision>

	<node name="Decision1">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin"></transition>
	</node>

	<node name="Decision2">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
	</node>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>