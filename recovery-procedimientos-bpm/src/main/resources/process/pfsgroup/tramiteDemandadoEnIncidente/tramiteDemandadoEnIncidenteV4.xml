<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteDemandadoEnIncidenteV4">

	<start-state name="Inicio">
		<transition to="P96_notificacionDemandaIncidental"></transition>
	</start-state>

	<state name="P96_notificacionDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P96_dictarInstrucciones" name="avanzaBPM"></transition>
		<transition to="P96_notificacionDemandaIncidental" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_dictarInstrucciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P96_dictarInstruccionesDecision" name="avanzaBPM"></transition>
		<transition to="P96_dictarInstrucciones" name="activarAlerta"></transition>
		<transition to="P96_notificacionDemandaIncidental" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_registrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P96_admisionEscritoOposicion" name="avanzaBPM"></transition>
		<transition to="P96_registrarOposicion" name="activarAlerta"></transition>
		<transition to="P96_dictarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_admisionEscritoOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P96_admisionEscritoOposicionDecision" name="avanzaBPM"></transition>
		<transition to="P96_admisionEscritoOposicion" name="activarAlerta"></transition>
		<transition to="P96_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P96_admisionEscritoOposicionDecision" expression="#{P96_admisionEscritoOposicionDecision}">
		<transition to="P96_registrarVista" name="SI"></transition>
		<transition to="P96_registrarResolucionOposicion" name="NO"></transition>
	</decision>

	<state name="P96_registrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P96_registrarResolucionOposicion" name="avanzaBPM"></transition>
		<transition to="P96_registrarVista" name="activarAlerta"></transition>
		<transition to="P96_admisionEscritoOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_registrarResolucionOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P96_resolucionFirme" name="avanzaBPM"></transition>
		<transition to="P96_registrarResolucionOposicion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_registrarAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P96_admisionEscritoAllanamiento" name="avanzaBPM"></transition>
		<transition to="P96_registrarAllanamiento" name="activarAlerta"></transition>
		<transition to="P96_dictarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_admisionEscritoAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P96_registrarResolucionAllanamiento" name="avanzaBPM"></transition>
		<transition to="P96_admisionEscritoAllanamiento" name="activarAlerta"></transition>
		<transition to="P96_registrarAllanamiento" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_registrarResolucionAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P96_resolucionFirme" name="avanzaBPM"></transition>
		<transition to="P96_registrarResolucionAllanamiento" name="activarAlerta"></transition>
		<transition to="P96_admisionEscritoAllanamiento" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P96_resolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P96_resolucionFirmeDecision" name="avanzaBPM"></transition>
		<transition to="P96_resolucionFirme" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>
	
	<decision name="P96_resolucionFirmeDecision"  expression="#{P96_resolucionFirmeDecision}">
		<transition to="Decision" name="FAVORABLE"></transition>
		<transition to="Decision" name="NO RECURRIDO"></transition>
		<transition to="P96_registrarImporteCostas" name="NO FAVORABLE"></transition>
		<transition to="Fin" name="Fin"></transition>
	</decision>
	
	<state name="P96_registrarImporteCostas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="P96_registrarImporteCostas" name="activarAlerta"></transition>
		<transition to="P96_resolucionFirme" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P96_dictarInstruccionesDecision" expression="#{P96_dictarInstruccionesDecision}">
		<transition to="P96_registrarAllanamiento" name="ALLANAMIENTO TOTAL"></transition>
		<transition to="P96_registrarOposicion" name="ALLANAMIENTO PARCIAL"></transition>
		<transition to="P96_registrarOposicion" name="SI"></transition>
	</decision>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>	
		<transition to="Fin"></transition>
	</node>

	


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>