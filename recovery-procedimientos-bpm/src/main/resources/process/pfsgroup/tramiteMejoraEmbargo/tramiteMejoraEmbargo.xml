<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteMejoraEmbargo">

	<start-state name="Inicio">
		<transition to="P14_SolicitudMejoraEmbargo" name="iniciarProceso"></transition>
	</start-state>


	<state name="P14_SolicitudMejoraEmbargo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P14_SolicitudMejoraEmbargo" name="activarAlerta"></transition>
		<transition to="P14_RegistroDecretoEmbargo_new1" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P14_RegistroDecretoEmbargo_new1">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P14_RegistroDecretoEmbargo_new1" name="activarAlerta"></transition>
		<transition to="P14_RegistroDecretoEmbargo_new1Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P14_SolicitudMejoraEmbargo" name="vueltaAtras"></transition>
	</state>

	<state name="P14_BPMVigilanciaCaducidadAnotacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Decision" name="vigilanciaCaducidadAnotacion"></transition>
	</state>

	<node name="Decision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<decision name="P14_RegistroDecretoEmbargo_new1Decision" expression="#{P14_RegistroDecretoEmbargo_new1Decision}">
		<transition to="P14_BPMVigilanciaCaducidadAnotacion" name="si"></transition>
		<transition to="Decision" name="no"></transition>
	</decision>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>