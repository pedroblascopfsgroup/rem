<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2"
	name="tramiteNotificacion">

	<start-state name="inicio">
		<transition to="P06_NotificacionAveriguacionDomicilio"></transition>
	</start-state>


	<state name="P06_NotificacionAveriguacionDomicilio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P06_NotificacionAveriguacionDomicilio"
			name="activarAlerta"></transition>
		<transition to="P06_ResultadoDomicilio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P06_ResultadoDomicilio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P06_ResultadoDomicilio" name="activarAlerta"></transition>
		<transition to="P06_ResultadoDomicilioDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P06_NotificacionAveriguacionDomicilio" name="vueltaAtras"></transition>
	</state>

	<state name="P06_NotificacionEdicto">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P06_NotificacionEdicto" name="activarAlerta"></transition>
		<transition to="P06_ResultadoEdicto" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P06_ResultadoEdicto">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P06_ResultadoEdicto" name="activarAlerta"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P06_NotificacionEdicto" name="vueltaAtras"></transition>
	</state>

	<decision name="P06_ResultadoDomicilioDecision" expression="#{P06_ResultadoDomicilioDecision}">
		<transition to="Fin" name="NotificacionPositiva"></transition>
		<transition to="P06_NotificacionAveriguacionDomicilio" name="AveriguacionPositiva"></transition>
		<transition to="P06_NotificacionEdicto" name="NotificacionAveriguacionNegativa"></transition>
	</decision>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>