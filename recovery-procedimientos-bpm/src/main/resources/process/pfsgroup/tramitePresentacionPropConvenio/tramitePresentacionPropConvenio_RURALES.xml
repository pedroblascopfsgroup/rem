<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramitePresentacionPropConvenio_RURALES">

	<start-state name="Inicio">
		<transition to="P35_registrarConvenioPropio" name="inciarProceso"></transition>
	</start-state>

	<state name="P35_registrarConvenioPropio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_presentarPropuesta" name="avanzaBPM"></transition>
		<transition to="P35_registrarConvenioPropio" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P35_presentarPropuesta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_RegistrarAdmision" name="avanzaBPM"></transition>
		<transition to="P35_registrarConvenioPropio" name="vueltaAtras"></transition>
		<transition to="P35_presentarPropuesta" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>


	<state name="P35_RegistrarAdmision">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_presentarPropuesta" name="vueltaAtras"></transition>
		<transition to="P35_RegistrarAdmision" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>