<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteSubasta" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P11_SolicitudSubasta" name="iniciaProceso"></transition>
	</start-state>

	<state name="P11_SolicitudSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_SolicitudSubasta" name="activarAlerta"></transition>
		<transition to="P11_AnuncioSubasta_new2" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<fork name="ForkSolucitudSubasta">
		<transition to="P11_CelebracionSubasta" name="anunciaSubasta"></transition>
		<transition to="P11_solicitarTasacionInterna" name="dictaInstrucciones"></transition>
	</fork>

	<state name="P11_DictarInstrucciones_new1">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_DictarInstrucciones_new1" name="activarAlerta"></transition>
		<transition to="P11_LeerInstrucciones" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P11_registrarFichaSubasta" name="vueltaAtras"></transition>
	</state>

	<state name="P11_LeerInstrucciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_LeerInstrucciones" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<state name="P11_AnuncioSubasta_new2">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_AnuncioSubasta_new2" name="activarAlerta"></transition>
		<transition to="P11_AnuncioSubasta_new2Decision1" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P11_SolicitudSubasta" name="vueltaAtras"></transition>
	</state>

	<state name="P11_CelebracionSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_CelebracionSubasta" name="activarAlerta"></transition>
		<transition to="CelebracionSubastaDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>


	<decision name="CelebracionSubastaDecision" expression="#{P11_CelebracionSubastaDecision}">
		<transition to="P11_SolicitudPago" name="terceros"></transition>
		<transition to="Decision1" name="nosotros"></transition>
	</decision>

	<state name="P11_SolicitudPago">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_SolicitudPago" name="activarAlerta"></transition>
		<transition to="P11_Cobro" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P11_Cobro">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_Cobro" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Decision1" name="avanzaBPM"></transition>
		<transition to="P11_SolicitudPago" name="vueltaAtras"></transition>
	</state>

	<node name="Decision1">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<decision name="P11_AnuncioSubasta_new2Decision1" expression="#{P11_AnuncioSubasta_new2Decision}">
		
		<transition to="P11_ValidarAnuncioSubasta" name="si"></transition>
		<transition to="Decision2" name="no"></transition>
	</decision>

	<node name="Decision2">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P11_solicitarTasacionInterna">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_registrarTasacionInterna" name="avanzaBPM"></transition>
		<transition to="P11_solicitarTasacionInterna" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P11_registrarTasacionInterna">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_registrarFichaSubasta" name="avanzaBPM"></transition>
		<transition to="P11_registrarTasacionInterna" name="activarAlerta"></transition>
		<transition to="P11_solicitarTasacionInterna" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P11_ValidarAnuncioSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_ValidarAnuncioSubastaDecision" name="avanzaBPM"></transition>
		<transition to="P11_ValidarAnuncioSubasta" name="activarAlerta"></transition>
	</state>

	<decision name="P11_ValidarAnuncioSubastaDecision" expression="#{P11_ValidarAnuncioSubastaDecision}">
		<transition to="ForkSolucitudSubasta" name="si"></transition>
		<transition to="P11_AnuncioSubasta_new2" name="no"></transition>
	</decision>

	<state name="P11_registrarFichaSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P11_DictarInstrucciones_new1" name="avanzaBPM"></transition>
		<transition to="P11_registrarTasacionInterna" name="vueltaAtras"></transition>
	</state>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>
</process-definition>