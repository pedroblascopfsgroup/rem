<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteValoracionBienesMuebles" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P13_SolicitarJustiprecio" name="iniciarProceso"></transition>
	</start-state>


	<state name="P13_SolicitarJustiprecio">
      	<event type="node-enter">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericEnterActionHandler</targetBean>
         	</action>
      	</event>
      	<event type="node-leave">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericLeaveActionHandler</targetBean>
         	</action>
      	</event>
      	<transition to="P13_SolicitarJustiprecio" name="activarAlerta"></transition>
      	<transition to="P13_RegistrarJustiprecio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>		
	</state>

	<state name="P13_RegistrarJustiprecio">
		<event type="node-enter">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericEnterActionHandler</targetBean>
         	</action>
      	</event>
      	<event type="node-leave">
         	<action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            	<targetBean>genericLeaveActionHandler</targetBean>
         	</action>
      	</event>
      	<transition to="P13_RegistrarJustiprecio" name="activarAlerta"></transition>
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P13_SolicitarJustiprecio" name="vueltaAtras"></transition>
	</state>

   	<node name="Decision">
      	<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin" name="Fin"></transition>
   	</node>

	<end-state name="Fin">
	<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>