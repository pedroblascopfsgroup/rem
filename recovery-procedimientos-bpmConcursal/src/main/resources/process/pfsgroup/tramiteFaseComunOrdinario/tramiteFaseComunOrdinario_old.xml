<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteFaseComunOrdinario"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P24_registrarPublicacionBOE" name="iniciarProceso"></transition>
	</start-state>

	<state name="P24_registrarPublicacionBOE">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P24_regInsinuacionCreditosSup" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_registrarPublicacionBOE" name="activarAlerta"></transition>
	</state>
	
	<state name="P24_regInsinuacionCreditosSup">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="P24_regInsinuacionCreditosSupDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_regInsinuacionCreditosSup" name="activarAlerta"></transition>
		<transition to="P24_registrarPublicacionBOE" name="vueltaAtras"></transition>
	</state>

	<state name="P24_informeAdministracionConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>		
		<transition to="P24_revisarResultadoInfAdmon" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_informeAdministracionConcursal" name="activarAlerta"></transition>
		<transition to="P24_presentarEscritoInsinuacionCreditos" name="vueltaAtras"></transition>
	</state>

	<state name="P24_registrarInsinuacionCreditosExt">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_registrarInsinuacionCreditosDef" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_registrarInsinuacionCreditosExt" name="activarAlerta"></transition>
	</state>

	<state name="P24_presentarEscritoInsinuacionCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_presentarEscritoInsinuacionCreditosDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_presentarEscritoInsinuacionCreditos" name="activarAlerta"></transition>
		<transition to="P24_registrarInsinuacionCreditosDef" name="vueltaAtras"></transition>
	</state>

	<decision name="P24_regInsinuacionCreditosSupDecision" expression="#{P24_regInsinuacionCreditosSupDecision}">
		<transition to="P24_registrarInsinuacionCreditosExt" name="SI"></transition>
		<transition to="P24_registrarInsinuacionCreditosDef" name="NO"></transition>
	</decision>

	<state name="P24_revisarResultadoInfAdmon">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_revisarResultadoInfAdmonDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_revisarResultadoInfAdmon" name="activarAlerta"></transition>
		<transition to="P24_informeAdministracionConcursal" name="vueltaAtras"></transition>
	</state>

	<decision name="P24_revisarResultadoInfAdmonDecision" expression="#{P24_revisarResultadoInfAdmonDecision}">
		<transition to="P24_BPMTramiteDemandaIncidental" name="SI"></transition>
		<transition to="P24_registrarFinFaseComun" name="NO"></transition>
	</decision>

	<state name="P24_BPMTramiteDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_actualizarEstadoCreditos" name="tramiteDemandaIncidental"></transition>
	</state>

	<state name="P24_registrarInsinuacionCreditosDef">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_presentarEscritoInsinuacionCreditos" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_registrarInsinuacionCreditosDef" name="activarAlerta"></transition>
	</state>

	<state name="P24_registrarFinFaseComun">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_registrarFinFaseComunDecision" name="avanzaBPM"></transition>
		<transition to="P24_revisarResultadoInfAdmon" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P24_registrarFinFaseComunDecision" expression="#{P24_registrarFinFaseComunDecision}">
		<transition to="P24_BPMTramiteFaseConvenio" name="faseConvenio"></transition>
		<transition to="P24_BPMTramiteFaseLiquidacion" name="faseLiquidacion"></transition>
	</decision>

	<state name="P24_BPMTramiteFaseLiquidacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="tramiteFaseLiquidacion"></transition>
	</state>

	<state name="P24_BPMTramiteFaseConvenio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="tramiteFaseConvenio"></transition>
	</state>

	<state name="P24_actualizarEstadoCreditos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>	
		<transition to="P24_actualizarEstadoCreditos" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P24_registrarFinFaseComun" name="avanzaBPM"></transition>
	</state>

	<decision name="P24_presentarEscritoInsinuacionCreditosDecision" expression="#{P24_presentarEscritoInsinuacionCreditosDecision}">
		<transition to="P24_informeAdministracionConcursal" name="SI"></transition>
		<transition to="P24_presentarSolicitudRectificacion" name="NO"></transition>
	</decision>

	<state name="P24_presentarSolicitudRectificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P24_informeAdministracionConcursal" name="avanzaBPM"></transition>
		<transition to="P24_presentarEscritoInsinuacionCreditos" name="vueltaAtras"></transition>
		<transition to="P24_presentarSolicitudRectificacion" name="activarAlerta"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>