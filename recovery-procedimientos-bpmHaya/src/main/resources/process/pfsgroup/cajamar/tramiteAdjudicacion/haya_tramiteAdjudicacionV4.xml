			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin3" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H005_declararIVAeIGIC" name="activarAlerta"></transition>
	</state>

	<fork name="fork2">
		<transition to="H005_declararIVAeIGIC" name="declararIMP"></transition>
		<transition to="H005_notificacionDecretoAdjudicacionAlContrario" name="notificacionDecretoAdjudicacionAlContrario"></transition>
	</fork>

	<fork name="fork3">
		<transition to="H005_declararIVAeIGIC" name="declararIMP"></transition>
		<transition to="H005_SolicitudTestimonioDecretoAdjudicacion" name="solicitudTestimonio"></transition>
	</fork>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>	
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>	
	</end-state>

	<end-state name="Fin3">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
