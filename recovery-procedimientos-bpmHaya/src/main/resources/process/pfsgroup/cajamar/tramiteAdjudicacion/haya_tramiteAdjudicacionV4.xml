			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H005_SolicitudDecretoAdjudicacion" name="vueltaAtras"></transition>
		<transition to="H005_notificacionDecretoAdjudicacionAEntidad" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H005_notificacionDecretoAdjudicacionAEntidadDecision" name="avanzaBPM"></transition>
	</state>

	<state name="H005_notificacionDecretoAdjudicacionAlContrario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
