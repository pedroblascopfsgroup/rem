		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H005_notificacionDecretoAdjudicacionAlContrario" name="activarAlerta"></transition>
		<transition to="H005_notificacionDecretoAdjudicacionAlContrarioDecision" name="avanzaBPM"></transition>
	</state>

	<state name="H005_ConfirmarTestimonio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H005_ConfirmarTestimonioDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H005_SolicitudTestimonioDecretoAdjudicacion" name="vueltaAtras"></transition>
		<transition to="H005_ConfirmarTestimonio" name="activarAlerta"></transition>
	</state>

	<state name="H005_RegistrarEntregaTitulo">	
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H005_RegistrarPresentacionEnHacienda" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H005_RegistrarEntregaTitulo" name="activarAlerta"></transition>
	</state>

	<state name="H005_RegistrarPresentacionEnHacienda">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H005_RegistrarPresentacionEnRegistro" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H005_RegistrarPresentacionEnHacienda" name="activarAlerta"></transition>
		<transition to="H005_RegistrarEntregaTitulo" name="vueltaAtras"></transition>
	</state>

	<state name="H005_RegistrarPresentacionEnRegistro">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
