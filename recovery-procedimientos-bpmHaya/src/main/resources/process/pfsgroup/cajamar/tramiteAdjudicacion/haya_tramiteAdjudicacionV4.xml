				</targetBean>
			</action>
		</event>	
		<transition to="H005_RegistrarPresentacionEnRegistro" name="haya_tramiteSubsanacionEmbargo"></transition>
	</state>

	<state name="H005_ConfirmarContabilidad">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					adjudicacionHayaLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H005_ConfirmarContabilidad" name="activarAlerta"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<state name="H005_declararIVAeIGIC">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
