<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="hcj_certificadoLibertadArrendamiento" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="HC101_EmitirCertificado" name="avanzaBPM"></transition>
	</start-state>

	<state name="HC101_EmitirCertificado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="HC101_EmitirCertificado" name="activarAlerta"></transition>
		<transition to="HC101_LegitimacionNotarialFirma" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="HC101_LegitimacionNotarialFirma">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="HC101_LegitimacionNotarialFirma" name="activarAlerta"></transition>
		<transition to="HC101_RecepcionCertificacionNotarial" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="HC101_EmitirCertificado" name="vueltaAtras"></transition>
	</state>

	<state name="HC101_RecepcionCertificacionNotarial">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="HC101_RecepcionCertificacionNotarial" name="activarAlerta"></transition>
		<transition to="HC101_RemitirCertificadoLetrado" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="HC101_LegitimacionNotarialFirma" name="vueltaAtras"></transition>
	</state>

	<state name="HC101_RemitirCertificadoLetrado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="HC101_RemitirCertificadoLetrado" name="activarAlerta"></transition>
		<transition to="HC101_ConfirmarRecepcionCertificado" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="HC101_RecepcionCertificacionNotarial" name="vueltaAtras"></transition>
	</state>

	<state name="HC101_ConfirmarRecepcionCertificado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="HC101_ConfirmarRecepcionCertificado" name="activarAlerta"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="HC101_RemitirCertificadoLetrado" name="vueltaAtras"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>
</process-definition>