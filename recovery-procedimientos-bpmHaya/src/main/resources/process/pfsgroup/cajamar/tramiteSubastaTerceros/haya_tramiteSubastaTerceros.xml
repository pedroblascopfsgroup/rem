		<transition to="H004_SolicitarDueDiligence" name="vueltaAtras"></transition>
	</state>

	<fork name="fork1">
		<transition to="H004_BPMTramiteTributacion" name="tramiteTributacion"></transition>
		<transition to="H004_ContactarConDeudorYPrepararInformeSubastaFisc" name="contactarConDeudorYPrepararInformeSubastaFiscal"></transition>
		<transition to="H004_CumplimentarParteEconomica" name="cumplimentarParteEconomica"></transition>
		<transition to="H004_PrepararPropuestaSubasta" name="prepararPropuestaSubasta"></transition>
		<transition to="H004_CelebracionSubasta" name="celebracionDeSubasta"></transition>
		<transition to="H004_AdjuntarInformeSubasta" name="adjuntarInformeDeSubasta"></transition>
	</fork>

	<state name="H004_SuspenderSubasta">
