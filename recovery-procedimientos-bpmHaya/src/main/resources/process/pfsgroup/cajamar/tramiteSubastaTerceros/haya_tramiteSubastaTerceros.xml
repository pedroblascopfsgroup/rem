		<transition to="H004_SuspenderSubasta" name="activarAlerta"></transition>
		<transition to="Fin3" name="Fin"></transition>
	</state>

	<decision name="H004_ValidarInformeDeSubastaYPrepararCuadroDePujasDecision" expression="#{H004_ValidarInformeDeSubastaYPrepararCuadroDePujasDecision}">
		<transition to="fork2" name="DueDiligenceYTramiteConsignacion"></transition>
		<transition to="fork3" name="SolicitarDueDiligence"></transition>
		<transition to="fork4" name="TramiteConsignacion"></transition>
		<transition to="fork5" name="Fin"></transition>
	</decision>
	
	<node name="H004_SolicitarNumActivo">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>subastaSolicitarNumActivoHandler
			</targetBean>
		</action>
		<transition to="Fin3" name="avanzaBPM"></transition>
	</node>
	
	<node name="H004_SolicitarTasacion">
