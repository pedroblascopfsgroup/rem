		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="Fin2" name="hcj_tramiteTributacionBienesSareb"></transition>
	</state>

	<state name="H004_ContactarConDeudorYPrepararInformeSubastaFisc">
