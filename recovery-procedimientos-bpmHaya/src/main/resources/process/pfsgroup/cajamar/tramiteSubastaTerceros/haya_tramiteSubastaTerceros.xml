				config-type="bean">
				<targetBean>subastaV4HayaConcursalLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H004_ValidarInformeDeSubastaYPrepararCuadroDePujas" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H004_AdjuntarInformeSubasta" name="activarAlerta"></transition>
	</state>

	<state name="H004_ValidarInformeDeSubastaYPrepararCuadroDePujas">
