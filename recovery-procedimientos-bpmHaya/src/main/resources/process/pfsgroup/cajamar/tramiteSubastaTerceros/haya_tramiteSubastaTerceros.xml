		<transition to="H004_ContactarConDeudorYPrepararInformeSubastaFisc" name="activarAlerta"></transition>
	</state>

	<state name="H004_CumplimentarParteEconomica">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
