		<transition to="Fin2" name="Fin"></transition>
		<transition to="H004_ValidarInformeDeSubastaYPrepararCuadroDePujas" name="activarAlerta"></transition>
		<transition to="H004_AdjuntarInformeSubasta" name="vueltaAtras"></transition>
