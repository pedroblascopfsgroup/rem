		</event>	
		<transition to="H004_ValidarPropuestaDecision" name="avanzaBPM"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H004_ValidarPropuesta" name="activarAlerta"></transition>
		<transition to="H004_PrepararPropuestaSubasta" name="vueltaAtras"></transition>
	</state>

	<decision name="H004_ValidarPropuestaDecision" expression="#{H004_ValidarPropuestaDecision}">
		<transition to="H004_ElevarPropuestaASareb" name="NoDelegada"></transition>
		<transition to="H004_SuspenderSubasta" name="Rechazada"></transition>
		<transition to="H004_ElevarPropuestaAComite" name="DelegadaSinAtribuciones"></transition>
		<transition to="H004_LecturaAceptacionInstrucciones" name="DelegadaConAtribuciones"></transition>
		<transition to="H004_PrepararPropuestaSubasta" name="PrepararPropuestaSubasta"></transition>
	</decision>

	<state name="H004_ElevarPropuestaASareb">
