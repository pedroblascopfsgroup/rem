<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="hcj_tramiteSubastaTerceros">

	<start-state name="Inicio">
		<transition to="H004_SenyalamientoSubasta" name="iniciarProceso"></transition>
	</start-state>


	<state name="H004_SenyalamientoSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaV4HayaConcursalEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>subastaV4HayaConcursalLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="fork1" name="avanzaBPM"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="H004_SenyalamientoSubasta" name="activarAlerta"></transition>
	</state>

	<state name="H004_AdjuntarInformeSubasta">
		<event type="node-enter">
			<action name="miTarea"
