<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="haya_tramiteAsignacionGestores">

	<start-state name="Inicio">
		<transition to="H067_seleccionarProcedimiento"></transition>
	</start-state>


	<state name="H067_seleccionarProcedimiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H067_seleccionarProcedimiento" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H067_transicionProcedimiento" name="avanzaBPM"></transition>
	</state>

	<node name="H067_transicionProcedimiento">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>	
      	<transition to="Fin" name="Fin"></transition>
	</node>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>