<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="haya_tramiteCesionRemateV4" xmlns="urn:jbpm.org:jpdl-3.2">



	<start-state name="Inicio">
		<transition to="H006_AperturaPlazo" name="avanzaBPM"></transition>
	</start-state>


	<state name="H006_AperturaPlazo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="H006_AperturaPlazo" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H006_AperturaPlazoDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="H006_AperturaPlazoDecision" expression="#{H006_AperturaPlazoDecision}">
		<transition to="H006_NoCesionRemateDecision" name="NO"></transition>
		<transition to="H006_ResenyarFechaComparecencia" name="SI"></transition>
	</decision>

	<node name="H006_NoCesionRemateDecision">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="H006_ResenyarFechaComparecencia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H006_RealizacionCesionRemate" name="avanzaBPM"></transition>
		<transition to="H006_ResenyarFechaComparecencia" name="activarAlerta"></transition>
		<transition to="H006_AperturaPlazo" name="vueltaAtras"></transition>
	</state>

	<state name="H006_RealizacionCesionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H006_RealizacionCesionRemate" name="activarAlerta"></transition>
		<transition to="H006_ResenyarFechaComparecencia" name="vueltaAtras"></transition>
		<transition to="H006_RealizacionCesionRemateDecision" name="avanzaBPM"></transition>
	</state>

	<state name="H006_BPMTramiteAdjudicacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="haya_tramiteAdjudicacionV4"></transition>	
	</state>

	<decision name="H006_RealizacionCesionRemateDecision" expression="#{H006_RealizacionCesionRemateDecision}">
		<transition to="fork1" name="SI"></transition>
		<transition to="fork2" name="NO"></transition>
	</decision>

	<fork name="fork1">
		<transition to="H006_BPMTramiteAdjudicacion" name="lanzarTramiteAdjudicacion"></transition>
		<transition to="H006_ConfirmarContabilidad" name="confirmarContabilidad"></transition>
	</fork>

	<fork name="fork2">
		<transition to="H006_ConfirmarContabilidad" name="confirmarContabilidad"></transition>
		<transition to="H006_NoCesionRemateDecision" name="decidir"></transition>
	</fork>

	<state name="H006_ConfirmarContabilidad">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H006_ConfirmarContabilidad" name="activarAlerta"></transition>
		<transition to="Fin" name="avanzaBPM"></transition>	
	</state>



	<end-state name="Fin">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>
