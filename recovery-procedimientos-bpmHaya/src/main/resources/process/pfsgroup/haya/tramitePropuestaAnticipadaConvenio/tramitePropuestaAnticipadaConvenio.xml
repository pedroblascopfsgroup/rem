<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="haya_tramitePropuestaAnticipadaConvenio" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="H031_resgistrarNumProcedimiento" name="iniciarProceso"></transition>
	</start-state>


	<state name="H031_registrarPropAnticipadaConvenio">
      	<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
      	<transition to="H031_registrarPropAnticipadaConvenio" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_resgistrarNumProcedimiento" name="vueltaAtras"></transition>
		<transition to="H031_BPMElevarPropuestaSarebLitigios" name="avanzaBPM"></transition>
	</state>

	<state name="H031_admisionTramiteConvenio">
      	<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
      	<transition to="H031_admisionTramiteConvenio" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_admisionTramiteConvenioDecision" name="avanzaBPM"></transition>
		<transition to="H031_ValidarInstrucciones" name="vueltaAtras"></transition>
	</state>

	<decision name="H031_admisionTramiteConvenioDecision" expression="#{H031_admisionTramiteConvenioDecision}">
		<transition to="H031_ConvenioDecision" name="NO"></transition>
		<transition to="H031_informeAdmonConcursal" name="SI"></transition>
	</decision>

	<node name="H031_ConvenioDecision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="H031_informeAdmonConcursal">
		<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
		<transition to="H031_admisionTramiteConvenio" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_informeAdmonConcursal" name="activarAlerta"></transition>
		<transition to="H031_informeAdmonConcursalDecision" name="avanzaBPM"></transition>
	</state>

	<state name="H031_RegResolucionConvenio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
      	<transition to="H031_ContabilizarConvenio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_RegResolucionConvenio" name="activarAlerta"></transition>
		<transition to="H031_informeAdmonConcursal" name="vueltaAtras"></transition>
	</state>

	<state name="H031_resgistrarNumProcedimiento">
		<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
      	<transition to="H031_registrarPropAnticipadaConvenio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_resgistrarNumProcedimiento" name="activarAlerta"></transition>
	</state>

	<decision name="H031_informeAdmonConcursalDecision" expression="#{H031_informeAdmonConcursalDecision}">
		<transition to="H031_ConvenioDecision" name="noDescartar"></transition>
		<transition to="H031_RegResolucionConvenio" name="favOContinuar"></transition>
	</decision>

	<state name="H031_BPMElevarPropuestaSarebLitigios">
		<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
		<transition to="H031_ValidarInstrucciones" name="ACEPTADA"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_SarebDecision" name="DENEGADA"></transition>
		<transition to="H031_ValidarInstrucciones" name="ACCONCAM"></transition>
	</state>

	<state name="H031_ValidarInstrucciones">
		<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
		<transition to="H031_ValidarInstrucciones" name="activarAlerta"></transition>
		<transition to="H031_admisionTramiteConvenio" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H031_ContabilizarConvenio">
		<event type="node-enter">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericEnterActionHandler</targetBean>
         </action>
      	</event>
      	<event type="node-leave">
         <action name="miTarea" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
            <targetBean>genericLeaveActionHandler</targetBean>
         </action>
      	</event>
		<transition to="H031_ContabilizarDecision" name="avanzaBPM"></transition>
		<transition to="H031_ContabilizarConvenio" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="H031_RegResolucionConvenio" name="vueltaAtras"></transition>
	</state>

	<node name="H031_ContabilizarDecision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<node name="H031_SarebDecision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<end-state name="Fin">      
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event> 
    </end-state>


</process-definition>
