	<decision name="H002_ValidarPropuestaDecision" expression="#{H002_ValidarPropuestaDecision}">
		<transition to="H002_ElevarPropuestaASareb" name="NoDelegada"></transition>
		<transition to="H002_SuspenderSubasta" name="Rechazada"></transition>
		<transition to="H002_ElevarPropuestaAComite" name="DelegadaSinAtribuciones"></transition>
		<transition to="H002_LecturaAceptacionInstrucciones" name="DelegadaConAtribuciones"></transition>
		<transition to="H002_PrepararPropuestaSubasta" name="PrepararPropuestaSubasta"></transition>
	</decision>

