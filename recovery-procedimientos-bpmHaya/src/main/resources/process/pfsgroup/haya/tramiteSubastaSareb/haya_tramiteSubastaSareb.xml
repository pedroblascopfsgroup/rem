		<transition to="H002_SuspenderSubasta" name="SuspenderSubasta"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="Fin3" name="Finalizar"></transition>
	</decision>
	
	<fork name="fork2">
		<transition to="H002_SolicitarDueDiligence" name="SolicitarDueDiligence"></transition>
		<transition to="fork3" name="SolicitarNumActivoTasacion"></transition>
	</fork>
	
	<fork name="fork3">
		<transition to="H002_SolicitarNumActivo" name="SolicitarNumActivo"></transition>
		<transition to="H002_SolicitarTasacion" name="SolicitarTasacion"></transition>
	</fork>
	
	<node name="H002_SolicitarNumActivo">
