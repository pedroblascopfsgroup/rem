			<action class="es.pfsgroup.procedimientos.GenericTimerDelayActionHandler" config-type="field">
				<name>Timer espera - F. auto declación concurso</name>
				<duration>20 days</duration>
				<transicion>avanzaBPM</transicion>
			</action>
		</event>	
		<transition to="H002_BPMTramiteAdjudicacionV4" name="avanzaBPM"></transition>
