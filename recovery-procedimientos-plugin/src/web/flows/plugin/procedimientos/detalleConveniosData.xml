<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="idConvenio" type="java.lang.Long" required="false"/>

	<!-- renderiza el formulario -->
   <end-state id="getConvenio" view="plugin/procedimientos/concursal/detalleConvenio" >
		<on-entry>
			<!-- <evaluate expression="requestParameters.idConvenio" result="flowScope.idConvenio"/>-->
			<evaluate expression="executor.execute('convenioManager.listadoConvenioCreditos', idConvenio)" result="flowScope.pagina"/>
		</on-entry>
	</end-state>

</flow>