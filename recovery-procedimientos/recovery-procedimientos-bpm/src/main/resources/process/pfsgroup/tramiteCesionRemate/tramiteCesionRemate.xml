<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteCesionRemate" xmlns="urn:jbpm.org:jpdl-3.2">



	<start-state name="Inicio">
		<transition to="P65_AperturaPlazo" name="avanzaBPM"></transition>
	</start-state>


	<state name="P65_AperturaPlazo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P65_AperturaPlazo" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_AperturaPlazoDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P65_AperturaPlazoDecision" expression="#{P65_AperturaPlazoDecision}">
		<transition to="Decision1" name="NO"></transition>
		<transition to="P65_Rese単arFechaComparecencia" name="SI"></transition>
	</decision>

	<node name="Decision1">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>

	<state name="P65_Rese単arFechaComparecencia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_RealizacionCesionRemate" name="avanzaBPM"></transition>
		<transition to="P65_Rese単arFechaComparecencia" name="activarAlerta"></transition>
		<transition to="P65_AperturaPlazo" name="vueltaAtras"></transition>
	</state>

	<state name="P65_RealizacionCesionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_RealizacionCesionRemate" name="activarAlerta"></transition>
		<transition to="P65_Rese単arFechaComparecencia" name="vueltaAtras"></transition>
		<transition to="Decision2" name="avanzaBPM"></transition>
	</state>

	<node name="Decision2">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler</targetBean>
		</action>
		<transition to="Fin" name="Fin"></transition>
	</node>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>endProcessActionHandler</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>
