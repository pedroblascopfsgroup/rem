<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="tramiteDemandadoEnIncidente">

	<start-state name="Inicio">
		<transition to="P26_notificacionDemandaIncidental"></transition>
	</start-state>

	<state name="P26_notificacionDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P26_dictarInstrucciones" name="avanzaBPM"></transition>
		<transition to="P26_notificacionDemandaIncidental" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_dictarInstrucciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="P26_dictarInstruccionesDecision" name="avanzaBPM"></transition>
		<transition to="P26_dictarInstrucciones" name="activarAlerta"></transition>
		<transition to="P26_notificacionDemandaIncidental" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_registrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P26_admisionEscritoOposicion" name="avanzaBPM"></transition>
		<transition to="P26_registrarOposicion" name="activarAlerta"></transition>
		<transition to="P26_dictarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_admisionEscritoOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P26_admisionEscritoOposicionDecision" name="avanzaBPM"></transition>
		<transition to="P26_admisionEscritoOposicion" name="activarAlerta"></transition>
		<transition to="P26_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P26_admisionEscritoOposicionDecision" expression="#{P26_admisionEscritoOposicionDecision}">
		<transition to="P26_registrarVista" name="SI"></transition>
		<transition to="P26_registrarResolucionOposicion" name="NO"></transition>
	</decision>

	<state name="P26_registrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P26_registrarResolucionOposicion" name="avanzaBPM"></transition>
		<transition to="P26_registrarVista" name="activarAlerta"></transition>
		<transition to="P26_admisionEscritoOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_registrarResolucionOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="P26_registrarResolucionOposicionDecision" name="avanzaBPM"></transition>
		<transition to="P26_registrarResolucionOposicion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_registrarAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P26_admisionEscritoAllanamiento" name="avanzaBPM"></transition>
		<transition to="P26_registrarAllanamiento" name="activarAlerta"></transition>
		<transition to="P26_dictarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_admisionEscritoAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P26_registrarResolucionAllanamiento" name="avanzaBPM"></transition>
		<transition to="P26_admisionEscritoAllanamiento" name="activarAlerta"></transition>
		<transition to="P26_registrarAllanamiento" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_registrarResolucionAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="P26_registrarResolucionAllanamientoDecision" name="avanzaBPM"></transition>
		<transition to="P26_registrarResolucionAllanamiento" name="activarAlerta"></transition>
		<transition to="P26_admisionEscritoAllanamiento" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P26_resolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="P26_resolucionFirme" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P26_dictarInstruccionesDecision" expression="#{P26_dictarInstruccionesDecision}">
		<transition to="P26_registrarAllanamiento" name="NO"></transition>
		<transition to="P26_registrarOposicion" name="SI"></transition>
	</decision>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>	
		<transition to="Fin"></transition>
	</node>

	<decision name="P26_registrarResolucionOposicionDecision" expression="#{P26_registrarResolucionOposicionDecision}">
		<transition to="P26_resolucionFirme" name="FAVORABLE"></transition>
		<transition to="Decision" name="NO FAVORABLE"></transition>
	</decision>

	<decision name="P26_registrarResolucionAllanamientoDecision" expression="#{P26_registrarResolucionAllanamientoDecision}">
		<transition to="P26_resolucionFirme" name="FAVORABLE"></transition>
		<transition to="Decision" name="NO FAVORABLE"></transition>
	</decision>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>