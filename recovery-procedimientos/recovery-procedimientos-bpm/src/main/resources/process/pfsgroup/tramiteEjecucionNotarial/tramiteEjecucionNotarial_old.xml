<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteEjecucionNotarial"
	xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P65_entregaActaRequerimiento" name="iniciarProceso"></transition>
	</start-state>
   
	<state name="P65_entregaActaRequerimiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_entregaActaRequerimiento" name="activarAlerta"></transition>
		<transition to="P65_registrarSolicitudCertCargas" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P65_registrarSolicitudCertCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarSolicitudCertCargas" name="activarAlerta"></transition>
		<transition to="P65_registrarRecepcionCertCargas" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_entregaActaRequerimiento" name="vueltaAtras"></transition>
	</state>

 	<state name="P65_registrarRecepcionCertCargas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
      	<transition to="P65_registrarSolicitudCertCargas" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_registrarRecepcionCertCargas" name="activarAlerta"></transition>
		<transition to="P65_registrarRecepcionCertCargasDecision" name="avanzaBPM"></transition>
	</state>

	<node name="Decision">
		<transition to="Fin"></transition>
	</node>

	<decision name="P65_registrarRecepcionCertCargasDecision" expression="#{P65_registrarRecepcionCertCargasDecision}">
		<transition to="P65_registrarRequerimientoAdeudor" name="correcto"></transition>
		<transition to="Decision" name="incorrecto"></transition>
	</decision>

	<state name="P65_registrarRequerimientoAdeudor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarRequerimientoAdeudorDecision" name="avanzaBPM"></transition>
		<transition to="P65_registrarRecepcionCertCargas" name="vueltaAtras"></transition>
		<transition to="P65_registrarRequerimientoAdeudor" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P65_registrarRequerimientoAdeudorDecision" expression="#{P65_registrarRequerimientoAdeudorDecision}">
		<transition to="P65_registrarPagoDelDeudor" name="positivo"></transition>
		<transition to="Decision" name="negativo"></transition>
	</decision>

	<state name="P65_registrarPagoDelDeudor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_registrarPagoDelDeudorDecision" name="avanzaBPM"></transition>
		<transition to="P65_registrarRequerimientoAdeudor" name="vueltaAtras"></transition>
		<transition to="P65_registrarPagoDelDeudor" name="activarAlerta"></transition>
	</state>

	<decision name="P65_registrarPagoDelDeudorDecision" expression="#{P65_registrarPagoDelDeudorDecision}">
		<transition to="Decision" name="positivo"></transition>
		<transition to="P65_registrarNotificacion" name="negativo"></transition>
	</decision>

	<state name="P65_registrarNotificacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P65_registrarNotificacion" name="activarAlerta"></transition>
		<transition to="P65_registrarNotificacionDecision" name="avanzaBPM"></transition>
	</state>

	<state name="P65_notificarPorAnuncio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_notificarPorAnuncio" name="activarAlerta"></transition>
	</state>

	<decision name="P65_registrarNotificacionDecision" expression="#{P65_registrarNotificacionDecision}">
		<transition to="fork1" name="porAnuncioYpendientes"></transition>
		<transition to="fork2" name="porAnuncioYnoPendientes"></transition>
		<transition to="P65_registrarNotificacion" name="notificadoYpendientes"></transition>
		<transition to="P65_registrarEnvioAlTitular" name="notificadoYnoPendientes"></transition>
	</decision>

	<fork name="fork1">
		<transition to="P65_registrarNotificacion" name="otraNotificacion"></transition>
		<transition to="P65_notificarPorAnuncio" name="porAnuncio1"></transition>
	</fork>

	<fork name="fork2">
		<transition to="P65_notificarPorAnuncio" name="porAnuncio2"></transition>
		<transition to="P65_registrarEnvioAlTitular" name="noPendientes"></transition>
	</fork>

	<state name="P65_registrarEnvioAlTitular">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarRecepcionAlTitular" name="avanzaBPM"></transition>
		<transition to="P65_registrarEnvioAlTitular" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="P65_registrarRecepcionAlTitular">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarAnuncioSubasta" name="avanzaBPM"></transition>
		<transition to="P65_registrarEnvioAlTitular" name="vueltaAtras"></transition>
		<transition to="P65_registrarRecepcionAlTitular" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="P65_registrarAnuncioSubasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarCelebracion1Subasta" name="avanzaBPM"></transition>
		<transition to="P65_registrarRecepcionAlTitular" name="vueltaAtras"></transition>
		<transition to="P65_registrarAnuncioSubasta" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="P65_registrarCelebracion1Subasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarAnuncioSubasta" name="vueltaAtras"></transition>
		<transition to="P65_registrarCelebracion1Subasta" name="ActivarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarCelebracion1SubastaDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="P65_registrarCelebracion1SubastaDecision" expression="#{P65_registrarCelebracion1SubastaDecision}">
		<transition to="P65_registrarCelebracion2Subasta" name="noAdjudicado1"></transition>
		<transition to="P65_registrarLiquidacionRemate" name="adjEntidad1"></transition>
		<transition to="Decision2" name="adjTercero1"></transition>
	</decision>

	<state name="P65_registrarCelebracion2Subasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarCelebracion2SubastaDecision" name="avanzaBPM"></transition>
		<transition to="P65_registrarCelebracion2Subasta" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarCelebracion1Subasta" name="vueltaAtras"></transition>
	</state>

	<decision name="P65_registrarCelebracion2SubastaDecision" expression="#{P65_registrarCelebracion2SubastaDecision}">
		<transition to="P65_registrarCelebracion3Subasta" name="noAdjudicado2"></transition>
		<transition to="P65_registrarLiquidacionRemate" name="adjEntidad2"></transition>
		<transition to="Decision2" name="adjTercero2"></transition>
	</decision>

	<state name="P65_registrarCelebracion3Subasta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarCelebracion3SubastaDecision" name="avanzaBPM"></transition>
		<transition to="P65_registrarCelebracion3Subasta" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarCelebracion2Subasta" name="vueltaAtras"></transition>
	</state>

	<decision name="P65_registrarCelebracion3SubastaDecision" expression="#{P65_registrarCelebracion3SubastaDecision}">
		<transition to="P65_registrarCesionRemate" name="noAdjudicado3"></transition>
		<transition to="P65_registrarLiquidacionRemate" name="adjEntidad3"></transition>
		<transition to="Decision2" name="adjTercero3"></transition>
	</decision>

	<state name="P65_registrarCesionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarLiquidacionRemate" name="avanzaBPM"></transition>
		<transition to="P65_registrarCesionRemate" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarCelebracion3Subasta" name="vueltaAtras"></transition>
	</state>

	<state name="P65_registrarLiquidacionRemate">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarEscritura" name="avanzaBPM"></transition>
		<transition to="P65_registrarLiquidacionRemate" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarCesionRemate" name="vueltaAtras"></transition>
	</state>

	<state name="P65_registrarEscritura">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P65_registrarEscritura" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="P65_registrarLiquidacionRemate" name="vueltaAtras"></transition>
		<transition to="P65_BPMtramiteDePosesion" name="avanzaBPM"></transition>
	</state>

	<node name="Decision2">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>genericUserDecisionActionHandler
			</targetBean>
		</action>
		<transition to="Fin2" name="Fin"></transition>
	</node>

	<state name="P65_BPMtramiteDePosesion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin2" name="Fin"></transition>
		<transition to="Fin2" name="tramiteDePosesion"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>