<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="tramitePresentacionPropConvenio">

	<start-state name="Inicio">
		<transition to="P35_determinarConfeccionDeConvenio" name="inciarProceso"></transition>
	</start-state>

	<state name="P35_registrarConvenioPropio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_presentarPropuesta" name="avanzaBPM"></transition>
		<transition to="P35_registrarConvenioPropio" name="activarAlerta"></transition>
		<transition to="P35_determinarConfeccionDeConvenio" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P35_presentarPropuesta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_registrarAdmision" name="avanzaBPM"></transition>
		<transition to="P35_presentarPropuesta" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>


	<state name="P35_registrarAdmision">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_presentarPropuesta" name="vueltaAtras"></transition>
		<transition to="P35_registrarAdmision" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P35_registrarAdmisionDecision" name="avanzaBPM"></transition>
	</state>

	<state name="P35_determinarConfeccionDeConvenio">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_determinarConfeccionDeConvenioDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P35_determinarConfeccionDeConvenio" name="activarAlerta"></transition>
	</state>

	<decision name="P35_determinarConfeccionDeConvenioDecision" expression="#{P35_determinarConfeccionDeConvenioDecision}">
		<transition to="P35_registrarConvenioPropio" name="supervisor"></transition>
		<transition to="P35_registrarConvenioPropioGestor" name="gestor"></transition>
	</decision>

	<state name="P35_registrarConvenioPropioGestor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_determinarConfeccionDeConvenio" name="vueltaAtras"></transition>
		<transition to="P35_registrarConvenioPropioGestor" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P35_presentarPropuesta" name="avanzaBPM"></transition>
	</state>

	<decision name="P35_registrarAdmisionDecision" expression="#{P35_registrarAdmisionDecision}">
		<transition to="P35_registrarPresentacionSubsanacion" name="subsanable"></transition>
		<transition to="P35_registrarInformeAdmConcursal" name="SI"></transition>
		<transition to="Decision" name="noSubsanable"></transition>
	</decision>

	<state name="P35_registrarPresentacionSubsanacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_registrarResultado" name="avanzaBPM" ></transition>
		<transition to="P35_registrarAdmision" name="vueltaAtras"></transition>
		<transition to="P35_registrarPresentacionSubsanacion" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P35_registrarResultado">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_registrarPresentacionSubsanacion" name="vueltaAtras" ></transition>
		<transition to="P35_registrarResultado" name="activarAlerta"></transition>
		<transition to="P35_registrarResultadoDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P35_registrarInformeAdmConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_registrarAdmision" name="vueltaAtras"></transition>
		<transition to="P35_registrarAdhesiones" name="avanzaBPM"></transition>
		<transition to="P35_registrarInformeAdmConcursal" name="activarAlerta"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="P35_registrarAdhesiones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="P35_registrarInformeAdmConcursal" name="vueltaAtras"></transition>
		<transition to="P35_registrarAdhesiones" name="activarAlerta"></transition>
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="P35_registrarResultadoDecision" expression="#{P35_registrarResultadoDecision}">
		<transition to="P35_registrarInformeAdmConcursal" name="favorable"></transition>
		<transition to="Decision" name="desfavorable"></transition>
	</decision>

	<node name="Decision">
		<action name="miTarea" config-type="bean" 
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin"></transition>
	</node>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>