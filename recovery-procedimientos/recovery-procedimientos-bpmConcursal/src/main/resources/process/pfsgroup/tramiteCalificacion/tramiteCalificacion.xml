<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="tramiteCalificacion" xmlns="urn:jbpm.org:jpdl-3.2">

	<start-state name="Inicio">
		<transition to="P34_aperturaLiquidacion" name="iniciarProceso"></transition>
	</start-state>

	<state name="P34_aperturaLiquidacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_personacionFaseLiquidacion" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_aperturaLiquidacion" name="activarAlerta"></transition>
	</state>

	<state name="P34_personacionFaseLiquidacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_personacionFaseLiquidacionDecision" name="avanzaBPM"></transition>
		<transition to="P34_aperturaLiquidacion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_personacionFaseLiquidacion" name="activarAlerta"></transition>
	</state>

	<state name="P34_escritoPersonacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_registrarCalificacionConcursal" name="avanzaBPM"></transition>
		<transition to="P34_personacionFaseLiquidacion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_escritoPersonacion" name="activarAlerta"></transition>
	</state>

	<decision name="P34_personacionFaseLiquidacionDecision" expression="#{P34_personacionFaseLiquidacionDecision}">
		<transition to="P34_escritoPersonacion" name="SI"></transition>
		<transition to="P34_registrarResolucion" name="NO"></transition>
	</decision>

	<state name="P34_registrarCalificacionFiscal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_registrarCalificacionFiscal" name="activarAlerta"></transition>
		<transition to="P34_registrarOposicion" name="avanzaBPM"></transition>
		<transition to="P34_registrarCalificacionConcursal" name="vueltaAtras"></transition>
	</state>

	<state name="P34_registrarCalificacionConcursal">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_registrarCalificacionConcursal" name="activarAlerta"></transition>
		<transition to="P34_registrarCalificacionFiscal" name="avanzaBPM"></transition>
	</state>

	<state name="P34_registrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_registrarOposicionDecision" name="avanzaBPM"></transition>
		<transition to="P34_registrarCalificacionFiscal" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_registrarOposicion" name="activarAlerta"></transition>
	</state>

	<decision name="P34_registrarOposicionDecision" expression="#{P34_registrarOposicionDecision}">
		<transition to="P34_BPMtramiteDemandaIncidental" name="SI"></transition>
		<transition to="P34_registrarResolucion" name="NO"></transition>
	</decision>

	<state name="P34_BPMtramiteDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="Fin" name="tramiteDemandaIncidental"></transition>
	</state>

	<state name="P34_registrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_resolucionFirme" name="avanzaBPM"></transition>
		<transition to="P34_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_registrarResolucion" name="activarAlerta"></transition>
	</state>

	<state name="P34_resolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler</targetBean>
			</action>
		</event>
		<transition to="P34_registrarResolucion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="P34_resolucionFirme" name="activarAlerta"></transition>
		<transition to="Decision" name="avanzaBPM"></transition>
	</state>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
        	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
      	<transition to="Fin"></transition>
	</node>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>