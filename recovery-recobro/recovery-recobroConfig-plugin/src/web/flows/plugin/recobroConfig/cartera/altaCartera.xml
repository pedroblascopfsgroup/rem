<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
  	
  	<end-state id="altaCartera" view="plugin/recobroConfig/cartera/altaCartera">
  		<secured attributes="ROLE_VER_CARTERAS" match="any"/>
  		<on-entry>
  			<evaluate expression="executor.execute('es.pfsgroup.plugin.recovery.core.api.getReglas')" result="flowScope.ddRegla" />
		</on-entry>
	</end-state>
</flow>
