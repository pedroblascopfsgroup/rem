<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="dto" class="es.pfsgroup.recovery.recobroCommon.agenciasRecobro.dto.RecobroAgenciaDto" />

    <end-state id="agencias" view="reportXLS/plugin/recobroConfig/listaAgencias">
	     <on-entry>
	    	<evaluate expression="binder.bindAndValidate(flowRequestContext, dto)" />
	    	<evaluate expression="executor.execute('plugin.recobroConfig.recobroAgenciaApi.buscaAgencias', dto)" result="flowScope.agenciasPage" />
	    </on-entry>
    </end-state> 	

</flow>
