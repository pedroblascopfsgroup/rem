	<state name="T004_SolicitudExtraordinaria">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_AutorizacionPropietario" name="avanzaBPM"></transition>
	</state>


	<state name="T004_AutorizacionPropietario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_AutorizacionPropietarioDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_AutorizacionPropietarioDecision" expression="#{T004_AutorizacionPropietarioDecision}">
		<transition to="Fin" name="KO"></transition>
		<transition to="T004_FijacionPlazo" name="OK"></transition>
	</decision>
	
	<state name="T004_FijacionPlazo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_FijacionPlazoDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_FijacionPlazoDecision" expression="#{T004_FijacionPlazoDecision}">
		<transition to="T004_ResultadoTarificada" name="Tarificada"></transition>
		<transition to="T004_ResultadoNoTarificada" name="NoTarificada"></transition>
	</decision>


	<state name="T004_ResultadoTarificada">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_ValidacionTrabajo" name="avanzaBPM"></transition>
	</state>
	
		<state name="T004_ResultadoNoTarificada">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_ResultadoNoTarificadaDecision" name="avanzaBPM"></transition>
	</state>
	

	<decision name="T004_ResultadoNoTarificadaDecision" expression="#{T004_ResultadoNoTarificadaDecision}">
		<transition to="T004_SolicitudPresupuestoComplementario" name="ModificacionPresupuesto"></transition>
		<transition to="T004_ValidacionTrabajo" name="Ejecutado"></transition>
	</decision>


	<state name="T004_SolicitudPresupuestoComplementario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_FijacionPlazo" name="avanzaBPM"></transition>
	</state>
	
	
	<state name="T004_ValidacionTrabajo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_ValidacionTrabajoDecision" name="avanzaBPM"></transition>
	</state>

	<decision name="T004_ValidacionTrabajoDecision" expression="#{T004_ValidacionTrabajoDecision}">
		<transition to="T004_CierreEconomico" name="OK"></transition>
		<transition to="T004_FijacionPlazo" name="KO"></transition>
	</decision>
	
	<state name="T004_CierreEconomico">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<state name="T004_EleccionProveedorYTarifa">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_EleccionProveedorYTarifaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_EleccionProveedorYTarifaDecision" expression="#{T004_EleccionProveedorYTarifaDecision}">
		<transition to="T004_SolicitudExtraordinaria" name="SinSaldo"></transition>
		<transition to="T004_AutorizacionBankia" name="SuperaLimiteBankia"></transition>
		<transition to="T004_FijacionPlazo" name="ConSaldo"></transition>
	</decision>
	
	<state name="T004_AutorizacionBankia">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_AutorizacionBankiaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_AutorizacionBankiaDecision" expression="#{T004_AutorizacionBankiaDecision}">
		<transition to="FIn2" name="KO"></transition>
		<transition to="T004_FijacionPlazo" name="OK"></transition>
	</decision>
	
	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>activoEndProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="FIn2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>activoEndProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>