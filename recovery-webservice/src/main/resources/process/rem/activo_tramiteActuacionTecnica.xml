<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="activo_tramiteActuacionTecnica">

	<start-state name="inicio">
		<transition to="T004_AnalisisPeticion"></transition>
	</start-state>
	
	<state name="T004_AnalisisPeticion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_AnalisisPeticionDecision"	name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_AnalisisPeticionDecision" expression="#{T004_AnalisisPeticionDecision}">
		<transition to="Fin" name="KO"></transition>
		<transition to="T004_SolicitudPresupuestos" name="OKNoTarificada"></transition>
		<transition to="T004_EleccionProveedorYTarifa" name="OKTarificada"></transition>
	</decision>


	<state name="T004_SolicitudPresupuestos">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_EleccionPresupuesto" name="avanzaBPM"></transition>
	</state>
	
	<state name="T004_EleccionPresupuesto">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T004_EleccionPresupuestoDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T004_EleccionPresupuestoDecision" expression="#{T004_EleccionPresupuestoDecision}">
		<transition to="T004_SolicitudExtraordinaria" name="SinSaldo"></transition>
		<transition to="T004_SolicitudPresupuestos" name="PresupuestoInvalido"></transition>
		<transition to="T004_AutorizacionBankia" name="ValidoSuperaLimiteBankia"></transition>
		<transition to="T004_FijacionPlazo" name="ConSaldo"></transition>
	</decision>

<!--
	<state name="T004_DisponibilidadSaldo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
