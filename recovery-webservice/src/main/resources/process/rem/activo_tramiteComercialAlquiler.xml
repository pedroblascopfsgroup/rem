    </state>
    
    <decision name ="T015_AprobacionClienteClausulasDecision" expression="#{T015_AprobacionClienteClausulasDecision}">
		<transition to="T015_AgendarFechaFirma" name="Acepta"></transition>
        <transition to="T015_NegociacionClausulasAlquiler" name="NoAceptaConContraoferta"></transition>
		<transition to="Fin" name="NoAceptaSinContraoferta"></transition>
    </decision>
    
    <state name="T015_NegociacionClausulasAlquiler">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T015_NegociacionClausulasAlquilerDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
    </state>
    
    <decision name ="T015_NegociacionClausulasAlquilerDecision" expression="#{T015_NegociacionClausulasAlquilerDecision}">
        <transition to="T015_EnvioContrato" name="Acepta"></transition>
        <transition to="T015_AprobacionOfertaAlquilerComercial" name="NoAcepta"></transition>
    </decision>
    
    <state name="T015_RespuestaBcReagendacion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T015_RespuestaBcReagendacionDecision" name="avanzaBPM"></transition>
		<transition to="Fin" name="Fin"></transition>
    </state>
    
    <decision name ="T015_RespuestaBcReagendacionDecision" expression="#{T015_RespuestaBcReagendacionDecision}">
		<transition to="Fin" name="Anulada"></transition>
        <transition to="T015_AgendarFechaFirma" name="OK"></transition>
		<transition to="Fin" name="Fin"></transition>
    </decision>
    
    <state name="T015_EntregaFianzas">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T015_EntregaFianzasDecision" name="avanzaBPM"></transition>
    </state>
    
    <decision name ="T015_EntregaFianzasDecision" expression="#{T015_EntregaFianzasDecision}">
        <transition to="T015_Firma" name="FianzaOK"></transition>
        <transition to="T015_AgendarFechaFirma" name="ExoneradaMenor"></transition>
        <transition to="T015_RespuestaBcReagendacion" name="ExoneradaMayor"></transition>
    </decision>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>activoEndProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	

</process-definition>