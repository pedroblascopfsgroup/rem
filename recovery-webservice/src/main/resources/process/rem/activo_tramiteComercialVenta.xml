<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="activo_tramiteComercialVenta">

	<start-state name="inicio">
		<transition to="T013_DefinicionOferta"></transition>
	</start-state>
	
	<state name="T013_DefinicionOferta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_DefinicionOfertaDecision" name="avanzaBPM"></transition>
	</state>

	<decision name ="T013_DefinicionOfertaDecision" expression="#{T013_DefinicionOfertaDecision}">
		<transition to="T013_FirmaPropietario" name="SinFormalizacion"></transition>
		<transition to="T013_ResultadoPBC" name="ConFormalizacionConTanteo"></transition>
<!-- 		<transition to="T013_PosicionamientoYFirma" name="ConFormalizacionDeTanteo"></transition> -->
		<transition to="T013_ResolucionComite" name="ConFormalizacionSinTanteoSinAtribucion"></transition>
		<transition to="forkReserva" name="ConFormalizacionSinTanteoConAtribucionConReserva"></transition>
		<transition to="forkSinReservaSinTanteo" name="ConFormalizacionSinTanteoConAtribucionSinReservaConTanteo"></transition>
		<transition to="forkSinReservaConTanteo" name="ConFormalizacionSinTanteoConAtribucionSinReservaSinTanteo"></transition>
	</decision>
	

	<state name="T013_FirmaPropietario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_CierreEconomico" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_FirmaPropietarioDecision" expression="#{T013_FirmaPropietarioDecision}">
		<transition to="T013_CierreEconomico" name="Si"></transition>
		<transition to="Fin" name="No"></transition>
	</decision>
	
	
	<state name="T013_CierreEconomico">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					ActivoGenericSaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>
	
	<state name="T013_ResolucionComite">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_ResolucionComiteDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_ResolucionComiteDecision" expression="#{T013_ResolucionComiteDecision}">
		<transition to="Fin" name="Rechaza"></transition>
		<transition to="T013_RespuestaOfertante" name="Contraoferta"></transition>
		<transition to="forkReserva" name="ApruebaConReserva"></transition>
		<transition to="forkSinReservaConTanteo" name="ApruebaSinReservaConTanteo"></transition>
		<transition to="forkSinReservaSinTanteo" name="ApruebaSinReservaSinTanteo"></transition>
	</decision>

	<state name="T013_RespuestaOfertante">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_RespuestaOfertanteDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_RespuestaOfertanteDecision" expression="#{T013_RespuestaOfertanteDecision}">
		<transition to="forkReserva" name="AceptaConReserva"></transition>
		<transition to="forkSinReservaConTanteo" name="AceptaSinReservaConTanteo"></transition>
		<transition to="forkSinReservaSinTanteo" name="AceptaSinReservaSinTanteo"></transition>
		<transition to="Fin" name="Rechaza"></transition>
	</decision>

	<fork name="forkReserva">
		<transition to="T013_InstruccionesReserva" name="reserva"></transition>
		<transition to="T013_InformeJuridico" name="informe"></transition>
	</fork>

	<fork name="forkSinReservaConTanteo">
		<transition to="T013_InformeJuridico" name="informe"></transition>
		<transition to="T013_ResultadoPBC" name="resultado"></transition>
		<transition to="T013_ResolucionTanteo" name="resolucion"></transition>
	</fork>
	
	<fork name="forkSinReservaSinTanteo">
		<transition to="T013_InformeJuridico" name="informe"></transition>
		<transition to="T013_ResultadoPBC" name="resultado"></transition>
	</fork>

	<state name="T013_InstruccionesReserva">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_InstruccionesReservaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T013_InstrucionesReservaDecision" expression="#{T013_InstruccionesReservaDecision}">
		<transition to="forkConTanteo" name="Si"></transition>
		<transition to="forkSinTanteo" name="No"></transition>
	</decision>
	
	<fork name="forkConTanteo">
		<transition to="T013_ObtencionContratoReserva" name="obtencion"></transition>
		<transition to="T013_ResultadoPBC" name="resultado"></transition>
		<transition to="T013_ResolucionTanteo" name="resolucion"></transition>
	</fork>
	
	<fork name="forkSinTanteo">
		<transition to="T013_ObtencionContratoReserva" name="obtencion"></transition>
		<transition to="T013_ResultadoPBC" name="resultado"></transition>
	</fork>
	
	<state name="T013_InformeJuridico">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>
	
	<state name="T013_ResultadoPBC">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_ResultadoPBCDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_ResultadoPBCDecision" expression="#{T013_ResultadoPBCDecision}">
		<transition to="T013_PosicionamientoYFirma" name="Aprobado"></transition>
		<transition to="T013_ResolucionExpediente" name="NoAprobadoConReserva"></transition>
		<transition to="Fin" name="NoAprobadoSinReserva"></transition>
	</decision>
	
	
	
	
	
	
	
	
	
	
	<state name="T013_ObtencionContratoReserva">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_ObtencionContratoReservaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_ObtencionContratoReservaDecision" expression="#{T013_ObtencionContratoReservaDecision}">
		<transition to="T013_ResolucionTanteo" name="ConTanteo"></transition>
		<transition to="T013_ResultadoPBC" name="SinTanteo"></transition>
	</decision>
	
	<state name="T013_ResolucionTanteo">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_ResolucionTanteoDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_ResolucionTanteoDecision" expression="#{T013_ResolucionTanteoDecision}">
		<transition to="Fin" name="Ejerce"></transition>
		<transition to="T013_ResultadoPBC" name="NoEjerce"></transition>
	</decision>
	
	<decision name ="T013_ResultadoPBCDecision" expression="#{T013_ResultadoPBCDecision}">
		<transition to="Fin" name="NoAprobado"></transition>
		<transition to="T013_PosicionamientoYFirma" name="Aprobado"></transition>
	</decision>
	
	<state name="T013_PosicionamientoYFirma">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T013_PosicionamientoYFirmaDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name ="T013_PosicionamientoYFirmaDecision" expression="#{T013_PosicionamientoYFirmaDecision}">
		<transition to="T013_DevolucionLlaves" name="SinFirma"></transition>
		<transition to="fork1" name="ConFirma"></transition>
	</decision>
	
	<state name="T013_DevolucionLlaves">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>
	
	<fork name="fork1">
		<transition to="T013_DocumentosPostVenta" name="documentos"></transition>
		<transition to="T013_CierreEconomico" name="cierre"></transition>
	</fork>
	
	<state name="T013_DocumentosPostVenta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>

	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>activoEndProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>