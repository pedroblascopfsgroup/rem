<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="activo_tramiteTasacion">

	<start-state name="inicio">
		<transition to="T009_ComprobacionPropuestaPrecios"></transition>
	</start-state>

	<node name="T009_ComprobacionPropuestaPrecios">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>
				activoComprobarPropuestaPreciosActionHandler
			</targetBean>
		</action>
		<transition to="T009_ComprobacionGestor" name="Peticion"></transition>
		<transition to="T009_EnvioPropuestaPropietario" name="Manual"></transition>
	</node>
		
	<node name="T009_ComprobacionGestor">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>
				activoComprobarGestorActionHandler
			</targetBean>
		</action>
		<transition to="T009_AnalisisPeticion" name="OtrosGestores"></transition>
		<transition to="T009_GenerarPropuestaPrecios" name="GestorMarketingOPrecio"></transition>
	</node>
	
	<state name="T009_AnalisisPeticion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T009_AnalisisPeticionDecision" name="avanzaBPM"></transition>
	</state>
	
	<decision name="T009_AnalisisPeticionDecision" expression="#{T009_AnalisisPeticionDecision}">
		<transition to="Fin" name="KO"></transition>
		<transition to="T009_GenerarPropuestaPrecios" name="OK"></transition>
	</decision>

	<state name="T009_GenerarPropuestaPrecios">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T009_EnvioPropuestaPropietario" name="avanzaBPM"></transition>
	</state>
	
	<state name="T009_EnvioPropuestaPropietario">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="T009_SancionCargaPropuesta" name="avanzaBPM"></transition>
	</state>
	
	<state name="T009_SancionCargaPropuesta">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>
					activoGenericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="avanzaBPM"></transition>
	</state>
		
	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>activoEndProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

</process-definition>