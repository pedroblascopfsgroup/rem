<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="activo_tramiteTasacion">

	<start-state name="inicio">
		<transition to="T005_ComprobacionGestor"></transition>
	</start-state>
	
	<node name="T005_ComprobacionGestor">
		<action name="miTarea" config-type="bean"
			class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
			<targetBean>
				activoComprobarGestorActionHandler
			</targetBean>
		</action>
		<transition to="T005_AnalisisPeticion" name="OtrosGestores"></transition>
		<transition to="T005_EmisionTasacion" name="GestorActivo"></transition>
	</node>
	
	<!--  
	<state name="T005_ComprobacionGestor">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
