<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="codigo" type="java.lang.String" required="false"/>
	
	<!-- renderiza el formulario -->
    <end-state id="subfases" view="analisis/listadoSubfasesJSON" >
	    <on-entry>
	    	<evaluate expression="executor.execute('faseManager.getSubfases',codigo)" result="flowScope.subfases" />
	    </on-entry>
    </end-state> 	
    
</flow>