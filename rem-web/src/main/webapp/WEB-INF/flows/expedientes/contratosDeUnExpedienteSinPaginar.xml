<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <var name="dto" class="es.capgemini.pfs.contrato.dto.DtoBuscarContrato"/>

    <end-state id="contratosExpedientes" view="expedientes/contratosSinPaginarJSON">
    	<on-entry>
    		<evaluate expression="binder.bindAndValidate(flowRequestContext, dto)" />
			<evaluate expression="executor.execute('expedienteManager.contratosDeUnExpedienteSinPaginar', dto)" result="flowScope.expedienteContratos" />
		</on-entry>
    </end-state>

</flow>
