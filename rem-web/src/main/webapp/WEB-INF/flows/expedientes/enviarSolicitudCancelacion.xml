<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long"/>
	<input name="detalle" type="java.lang.String"/>
	<input name="esSupervisor" type="java.lang.Boolean"/>
    
    <end-state id="update" view="default">
		<on-entry>
			<evaluate expression="executor.execute('tareaNotificacionManager.solicitarCancelacionExpediente', id, detalle, esSupervisor)"/>
		</on-entry>
	</end-state>
	
	
	
	
        
</flow>
