<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="idPolitica" type="java.lang.Long" />
    
    <end-state id="permisos" view="politica/compruebaPermisosUsuarioJSON">
        <on-entry>
            <evaluate expression="executor.execute('politicaManager.compruebaPermisosUsuario', idPolitica)" result="flowScope.permisos" />
        </on-entry>
    </end-state>

</flow>