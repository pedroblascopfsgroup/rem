<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <input name="id" type="java.lang.Long" required="true" />
    <input name="adjuntar" type="java.lang.Boolean" required="true" />

    <end-state id="formulario" view="reportFormularioDemanda/formularioDemanda">
    	<on-entry>
    		 <evaluate expression="executor.execute('procedimientoManager.formularioDemanda', id)" result="flowScope.dto" />
    	</on-entry>
    </end-state>

</flow>
