<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
       
    <end-state id="generaPDF" view="reportPDF/acta">
    	<on-entry>
    		 <evaluate expression="executor.execute('comiteManager.getActaPDF', 1)" result="flowScope.dtoActa"/>
    		 <evaluate expression="executor.execute('usuarioManager.getUsuarioLogado')" result="flowScope.usuario"/>
    	</on-entry>
    </end-state>

</flow>
