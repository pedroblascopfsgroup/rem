<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn" %><%@ tag body-content="empty" trimDirectiveWhitespaces="true" %><%@ attribute name="value" required="true" type="java.util.Date"%><%@ attribute name="isoFormat" required="false" type="java.lang.Boolean"%><c:if test="${value != null}"><c:out value="${fn:substring(value,8,10)}/${fn:substring(value,5,7)}/${fn:substring(value,0,4)}" /></c:if>