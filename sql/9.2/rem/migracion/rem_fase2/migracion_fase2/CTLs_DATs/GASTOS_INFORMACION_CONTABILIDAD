OPTIONS (DIRECT=TRUE, BINDSIZE=512000, ROWS=10000, ERRORS=999999)
LOAD DATA
CHARACTERSET WE8ISO8859P1
INFILE './CTLs_DATs/DATs/GASTOS_INFORMACION_CONTABILIDAD.dat'
BADFILE './CTLs_DATs/bad/GASTOS_INFORMACION_CONTABILIDAD.bad'
