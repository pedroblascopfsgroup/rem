	GIC_COD_CUENTA_CONTABLE					POSITION(91:110)		CHAR NULLIF(GIC_COD_CUENTA_CONTABLE=BLANKS)							"REPLACE(REPLACE(REPLACE(TRIM(:GIC_COD_CUENTA_CONTABLE),';',' '), '\"',''),'''','')",
	GIC_FECHA_CONTABILIZACION				POSITION(91:110)		DATE 'YYYYMMDD' NULLIF(GIC_FECHA_CONTABILIZACION=BLANKS)			"REPLACE(:GIC_FECHA_CONTABILIZACION, '00000000', '')",
	GIC_COD_DESTINATARIO_CONTABILIZA		POSITION(91:110)		CHAR NULLIF(GIC_COD_DESTINATARIO_CONTABILIZA=BLANKS)				"REPLACE(REPLACE(REPLACE(TRIM(:GIC_COD_DESTINATARIO_CONTABILIZA),';',' '), '\"',''),'''','')",
	GIC_FECHA_DEVENGO_ESPECIAL				POSITION(91:110)		DATE 'YYYYMMDD' NULLIF(GIC_FECHA_DEVENGO_ESPECIAL=BLANKS)			"REPLACE(:GIC_FECHA_DEVENGO_ESPECIAL, '00000000', '')",
	GIC_COD_PERIODICIDAD_ESPECIAL			POSITION(91:110)		CHAR NULLIF(GIC_COD_PERIODICIDAD_ESPECIAL=BLANKS)					"REPLACE(REPLACE(REPLACE(TRIM(:GIC_COD_PERIODICIDAD_ESPECIAL),';',' '), '\"',''),'''','')",
