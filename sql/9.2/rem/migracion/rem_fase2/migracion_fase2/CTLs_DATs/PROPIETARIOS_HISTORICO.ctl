	PRH_NUMERO_ACTIVO					POSITION(18:34)		INTEGER EXTERNAL												"TO_NUMBER(SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:PRH_NUMERO_ACTIVO),';',' '), '\"',''),'''',''),2,16))",
	PRH_FECHA_INICIO_PROMOTOR			POSITION(35:42)		DATE 'YYYYMMDD' NULLIF(PRH_FECHA_INICIO_PROMOTOR=BLANKS)		"REPLACE(:PRH_FECHA_INICIO_PROMOTOR, '00000000', '')",
	PRH_FECHA_FIN_PROMOTOR				POSITION(43:50)		DATE 'YYYYMMDD' NULLIF(PRH_FECHA_FIN_PROMOTOR=BLANKS)			"REPLACE(:PRH_FECHA_FIN_PROMOTOR, '00000000', '')"
)
