OPTIONS (DIRECT=TRUE, BINDSIZE=512000, ROWS=10000, ERRORS=999999)
LOAD DATA
CHARACTERSET WE8ISO8859P1
INFILE './CTLs_DATs/DATs/PROPIETARIOS_HISTORICO.dat'
BADFILE './CTLs_DATs/bad/PROPIETARIOS_HISTORICO.bad'
