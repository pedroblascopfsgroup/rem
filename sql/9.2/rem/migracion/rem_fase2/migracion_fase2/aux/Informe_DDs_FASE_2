(SELECT COUNT(CLC_COD_TIPO_DOC_RTE) FROM MIG2_CLC_CLIENTE_COMERCIAL where NOT EXISTS (SELECT 1 FROM DD_TDI_TIPO_DOCUMENTO_ID WHERE CLC_COD_TIPO_DOC_RTE = DD_TDI_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(CLC_COD_TIPO_DOC_RTE) FROM MIG2_CLC_CLIENTE_COMERCIAL) AS NULOS,
(SELECT COUNT(*) FROM MIG2_CLC_CLIENTE_COMERCIAL) AS TOTAL
FROM DUAL
UNION ALL
