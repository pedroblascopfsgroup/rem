SELECT                                                                                           ------------------------ MIG2_CLC_CLIENTE_COMERCIAL
(SELECT 'MIG2_CLC_CLIENTE_COMERCIAL   ' FROM DUAL) AS TABLA ,
(SELECT 'CLC_COD_TIPO_DOCUMENTO   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_TDI_TIPO_DOCUMENTO_ID   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(CLC_COD_TIPO_DOCUMENTO) FROM MIG2_CLC_CLIENTE_COMERCIAL where EXISTS (SELECT 1 FROM DD_TDI_TIPO_DOCUMENTO_ID WHERE CLC_COD_TIPO_DOCUMENTO = DD_TDI_CODIGO)) AS OK,
(SELECT COUNT(CLC_COD_TIPO_DOCUMENTO) FROM MIG2_CLC_CLIENTE_COMERCIAL where NOT EXISTS (SELECT 1 FROM DD_TDI_TIPO_DOCUMENTO_ID WHERE CLC_COD_TIPO_DOCUMENTO = DD_TDI_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(CLC_COD_TIPO_DOCUMENTO) FROM MIG2_CLC_CLIENTE_COMERCIAL) AS NULOS,
(SELECT COUNT(*) FROM MIG2_CLC_CLIENTE_COMERCIAL) AS TOTAL
FROM DUAL
UNION ALL
SELECT                                                                                            
(SELECT 'MIG2_CLC_CLIENTE_COMERCIAL   ' FROM DUAL) AS TABLA ,
