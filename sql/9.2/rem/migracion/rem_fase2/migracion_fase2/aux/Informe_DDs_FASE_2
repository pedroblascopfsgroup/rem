(SELECT COUNT(*) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS TOTAL
FROM DUAL
UNION ALL
SELECT
(SELECT 'MIG2_GIC_GASTOS_INFO_CONTABI   ' FROM DUAL) AS TABLA ,
(SELECT 'GIC_COD_DESTINA_CONTABILIZA   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_DEP_DESTINATARIOS_PAGO   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(GIC_COD_DESTINA_CONTABILIZA) FROM MIG2_GIC_GASTOS_INFO_CONTABI where EXISTS (SELECT 1 FROM DD_DEP_DESTINATARIOS_PAGO WHERE GIC_COD_DESTINA_CONTABILIZA = DD_DEP_CODIGO)) AS OK,
(SELECT COUNT(GIC_COD_DESTINA_CONTABILIZA) FROM MIG2_GIC_GASTOS_INFO_CONTABI where NOT EXISTS (SELECT 1 FROM DD_DEP_DESTINATARIOS_PAGO WHERE GIC_COD_DESTINA_CONTABILIZA = DD_DEP_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(GIC_COD_DESTINA_CONTABILIZA) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS NULOS,
(SELECT COUNT(*) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS TOTAL
FROM DUAL
UNION ALL
SELECT
(SELECT 'MIG2_GIC_GASTOS_INFO_CONTABI   ' FROM DUAL) AS TABLA ,
(SELECT 'GIC_COD_PERIODICIDAD_ESPECIAL   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_TPE_TIPOS_PERIOCIDAD   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(GIC_COD_PERIODICIDAD_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where EXISTS (SELECT 1 FROM DD_TPE_TIPOS_PERIOCIDAD WHERE GIC_COD_PERIODICIDAD_ESPECIAL = DD_TPE_CODIGO)) AS OK,
(SELECT COUNT(GIC_COD_PERIODICIDAD_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where NOT EXISTS (SELECT 1 FROM DD_TPE_TIPOS_PERIOCIDAD WHERE GIC_COD_PERIODICIDAD_ESPECIAL = DD_TPE_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(GIC_COD_PERIODICIDAD_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS NULOS,
(SELECT COUNT(*) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS TOTAL
FROM DUAL
UNION ALL
SELECT
(SELECT 'MIG2_GIC_GASTOS_INFO_CONTABI   ' FROM DUAL) AS TABLA ,
(SELECT 'GIC_COD_PAR_PRESUP_ESPECIAL   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_PPR_PDAS_PRESUPUESTARIAS   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(GIC_COD_PAR_PRESUP_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where EXISTS (SELECT 1 FROM DD_PPR_PDAS_PRESUPUESTARIAS WHERE GIC_COD_PAR_PRESUP_ESPECIAL = DD_PPR_CODIGO)) AS OK,
(SELECT COUNT(GIC_COD_PAR_PRESUP_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where NOT EXISTS (SELECT 1 FROM DD_PPR_PDAS_PRESUPUESTARIAS WHERE GIC_COD_PAR_PRESUP_ESPECIAL = DD_PPR_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(GIC_COD_PAR_PRESUP_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS NULOS,
(SELECT COUNT(*) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS TOTAL
FROM DUAL
UNION ALL
SELECT
(SELECT 'MIG2_GIC_GASTOS_INFO_CONTABI   ' FROM DUAL) AS TABLA ,
(SELECT 'GIC_COD_CUENTA_CONT_ESPECIAL   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_CCO_CUENTAS_CONTABLES   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(GIC_COD_CUENTA_CONT_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where EXISTS (SELECT 1 FROM DD_CCO_CUENTAS_CONTABLES WHERE GIC_COD_CUENTA_CONT_ESPECIAL = DD_CCO_CODIGO)) AS OK,
(SELECT COUNT(GIC_COD_CUENTA_CONT_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI where NOT EXISTS (SELECT 1 FROM DD_CCO_CUENTAS_CONTABLES WHERE GIC_COD_CUENTA_CONT_ESPECIAL = DD_CCO_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(GIC_COD_CUENTA_CONT_ESPECIAL) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS NULOS,
(SELECT COUNT(*) FROM MIG2_GIC_GASTOS_INFO_CONTABI) AS TOTAL
FROM DUAL
UNION ALL
SELECT                                                                                           ------------------------ MIG2_PAC_PERIMETRO_ACTIVO
(SELECT 'MIG2_PAC_PERIMETRO_ACTIVO   ' FROM DUAL) AS TABLA ,
(SELECT 'PAC_COD_MOTIVO_COMERCIAL   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_MCO_MOTIVO_COMERCIALIZACION   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(PAC_COD_MOTIVO_COMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO where EXISTS (SELECT 1 FROM DD_MCO_MOTIVO_COMERCIALIZACION WHERE PAC_COD_MOTIVO_COMERCIAL = DD_MCO_CODIGO)) AS OK,
(SELECT COUNT(PAC_COD_MOTIVO_COMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO where NOT EXISTS (SELECT 1 FROM DD_MCO_MOTIVO_COMERCIALIZACION WHERE PAC_COD_MOTIVO_COMERCIAL = DD_MCO_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(PAC_COD_MOTIVO_COMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO) AS NULOS,
(SELECT COUNT(*) FROM MIG2_PAC_PERIMETRO_ACTIVO) AS TOTAL
FROM DUAL
UNION ALL
SELECT
(SELECT 'MIG2_PAC_PERIMETRO_ACTIVO   ' FROM DUAL) AS TABLA ,
(SELECT 'PAC_COD_MOTIVO_NOCOMERCIAL   ' FROM DUAL) AS CAMPO ,
(SELECT 'DD_MNC_MOT_NOCOMERCIALIZACION   ' FROM DUAL) AS DD ,
(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(PAC_COD_MOTIVO_NOCOMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO where EXISTS (SELECT 1 FROM DD_MNC_MOT_NOCOMERCIALIZACION WHERE PAC_COD_MOTIVO_NOCOMERCIAL = DD_MNC_CODIGO)) AS OK,
(SELECT COUNT(PAC_COD_MOTIVO_NOCOMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO where NOT EXISTS (SELECT 1 FROM DD_MNC_MOT_NOCOMERCIALIZACION WHERE PAC_COD_MOTIVO_NOCOMERCIAL = DD_MNC_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(PAC_COD_MOTIVO_NOCOMERCIAL) FROM MIG2_PAC_PERIMETRO_ACTIVO) AS NULOS,
(SELECT COUNT(*) FROM MIG2_PAC_PERIMETRO_ACTIVO) AS TOTAL
FROM DUAL


