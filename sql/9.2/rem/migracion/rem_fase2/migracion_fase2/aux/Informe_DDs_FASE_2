(SELECT 'NO' FROM DUAL) AS REQUERIDO,
(SELECT COUNT(PRD_COD_PROVINCIA) FROM MIG2_PRD_PROVEEDOR_DIRECCION where EXISTS (SELECT 1 FROM REMMASTER.DD_PRV_PROVINCIA WHERE PRD_COD_PROVINCIA = DD_PRV_CODIGO)) AS OK,
(SELECT COUNT(PRD_COD_PROVINCIA) FROM MIG2_PRD_PROVEEDOR_DIRECCION where NOT EXISTS (SELECT 1 FROM REMMASTER.DD_PRV_PROVINCIA WHERE PRD_COD_PROVINCIA = DD_PRV_CODIGO)) AS UNDEFINED,
(SELECT COUNT(*)-COUNT(PRD_COD_PROVINCIA) FROM MIG2_PRD_PROVEEDOR_DIRECCION) AS NULOS,
(SELECT COUNT(*) FROM MIG2_PRD_PROVEEDOR_DIRECCION) AS TOTAL
FROM DUAL
UNION ALL
