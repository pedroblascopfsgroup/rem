	CAT_FECHA_REV_VALOR_CATASTRAL	POSITION(537:544)	DATE 'YYYYMMDD' NULLIF(CAT_FECHA_REV_VALOR_CATASTRAL=BLANKS) 	"REPLACE(:CAT_FECHA_REV_VALOR_CATASTRAL, '00000000', '')",
