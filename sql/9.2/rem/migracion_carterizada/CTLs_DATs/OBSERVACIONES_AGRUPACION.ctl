	AGO_FECHA			POSITION(1092:1099)	DATE 'YYYYMMDD' NULLIF(AGO_FECHA=BLANKS)	"REPLACE(:AGO_FECHA, '00000000', '')",
	VALIDACION CONSTANT "0"
)
