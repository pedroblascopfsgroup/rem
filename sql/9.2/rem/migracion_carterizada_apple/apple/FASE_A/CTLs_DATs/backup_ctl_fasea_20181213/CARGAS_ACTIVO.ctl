	CRG_FECHA_PRESENTACION			POSITION(387:394)		DATE 'YYYYMMDD' NULLIF(CRG_FECHA_PRESENTACION=BLANKS) 		"REPLACE(:CRG_FECHA_PRESENTACION, '00000000', '')",
