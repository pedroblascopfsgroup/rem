OPTIONS (DIRECT=TRUE, BINDSIZE=512000, ROWS=10000, ERRORS=999999)
LOAD DATA
CHARACTERSET WE8ISO8859P1
INTO TABLE REM01.AUX_VISITAS
TRUNCATE
FIELDS TERMINATED BY ','
TRAILING NULLCOLS
(
	VIS			INTEGER EXTERNAL NULLIF(VIS=BLANKS) 	"TO_NUMBER(SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:VIS),';',' '), '\"',''),'''',''),1,16))",	
	PVE			INTEGER EXTERNAL NULLIF(PVE=BLANKS) 	"TO_NUMBER(SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:PVE),';',' '), '\"',''),'''',''),1,16))"
)