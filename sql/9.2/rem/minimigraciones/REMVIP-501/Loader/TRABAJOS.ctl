OPTIONS (DIRECT=TRUE, BINDSIZE=512000, ROWS=10000, ERRORS=999999)
LOAD DATA
CHARACTERSET WE8ISO8859P1
INTO TABLE REM01.AUX_FECHA_FIN_TBJ
TRUNCATE
FIELDS TERMINATED BY ','
TRAILING NULLCOLS
(
	TBJ			INTEGER EXTERNAL NULLIF(TBJ=BLANKS) 		"TO_NUMBER(SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:TBJ),';',' '), '\"',''),'''',''),1,16))",	
	FECHA		NULLIF(FECHA=BLANKS) 						"SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:FECHA),';',' '), '\"',''),'''',''),1,10)"
)