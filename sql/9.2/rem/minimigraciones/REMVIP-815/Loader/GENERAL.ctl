OPTIONS (DIRECT=TRUE, BINDSIZE=512000, ROWS=10000, ERRORS=999999)
LOAD DATA
CHARACTERSET WE8ISO8859P1
INTO TABLE PFSREM.AUX_FECHA_INS
TRUNCATE
FIELDS TERMINATED BY ','
TRAILING NULLCOLS
(
	ID_HAYA	INTEGER EXTERNAL NULLIF(ID_HAYA=BLANKS)	"TO_NUMBER(SUBSTR(REPLACE(REPLACE(REPLACE(TRIM(:ID_HAYA),';',' '), '\"',''),'''',''),1,16))",
	FECHA_INS	NULLIF(FECHA_INS=BLANKS) 					"REPLACE(REPLACE(REPLACE(TRIM(:FECHA_INS),';',' '), '\"',''),'''','')"	
)