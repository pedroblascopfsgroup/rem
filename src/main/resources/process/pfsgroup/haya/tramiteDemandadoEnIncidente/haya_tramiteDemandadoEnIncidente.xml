<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="haya_tramiteDemandadoEnIncidente">

	<start-state name="Inicio">
		<transition to="H025_notificacionDemandaIncidental"></transition>
	</start-state>

	<state name="H025_notificacionDemandaIncidental">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="fork1" name="avanzaBPM"></transition>
		<transition to="H025_notificacionDemandaIncidental" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H025_validarInstrucciones">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>		
		<transition to="H025_validarInstruccionesDecision" name="avanzaBPM"></transition>
		<transition to="H025_validarInstrucciones" name="activarAlerta"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H025_registrarOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H025_admisionEscritoOposicion" name="avanzaBPM"></transition>
		<transition to="H025_registrarOposicion" name="activarAlerta"></transition>
		<transition to="H025_validarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<state name="H025_admisionEscritoOposicion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H025_admisionEscritoOposicionDecision" name="avanzaBPM"></transition>
		<transition to="H025_admisionEscritoOposicion" name="activarAlerta"></transition>
		<transition to="H025_registrarOposicion" name="vueltaAtras"></transition>
		<transition to="Fin" name="Fin"></transition>
	</state>

	<decision name="H025_admisionEscritoOposicionDecision" expression="#{H025_admisionEscritoOposicionDecision}">
		<transition to="H025_registrarVista" name="SI"></transition>
		<transition to="H025_registrarResolucion" name="NO"></transition>
	</decision>

	<state name="H025_registrarVista">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H025_registrarVista" name="activarAlerta"></transition>
		<transition to="H025_admisionEscritoOposicion" name="vueltaAtras"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_registrarResolucion" name="avanzaBPM"></transition>
	</state>

	<state name="H025_registrarAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H025_admisionEscritoAllanamiento" name="avanzaBPM"></transition>
		<transition to="H025_registrarAllanamiento" name="activarAlerta"></transition>
		<transition to="H025_validarInstrucciones" name="vueltaAtras"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H025_admisionEscritoAllanamiento">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>	
		<transition to="H025_registrarResolucion" name="avanzaBPM"></transition>
		<transition to="H025_admisionEscritoAllanamiento" name="activarAlerta"></transition>
		<transition to="H025_registrarAllanamiento" name="vueltaAtras"></transition>
		<transition to="Fin2" name="Fin"></transition>
	</state>

	<state name="H025_resolucionFirme">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>			
		<transition to="H025_resolucionFirmeDecision" name="avanzaBPM"></transition>
		<transition to="H025_resolucionFirme" name="activarAlerta"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_registrarResolucion" name="vueltaAtras"></transition>
	</state>

	<decision name="H025_validarInstruccionesDecision" expression="#{H025_validarInstruccionesDecision}">
		<transition to="H025_registrarAllanamiento" name="ALLANATOTAL"></transition>
		<transition to="H025_registrarOposicion" name="OPOSICION"></transition>
		<transition to="H025_registrarOposicion" name="ALLANAPARCIAL"></transition>
	</decision>

	<node name="Decision">
		<action name="miTarea" config-type="bean" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
         	<targetBean>genericUserDecisionActionHandler</targetBean>
      	</action>
		<transition to="Fin2" name="Fin"></transition>
	</node>

	<state name="H025_BPMElevarPropuestaSareb">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin" name="ACEPTADA"></transition>
		<transition to="Fin" name="Fin"></transition>
		<transition to="Fin" name="ACCONCAM"></transition>
		<transition to="Fin" name="DENEGADA"></transition>
	</state>

	<state name="H025_registrarResolucion">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_registrarResolucion" name="activarAlerta"></transition>
		<transition to="H025_resolucionFirme" name="avanzaBPM"></transition>
	</state>

	<decision name="H025_resolucionFirmeDecision" expression="#{H025_resolucionFirmeDecision}">
		<transition to="Decision" name="FAVORABLE"></transition>
		<transition to="H025_registrarImporteCuotasAbonar" name="DESFAVORABLE"></transition>
	</decision>

	<state name="H025_comunicarSareb">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H025_contabilizar" name="avanzaBPM"></transition>
		<transition to="H025_registrarImporteCuotasAbonar" name="vueltaAtras"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_comunicarSareb" name="activarAlerta"></transition>
	</state>

	<state name="H025_registrarImporteCuotasAbonar">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="H025_comunicarSareb" name="avanzaBPM"></transition>
		<transition to="H025_resolucionFirme" name="vueltaAtras"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_registrarImporteCuotasAbonar" name="activarAlerta"></transition>
	</state>

	<state name="H025_contabilizar">
		<event type="node-enter">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericEnterActionHandler
				</targetBean>
			</action>
		</event>
		<event type="node-leave">
			<action name="miTarea"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy"
				config-type="bean">
				<targetBean>genericLeaveActionHandler
				</targetBean>
			</action>
		</event>
		<transition to="Decision" name="avanzaBPM"></transition>
		<transition to="H025_comunicarSareb" name="vueltaAtras"></transition>
		<transition to="Fin3" name="Fin"></transition>
		<transition to="H025_contabilizar" name="activarAlerta"></transition>
	</state>

	<fork name="fork1">
		<transition to="H025_validarInstrucciones" name="validarInstrucciones"></transition>
		<transition to="H025_BPMElevarPropuestaSareb" name="elevarPropuestaSareb"></transition>
	</fork>


	<end-state name="Fin">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin2">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>

	<end-state name="Fin3">
		<event type="node-enter">
			<action name="myAction" config-type="bean"
				class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy">
				<targetBean>endProcessActionHandler
				</targetBean>
			</action>
		</event>
	</end-state>


</process-definition>