<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	<view-state id="usuariosList" view="admin/usuariosList">
		<on-render>
			<evaluate expression="executor.execute('usuarioManager.findByUsername', requestParameters.filtro)" result="flashScope.usuarios"/>
		</on-render>
		<!-- <exception-handler bean="swfExceptionHandler" /> -->
	</view-state>

</flow>
