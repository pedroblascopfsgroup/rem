<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="transitionName" type="java.lang.String"/>
	<input name="tokenId" type="java.lang.Long"/>

    <end-state id="signal" view="default">
		<on-entry>
			<evaluate expression="executor.execute('testBPMManager.signal',tokenId,transitionName)" result="flowScope.id"/>
		</on-entry>

	</end-state>

</flow>
