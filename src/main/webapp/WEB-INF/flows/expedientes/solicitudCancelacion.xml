<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="id" type="java.lang.Long"/>
    
    <end-state id="formulario" view="expedientes/solicitudCancelacion">
    	<on-entry>
    		<evaluate expression="requestParameters.esSupervisor" result="flowScope.esSupervisor" />
    		<evaluate expression="executor.execute('expedienteManager.getExpediente', id)" result="flowScope.expediente"/>
    	</on-entry>
    </end-state> 	
    
        
</flow>
