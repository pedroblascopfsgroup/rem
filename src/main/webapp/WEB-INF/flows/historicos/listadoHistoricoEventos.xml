<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="idEntidad" type="java.lang.Long"/>
    <input name="tipoEntidad" type="java.lang.String"/>
     
    <end-state id="historicoEventos" view="historicos/historicoEventosJSON" >
    	<on-entry>
			<evaluate expression="executor.execute('eventoManager.getHistoricoEventos', tipoEntidad,idEntidad)" result="flowScope.eventos"/>
		</on-entry>
    </end-state> 	
    
</flow>