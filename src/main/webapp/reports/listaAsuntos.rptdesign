            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[asuIt = asuntos.getResults().iterator();]]></method>
            <method name="fetch"><![CDATA[if (!asuIt.hasNext()) return false;
var asu = asuIt.next();

row["id"] = asu.getId();
row["nombre"] = asu.getNombre();
row["fechaCreacion"] = asu.getAuditoria().getFechaCrear();
row["estado"] = asu.getCodigoDecodificado();
row["gestor"] = asu.getGestor().getUsuario().getApellidoNombre();
row["despacho"] = asu.getGestor().getDespachoExterno().getDespacho();
row["supervisor"] = asu.getSupervisor().getUsuario().getApellidoNombre();
row["saldoTotal"] = asu.getSaldoTotal();
return true;]]></method>
