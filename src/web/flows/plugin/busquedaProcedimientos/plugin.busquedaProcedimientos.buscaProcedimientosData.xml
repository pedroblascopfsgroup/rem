<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <var name="dtoBusquedaProcedimientos" class="es.pfsgroup.plugin.recovery.busquedaProcedimientos.procedimiento.dto.BPRDtoBusquedaProcedimientos"/>

    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="listado" view="plugin/busquedaProcedimientos/listadoProcedimientosJSON" >
		<on-entry>
			<evaluate expression="binder.bindAndValidate(flowRequestContext,dtoBusquedaProcedimientos)" />
			<evaluate expression="executor.execute('plugin.busquedaProcedimientos.procedimiento.buscaProcedimientos',dtoBusquedaProcedimientos)" result="flowScope.procedimientos"/>
		</on-entry>
	</end-state>
	
    
</flow>


