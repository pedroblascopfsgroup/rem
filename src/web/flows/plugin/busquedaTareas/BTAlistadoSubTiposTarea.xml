<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
    <input name="id" type="java.lang.String"/>
	
	<!-- renderiza el formulario -->
    <end-state id="listadoSubTiposTarea" view="plugin/busquedaTareas/BTAListadoSubTiposTareaJSON" >
	    <on-entry>
	    	<evaluate expression="executor.execute('plugin.busquedaTareas.BTABusquedaTareaManager.subtiposPorTipoTarea',id)" result="flowScope.subTiposTarea" />
	    </on-entry>
    </end-state> 	
    
</flow>