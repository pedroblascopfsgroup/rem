<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<end-state id="listado" view="plugin/config/funciones/listadoFunciones">
		<secured attributes="ROLE_ADMIN" match="any" />
		<on-entry>
			<evaluate
				expression="executor.execute('plugin.config.web.funciones.buttons.left')"
				result="flowScope.buttonsLeft" />
			<evaluate
				expression="executor.execute('plugin.config.web.funciones.buttons.right')"
				result="flowScope.buttonsRight" />
		</on-entry>
	</end-state>

</flow>