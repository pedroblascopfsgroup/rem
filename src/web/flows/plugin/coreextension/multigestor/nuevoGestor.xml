<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="ugCodigo" type="java.lang.String"/>
	<input name="ugId" type="java.lang.Long"/>
	

	<view-state id="editarGestor" view="${view}">
		<on-entry>
			<set name="flowScope.view" value="'plugin/coreextension/multigestor/formulario/editarGestor'" />
			<evaluate expression="executor.execute('es.pfsgroup.recovery.ext.api.multigestor.getList')" result="flowScope.tiposGestores" />
			<evaluate expression="executor.execute('es.capgemini.usuario.getList')" result="flowScope.usuarios" />
		</on-entry>
	</view-state>

</flow>