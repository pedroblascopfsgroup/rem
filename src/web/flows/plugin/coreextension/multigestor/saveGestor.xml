<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<input name="ugCodigo" type="java.lang.String"/>
	<input name="ugId" type="java.lang.Long"/>
	<input name="idTipoGestor" type="java.lang.String"/>
	<input name="idUsuario" type="java.lang.Long"/>
	

	<end-state id="saveGestor" view="default">
      <on-entry>  
        <evaluate expression="executor.execute('es.pfsgroup.recovery.ext.api.multigestor.addGestor',ugCodigo,ugId,idTipoGestor,idUsuario)" result="flowScope.id"/>
      </on-entry>
    </end-state>
	
</flow>