<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
     
    <var name="dtoCliente" class="es.capgemini.pfs.persona.dto.EXTDtoBuscarClientes"/>

    <end-state id="xls" view="reportXLS/plugin/mejoras/clientes/listaClientes" >
      <on-entry>
         <evaluate expression="binder.bindAndValidate(flowRequestContext, dtoCliente)" />
         <evaluate expression="executor.execute('plugin.coreextension.persona.findClientesProveedorSolvenciaExcel', dtoCliente)" result="flowScope.lista"/>
      </on-entry>
    </end-state>

</flow>