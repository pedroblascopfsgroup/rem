<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	<input name="idContrato" type="java.lang.Long"/>
   
   	<end-state id="reportAcuerdoContrato" view="reportPDF/plugin/nuevoModeloBienes/contratos/informeAcuerdoTris" >
      	<on-entry>
	    	<evaluate expression="executor.execute('plugin.nuevoModeloBienes.contratos.NMBContratoManager.createReportAcuerdo', idContrato)" result="flowScope.resultado" />
	    	<evaluate expression="executor.execute('usuarioManager.getUsuarioLogado')" result="flowScope.usuario" />
	    	<evaluate expression="usuario.entidad.configValue('logo')" result="flowScope.logo" />
		</on-entry>
    </end-state> 	
    
</flow>
