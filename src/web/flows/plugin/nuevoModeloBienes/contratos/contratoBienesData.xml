<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
    
	<input name="idContrato" type="java.lang.Long"/>
    
    <!--a partir de este momento sÃ³lo enviamos ajax -->
    <end-state id="getData" view="plugin/nuevoModeloBienes/contratos/contratosBienesJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.nuevoModeloBienes.contratos.NMBContratoManager.getBienes',idContrato)" result="flowScope.contratosBienes"/>
		</on-entry>
	</end-state>
    
</flow>
