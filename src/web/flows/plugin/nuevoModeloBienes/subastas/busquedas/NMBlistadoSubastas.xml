  	<end-state id="listado" view="plugin/nuevoModeloBienes/subastas/menu/NMBlistadoBusquedaSubastas">
  		<on-entry>
  		
  			<!-- PESTANYA SUBASTA -->
  			<evaluate expression="executor.execute('es.pfsgroup.plugin.recovery.nuevoModeloBienes.subastas.buttons.left')" result="flowScope.buttonsLeft"/>
			<evaluate expression="executor.execute('es.pfsgroup.plugin.recovery.nuevoModeloBienes.subastas.buttons.right')" result="flowScope.buttonsRight"/>
			<evaluate expression="executor.execute('dictionaryManager.getList', 'DDSiNo')" result="flashScope.ddSiNo"/>			
			<evaluate expression="executor.execute('dictionaryManager.getList','DDEstadoSubasta')" result="flowScope.estadoSubasta"/>			
			<evaluate expression="executor.execute('dictionaryManager.getList','DDEntidadAdjudicatariaPropietaria')" result="flowScope.entidadAdjudicatariaPropietaria"/>
<!-- 			<evaluate expression="executor.execute('plugin.nuevoModeloBienes.subastas.manager.SubastaManager.buscarTareasSubastaBankia')" result="flowScope.tareasSubastaBankia" /> -->
<!-- 			<evaluate expression="executor.execute('plugin.nuevoModeloBienes.subastas.manager.SubastaManager.buscarTareasSubastaSareb')" result="flowScope.tareasSubastaSareb" /> -->
			
			<!-- PESTANYA CLIENTE -->
			<evaluate expression="executor.execute('personaManager.getTiposPersonas')" result="flowScope.tiposPersona" />
			
			<!-- PESTANYA CONTRATO -->
			<evaluate expression="executor.execute('dictionaryManager.getList', 'DDEstadoContrato')" result="flowScope.estadosContrato"/>			
			<evaluate expression="executor.execute('tipoProductoEntidadManager.getTiposProductoEntidad')" result="flowScope.tiposProductoEntidad" />
			<evaluate expression="executor.execute('usuarioManager.getZonasUsuarioLogado')" result="flowScope.zonas" />
			<evaluate expression="executor.execute('zonaManager.getNiveles')" result="flowScope.niveles" />

			<!--  PESTANYA ASUNTO -->			
			<evaluate expression="executor.execute('dictionaryManager.getList', 'DDGestionAsunto')" result="flowScope.gestiones"/>
			<evaluate expression="executor.execute('dictionaryManager.getList', 'DDPropiedadAsunto')" result="flowScope.propiedades"/>
		</on-entry>
  	</end-state>
  
</flow>