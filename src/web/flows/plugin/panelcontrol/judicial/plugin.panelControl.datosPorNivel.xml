<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="nivel" type="java.lang.Long" required="true"/>
	<input name="idNivel" type="java.lang.Long" required="true"/>
	<input name="cod" type="java.lang.String" required="false"/>
	<input name="subConsulta" type="java.lang.Boolean" required ="false"/>
	
	<end-state id="form" view="plugin/panelcontrol/judicial/json/listadoDatosPorJerarquiaJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.panelcontrol.getDatosPorJerarquia',nivel,idNivel,cod,subConsulta)" result="flowScope.listado"/>
		</on-entry>
	</end-state>
</flow>
