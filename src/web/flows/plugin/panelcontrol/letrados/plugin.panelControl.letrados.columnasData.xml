<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<input name="tipo" type="java.lang.String" required="false"/>
		
	<end-state id="form" view="plugin/panelcontrol/letrados/json/listadoColumnasJSON">
		<on-entry>
			<evaluate expression="executor.execute('plugin.letrados.panelcontrol.getColumns',tipo)" result="flowScope.listado"/>
		</on-entry>
	</end-state>
</flow>